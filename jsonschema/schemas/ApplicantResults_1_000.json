						"required": false,
						"type": "integer",
						"length": 4,
						"available": true,
						"canUpdate": false                      
                    }]
                },
                "ConvocationList": {
                    "description": "Número da chamada",
                    "type": "string",
                    "x-totvs": [{
                        "product": "RM",
                        "field": "SPSOPCAOINSCRITO.NUMCHAMADA",
						"required": false,
						"type": "integer",
						"length": 4,
						"available": true,
						"canUpdate": false                      
                    }]
                },
                "OptionalAreaStatusCode": {
                    "description": "Situação atual da inscrição opcional",
                    "type": "string",
                    "x-totvs": [{
                        "product": "RM",
                        "field": "SPSOPCAOINSCRITO.STATUS",
						"required": false,
						"type": "smallint",
						"length": 2,
						"available": true,
						"canUpdate": false                      
                    }]
                },
                "OptionalAreaStatusDescription": {
                    "description": "Descrição da situação atual da inscrição opcional",
                    "type": "string",
                    "x-totvs": [{
                        "product": "RM",
						"field": "SPSSTATUSOPCAO.DESCRICAO",
                        "required": false,
                        "type": "varchar",
						"length": 100,
                        "available": true,
                        "canUpdate": false                      
                    }]
                },
                "ChosenAreaNumber": {
                    "description": "Número da opção",
                    "type": "string",
                    "x-totvs": [{
                        "product": "RM",
                        "field": "SPSOPCAOINSCRITO.NUMEROOPCAO",
						"required": false,
						"type": "smallint",
						"length": 2,
						"available": true,
						"canUpdate": false                      
                    }]
                }
            }          
        }
    }
}
  