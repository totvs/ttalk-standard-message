{
	"$schema": "./TSIIntegrationErrors_1_000.json#",
	"info": {
		"description": "API para consultar os erros da integração",
		"version": "1.000",
		"title": "Consulta os erros da integração",
		"contact": {
			"name": "T-Talk",
			"url": "api.Totvs.com.br",
			"email": "comiteintegracao@totvs.com.br"
		},
		"x-totvs": {
			"messageDocumentation": {
				"name": "TSIIntegrationErrors",
				"description": "API para consultar os erros da integração",
				"segment": "Seviços"
			},
			"productInformation": [
				{
					"product": "protheus",
					"contact": "squad.taf.fisctb@totvs.com.br",
					"description": "API para consultar os erros da integração",
					"adapter": "WSTAF048.prw"
				}
			]
		}
	},
	"definitions": {
		"IntegrationErrors": {
			"type": "object",
			"allOf": [
				{
					"type": "object",
					"properties": {
						"integrationErrors": {
							"type": "array",
							"items": {
								"$ref": "#/definitions/ListErrors"
							}
						}
					}
				}
			]
		},
		"ListErrors": {
			"type": "object",
			"properties": {
				"branchCode": {
					"name": "branchCode",
					"description": "Codigo da Filial",
					"example": "D MG 01",
					"type": "string",
					"required": true
				},
				"layoutError": {
					"name": "layoutError",
					"description": "Layout do erro",
					"example": "T003 - Participantes",
					"type": "string",
					"required": true
				},
				"integrationDate": {
					"name": "integrationDate",
					"description": "Data da Integração",
					"example": "25/05/2022",
					"type": "string",
					"required": true
				},
				"integrationTime": {
					"name": "integrationTime",
					"description": "Hora de integração",
					"example": "15:50:45",
					"type": "string",
					"required": true
				},
				"keyIntegration": {
					"name": "keyIntegration",
					"description": "Chave de integração",
					"example": "00000110000010",
					"type": "string",
					"required": true
				},
				"errorMessage": {
					"name": "errorMessage",
					"description": "Mensagem de erro",
					"example": "Não foi encontrado o conteúdo 'CCT100101' informado no sistema de origem correspondente ao campo 'Id.Participante'( C20_CODPAR ) do TAF.",
					"type": "string",
					"required": true
				}
			}
		},
		"Paging": {
			"type": "object",
			"properties": {
				"hasNext": {
					"type": "boolean",
					"example": false
				}
			}
		}
	},
	"parameters": {
		"companyId": {
			"name": "companyId",
			"description": "Código da empresa e código da filial",
			"example": "T1|D MG 01",
			"in": "query",
			"required": true,
			"schema": {
				"type": "string"
			}
		},
		"integrationDate": {
			"name": "integrationDate",
			"description": "Data de integração",
			"example": "25/05/2022",
			"in": "query",
			"required": false,
			"schema": {
				"type": "string"
			}
		}		
	}
}