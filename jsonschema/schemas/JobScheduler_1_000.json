						"required": false,
						"type": "varchar",
						"length": "50",
						"note": "",
						"available": true,
						"canUpdate": false
					}]
				},
				"return": {
					"type": "object",
					"description": "Resultado da execução do processo",
					"x-totvs": [{
						"product": "rm",
						"field": "gjobxexecucao.mensagemstatus",
						"required": false,
						"type": "varchar",
						"length": "",
						"note": "",
						"available": true,
						"canUpdate": true
					}]
				},
				"error": {
					"type": "string",
					"description": "Mensagem de erro na execução do processo",
					"x-totvs": [{
						"product": "rm",
						"field": "gjobxexecucao.mensagemstatus",
						"required": false,
						"type": "varchar",
						"length": "",
