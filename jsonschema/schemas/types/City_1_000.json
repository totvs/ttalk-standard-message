{
    "$schema": "https://raw.githubusercontent.com/totvs/ttalk-standard-message/master/jsonschema/schemas/types/City_1_000.json#",
    "info": {
        "description": "Entidade de cidade para uso dos produtos TOTVS",
        "version": "1.000",
        "title": "City",
        "contact": {
            "name": "T-Talk",
            "url": "api.totvs.com.br",
            "email": "comiteintegracao@totvs.com.br"
        }
    },
    "definitions": {
        "PagedCities": {
            "type": "object",
            "allOf": [
                {
                    "$ref": "#/definitions/Paging"
                },
                {
                    "$ref": "#/definitions/Cities"
                }
            ]
        },
        "Cities": {
            "type": "object",
            "properties": {
                "Items": {
                    "type": "array",
                    "items": {
                        "$ref": "#/definitions/CitiesType"
                    }
                }
            }
        },
        "CityType": {
            "type": "object",
            "properties": {
                "cityCode": {
                    "type": "string",
                    "description": "Código da cidade"
                },
                "cityInternalId": {
                    "type": "string",
                    "description": "InternalId da Cidade no ERP"
                },
                "cityDescription": {
                    "type": "string",
                    "description": "Nome da cidade"
                }
            }
        },
        "Paging": {
            "type": "object",
            "properties": {
                "hasNext": {
                    "type": "boolean",
                    "example": false
                }
            }
        },
        "ErrorDetails": {
            "type": "object",
            "allOf": [
                {
                    "$ref": "#/definitions/ErrorModel"
                },
                {
                    "$ref": "#/definitions/ErrorDetail"
                }
            ]
        },
        "ErrorModel": {
            "type": "object",
            "required": [
                "code",
                "message",
                "detailedMessage"
            ],
            "properties": {
                "code": {
                    "type": "string",
                    "description": "Código identificador do erro."
                },
                "message": {
                    "type": "string",
                    "description": "Literal no idioma da requisição descrevendo o erro para o usuário."
                },
                "detailedMessage": {
                    "type": "string",
                    "format": "uri",
                    "description": "Mensagem técnica e mais detalhada do erro."
                },
                "helpUrl": {
                    "type": "string",
                    "format": "uri",
                    "description": "URI para documentação do erro."
                }
            }
        },
        "ErrorDetail": {
            "type": "object",
            "properties": {
                "details": {
                    "type": "array",
                    "items": {
                        "$ref": "#/definitions/ErrorModel"
                    }
                }
            }
        }
    }
}