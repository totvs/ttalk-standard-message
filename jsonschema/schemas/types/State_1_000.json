{
    "$schema": "https://raw.githubusercontent.com/totvs/ttalk-standard-message/master/jsonschema/schemas/types/State_1_000.json#",
    "info": {
        "description": "API para cadastro de estado(state) para uso dos produtos TOTVS",
        "version": "1.000",
        "title": "State",
        "contact": {
            "name": "T-Talk",
            "url": "api.totvs.com.br",
            "email": "comiteintegracao@totvs.com.br"
        }
    },
    "definitions": {
        "PagedStates": {
            "type": "object",
            "allOf": [
                {
                    "$ref": "#/definitions/Paging"
                },
                {
                    "$ref": "#/definitions/States"
                }
            ]
        },
        "States": {
            "type": "object",
            "properties": {
                "items": {
                    "type": "array",
                    "items": {
                        "$ref": "#/definitions/StateType"
                    }
                }
            }
        },
        "StateType": {
            "type": "object",
            "properties": {
                "stateId": {
                    "type": "string",
                    "description": "Código do estado"
                },
                "stateInternalId": {
                    "type": "string",
                    "description": "InternalId do Estado no ERP"
                },
                "stateDescription": {
                    "type": "string",
                    "description": "Nome do estado"
                }
            }
        },
        "Paging": {
            "type": "object",
            "properties": {
                "hasNext": {
                    "type": "boolean",
                    "example": false
                }
            }
        },
        "ErrorDetails": {
            "type": "object",
            "allOf": [
                {
                    "$ref": "#/definitions/ErrorModel"
                },
                {
                    "$ref": "#/definitions/ErrorDetail"
                }
            ]
        },
        "ErrorModel": {
            "type": "object",
            "required": [
                "code",
                "message",
                "detailedMessage"
            ],
            "properties": {
                "code": {
                    "type": "string",
                    "description": "Código identificador do erro."
                },
                "message": {
                    "type": "string",
                    "description": "Literal no idioma da requisição descrevendo o erro para o usuário."
                },
                "detailedMessage": {
                    "type": "string",
                    "format": "uri",
                    "description": "Mensagem técnica e mais detalhada do erro."
                },
                "helpUrl": {
                    "type": "string",
                    "format": "uri",
                    "description": "URI para documentação do erro."
                }
            }
        },
        "ErrorDetail": {
            "type": "object",
            "properties": {
                "details": {
                    "type": "array",
                    "items": {
                        "$ref": "#/definitions/ErrorModel"
                    }
                }
            }
        }
    }
}