{
	"$schema": "https://raw.githubusercontent.com/totvs/ttalk-standard-message/master/jsonschema/schemas/types/Address_2_000.json#",
	"info": {
		"description": "",
		"version": "2.000",
		"title": "Address",
		"contact": {},
		"x-totvs": {
			"messageDocumentation": {
				"name": "Address",
				"description": "Endereço",
				"segment": ""
			}
		}
	},
	"definitions": {
		"PagedAddress": {
			"type": "object",
			"allOf": [
				{
					"$ref": "#/definitions/Address"
				}
			]
		},
		"Address": {
			"type": "object",
			"properties": {
				"items": {
					"type": "array",
					"items": {
						"$ref": "#/definitions/AddressType"
					}
				}
			}
		},
		"AddressType": {
			"type": "object",
			"properties": {
				"address": {
				"x-totvs": [{
				  "product": "protheus",
					"type": "string",
					"description": "endereço e número",
					"example": "Rua, Avenida, Rodovia, etc. Ex.: Avenida Braz Leme nº9999"
				},
				{
				  "product": "datasul",
					"type": "string",
					"description": "endereço",
					"example": "Rua, Avenida, Rodovia, etc. Ex.: Avenida Braz Leme"
				},
				{
				  "product": "RM",
					"type": "string",
					"description": "endereço",
					"example": "Rua, Avenida, Rodovia, etc. Ex.: Avenida Braz Leme"
				}]},
				"number": {
				"x-totvs": [
                {
                    "product": "protheus",
                    "type": "string",
                    "description": "No Protheus a tag number não é utilizada, o número deve ser inserido no endereço."
                },
                {
				  "product": "datasul",
					"type": "string",
					"description": "Número do endereço"
				},
				{
				  "product": "RM",
					"type": "string",
					"description": "Número do endereço"
				}
				]},
				"complement": {
					"type": "string",
					"description": "Informações complementares do endereço"
				},
				"city": {
					"type": "object",
					"$ref": "https://raw.githubusercontent.com/totvs/ttalk-standard-message/master/jsonschema/schemas/types/City_1_000.json#/definitions/CityType"
				},
				"district": {
					"type": "string",
					"description": "Bairro",
					"example": "Casa Verde"
				},
				"state": {
					"type": "object",
					"$ref": "https://raw.githubusercontent.com/totvs/ttalk-standard-message/master/jsonschema/schemas/types/State_1_000.json#/definitions/StateType"
				},
				"country": {
					"type": "object",
					"$ref": "https://raw.githubusercontent.com/totvs/ttalk-standard-message/master/jsonschema/schemas/types/Country_1_000.json#/definitions/CountryType"
				},
				"zipCode": {
					"type": "string",
					"description": "CEP",
					"example": "02022010"
				},
				"region": {
					"type": "string",
					"description": "Região",
					"example": "Zona Norte"
				},
				"poBox": {
					"type": "string",
					"description": "Caixa postal"
				},
				"mainAddress": {
					"type": "boolean",
					"description": "Endereço principal?",
					"example": true
				},
				"shippingAddress": {
					"type": "boolean",
					"description": "Endereço de entregas?",
					"example": false
				},
				"billingAddress": {
					"type": "boolean",
					"description": "Endereço de cobrança?",
					"example": true
				}
			}
		}
	}
}