{
    "$schema": "https://raw.githubusercontent.com/izacsc/ttalk-standard-message/RequesterAdresses/v1/1_000/jsonschema/schemas/RequesterAddresses_1_000.json",
    "info": {
        "description": "API para a entidade Endereço de Solicitante para produtos TOTVS",
        "version": "1.000",
        "title": "RequesterAddresses",
        "contact": {
            "name": "T-Talk",
            "url": "api.Totvs.com.br",
            "email": "comiteintegracao@totvs.com.br"
        },
        "x-totvs": {
            "transactionDefinition": {
                "subType": "event",
                "businessContentType": {
                    "type": "object",
                    "$ref": "#/definitions/RequesterAddressesInfo"
                },
                "returnContentType": {
                    "type": "object",
                    "$ref": "https://raw.githubusercontent.com/totvs/ttalk-standard-message/master/jsonschema/schemas/ListOfInternalId_1_000.json#/definitions/ReturnContentWithModelType"
                }
            },
            "messageDocumentation": {
                "name": "RequesterAddresses",
                "description": "Cadastro de Endereço de Solicitante",
                "segment": "TMS"
            },
            "productInformation": [
                {
                    "product": "protheus",
                    "contact": "supply.ml.log.tms-operacional@totvs.com.br",
                    "description": "Cadastro de Endereço de Solicitante",
                    "adapter": "TMSA450I.prw"
                }
            ]
        }
    },
    "definitions": {
        "PagedRequesterAddresses": {
            "type": "object",
            "allOf": [
                {
                    "$ref": "https://raw.githubusercontent.com/totvs/ttalk-standard-message/master/jsonschema/apis/types/totvsApiTypesBase.json#/definitions/Paging"
                },
                {
                    "$ref": "#/definitions/RequesterAddresses"
                }
            ]
        },
        "RequesterAddresses": {
            "type": "object",
            "properties": {
                "items": {
                    "type": "array",
                    "items": {
                        "$ref": "#/definitions/RequesterAddressesInfo"
                    }
                }
            }
        },
        "RequesterAddressesInfo": {
            "type": "object",
            "description": "Endereços do Solicitante",
            "properties": {
                "CompanyId": {
                    "type": "string",
                    "example": "T1",
                    "description": "Código da empresa",
                    "maxLength": 3
                },
                "BranchId": {
                    "type": "string",
                    "description": "Código da Filial",
                    "example": "D MG 01 ",
                    "maxLength": 12,
                    "x-totvs": [
                        {
                            "product": "protheus",
                            "field": "DUL.DUL_FILIAL",
                            "required": true,
                            "length": "8",
                            "available": true,
                            "canUpdate": false
                        }
                    ]
                },
                "CompanyInternalId": {
                    "type": "string",
                    "description": "Código da Empresa",
                    "x-totvs": [
                        {
                            "product": "protheus",
                            "field": "EMPRESA|DUL.DUL_FILIAL",
                            "required": true,
                            "type": "varchar",
                            "length": "16",
                            "available": true,
                            "canUpdate": false
                        }
                    ]
                },
                "InternalId": {
                    "type": "string",
                    "description": "InternalId do Solicitante no ERP",
                    "x-totvs": [
                        {
                            "product": "protheus",
                            "field": "DUL.DUL_FILIAL|DUL.DUL_CODSOL",
                            "type": "varchar",
                            "length": "14",
                            "required": true,
                            "available": true,
                            "note": "O internalID do Solicitante é formado por DUL.DUL_FILIAL|DUL.DUL_CODSOL"
                        }
                    ]
                },
                "Code": {
                    "x-totvs": [
                        {
                            "field": "DUL_SEQEND",
                            "type": "string",
                            "available": true,
                            "length": "9",
                            "canupdate": false,
                            "product": "protheus",
                            "required": true
                        }
                    ],
                    "type": "string",
                    "example": "000000001",
                    "description": "Seq.Endereco             "
                },
                "RequesterCode": {
                    "x-totvs": [
                        {
                            "field": "DUL_CODSOL",
                            "type": "string",
                            "available": true,
                            "length": "9",
                            "canupdate": true,
                            "product": "protheus",
                            "required": false
                        }
                    ],
                    "type": "string",
                    "example": "000000001",
                    "description": "Codigo do Solicitante"
                },
                "customer": {
                    "type": "object",
                    "properties": {
                        "Code": {
                            "$ref": "https://raw.githubusercontent.com/totvs/ttalk-standard-message/master/jsonschema/schemas/CustomerVendor_2_007.json#/definitions/BusinessContentType/properties/Code"
                        },
                        "StoreId": {
                            "$ref": "https://raw.githubusercontent.com/totvs/ttalk-standard-message/master/jsonschema/schemas/CustomerVendor_2_007.json#/definitions/BusinessContentType/properties/StoreId"
                        },
                        "Name": {
                            "$ref": "https://raw.githubusercontent.com/totvs/ttalk-standard-message/master/jsonschema/schemas/CustomerVendor_2_007.json#/definitions/BusinessContentType/properties/Name"
                        }
                    }
                },
                "redispatcher": {
                    "type": "object",
                    "properties": {
                        "Code": {
                            "$ref": "https://raw.githubusercontent.com/totvs/ttalk-standard-message/master/jsonschema/schemas/CustomerVendor_2_007.json#/definitions/BusinessContentType/properties/Code"
                        },
                        "StoreId": {
                            "$ref": "https://raw.githubusercontent.com/totvs/ttalk-standard-message/master/jsonschema/schemas/CustomerVendor_2_007.json#/definitions/BusinessContentType/properties/StoreId"
                        },
                        "Name": {
                            "$ref": "https://raw.githubusercontent.com/totvs/ttalk-standard-message/master/jsonschema/schemas/CustomerVendor_2_007.json#/definitions/BusinessContentType/properties/Name"
                        }
                    }
                },
                "regionGroupCode": {
                    "x-totvs": [
                        {
                            "field": "DUL_CDRDES",
                            "type": "string",
                            "available": true,
                            "length": "6",
                            "canupdate": true,
                            "product": "protheus",
                            "required": true
                        }
                    ],
                    "type": "string",
                    "example": "Q50308",
                    "description": "Código do Grupo de Região"
                },
                "address": {
                    "$ref": "https://raw.githubusercontent.com/totvs/ttalk-standard-message/master/jsonschema/schemas/types/Address_2_000.json#/definitions/AddressType",
                    "type": "object"
                },
                "DifficultAccessFee": {
                    "x-totvs": [
                        {
                            "field": "DUL_TDA",
                            "note": "1=Sim, 2=Não                                                                                                                     ,",
                            "type": "string",
                            "length": "1",
                            "available": true,
                            "canupdate": true,
                            "product": "protheus",
                            "required": false
                        }
                    ],
                    "type": "string",
                    "example": "2",
                    "enum": [
                        "1",
                        "2"
                    ],
                    "description": "Taxa Dificil Acesso"
                },
                "GovernmentalInformation": {
                    "$ref": "https://raw.githubusercontent.com/totvs/ttalk-standard-message/master/jsonschema/schemas/types/GovernmentalInformation_1_000.json#/definitions/GovernmentalInformation"
                }
            }
        }
    }
}