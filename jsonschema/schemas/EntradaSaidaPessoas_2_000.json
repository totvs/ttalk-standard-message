{
    "$schema": "https://raw.githubusercontent.com/totvs/ttalk-standard-message/master/jsonschema/schemas/entradaSaidaPessoas_2_000.json#",
    "info": {
        "description": "API para Consulta RFB",
        "version": "2.000",
        "title": "Consulta RFB",
        "contact": {
            "name": "T-Talk",
            "url": "API.Totvs.com.br",
            "email": "comiteintegracao@totvs.com.br"
        },
        "x-totvs": {
            "messageDocumentation": {
                "name": "Consulta RFB",
                "description": "Joinville",
                "segment": "Logística"
            },
            "productInformation": [
                {
                    "product": "sara",
                    "contact": "jane.frankowiak@totvs.com.br",
                    "description": "Consulta RFB - Entrada e Saída de Pessoas",
                    "adapter": "proc_rfb_1_1.sql",
                    "helpUrl": "tdn.totvs.com/pages/viewpage.action?pageId=???"
                }
            ]
        }
    },
    "definitions": {
        "RegistroEntradaSaidaPessoasXls": {
            "type": "object",
            "properties": {
                "fileName": {
                    "type": "string",
                    "example": "www.totvs.com.br/sara/arquivos/arquivo_001.xls",
                    "description": "Arquivo Xls"
                }
            }
        },
        "RegistroEntradaSaidaPessoas": {
            "type": "object",
            "allOf": [
                {
                    "$ref": "#/definitions/Paging"
                },
                {
                    "$ref": "#/definitions/Pessoas"
                }
            ]
        },
        "Pessoas": {
            "type": "object",
            "properties": {
                "items": {
                    "type": "array",
                    "items": {
                        "$ref": "#/definitions/Pessoa"
                    }
                }
            }
        },
        "Pessoa": {
            "type": "object",
            "properties": {
                "IdPessoa": {
                    "type": "integer",
                    "example": 1,
                    "description": "Identificador da pessoa",
                    "x-totvs": [
                        {
                            "product": "sara",
                            "Field": "pes_id",
                            "Required": true,
                            "type": "integer",
                            "length": 4,
                            "available": true,
                            "canUpdate": false
                        }
                    ]
                },
                "NomePessoa": {
                    "type": "string",
                    "example": "Ramirez dos Santos",
                    "description": "Nome da Pessoa",
                    "x-totvs": [
                        {
                            "product": "sara",
                            "Field": "pes_nome",
                            "Required": true,
                            "type": "varchar",
                            "length": "50",
                            "available": true,
                            "canUpdate": false
                        }
                    ]
                },
                "TipoDocumento": {
                    "type": "string",
                    "example": "RG",
                    "description": "Tipo de Documento",
                    "x-totvs": [
                        {
                            "product": "sara",
                            "Field": "tipo_doc",
                            "Required": true,
                            "type": "varchar",
                            "length": 20,
                            "available": true,
                            "canUpdate": false
                        }
                    ]
                },
                "NumeroDocumento": {
                    "type": "string",
                    "example": "6565656565",
                    "description": "Número do Documento de Identificação",
                    "x-totvs": [
                        {
                            "product": "sara",
                            "Field": "documento",
                            "Required": true,
                            "type": "varchar",
                            "length": 15,
                            "available": true,
                            "canUpdate": false
                        }
                    ]
                },
                "OrgaoExpedidorDocumento": {
                    "type": "string",
                    "example": "SSP/SP",
                    "description": "Orgão Emissor",
                    "x-totvs": [
                        {
                            "product": "sara",
                            "Field": "pes_orgao_emissor",
                            "Required": true,
                            "type": "varchar",
                            "length": 20,
                            "available": true,
                            "canUpdate": false
                        }
                    ]
                },
                "FuncaoPessoa": {
                    "type": "string",
                    "example": "MOTORISTA",
                    "description": "Função da pessoa no terminal",
                    "x-totvs": [
                        {
                            "product": "sara",
                            "Field": "funcao",
                            "Required": true,
                            "type": "varchar",
                            "length": 50,
                            "available": true,
                            "canUpdate": false
                        }
                    ]
                },
                "DataEntrada": {
                    "type": "string",
                    "format": "date-time",
                    "example": "2017-11-04 11:03:25.000 ZZZ",
                    "description": "Data de Entrada",
                    "x-totvs": [
                        {
                            "product": "sara",
                            "Field": "ace_entrada",
                            "Required": true,
                            "type": "datetime",
                            "available": true,
                            "canUpdate": false
                        }
                    ]
                },
                "DataSaida": {
                    "type": "string",
                    "format": "date-time",
                    "example": "2017-11-08 11:06:18.000 ZZZ",
                    "description": "Data de Saída",
                    "x-totvs": [
                        {
                            "product": "sara",
                            "Field": "ace_saida",
                            "Required": false,
                            "type": "datetime",
                            "available": true,
                            "canUpdate": false
                        }
                    ]
                },
                "MotivoAcesso": {
                    "type": "string",
                    "example": "Retirar mercadoria",
                    "description": "Motivo do acesso ao terminal",
                    "x-totvs": [
                        {
                            "product": "sara",
                            "Field": "motivo_acesso",
                            "Required": false,
                            "type": "varchar",
                            "length": 50,
                            "available": true,
                            "canUpdate": false
                        }
                    ]
                }
            }
        },
        "Paging": {
            "type": "object",
            "properties": {
                "hasNext": {
                    "type": "boolean",
                    "example": false
                }
            }
        }
    }
}