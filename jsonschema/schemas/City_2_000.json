								"$ref": "#/definitions/CityType"
							}
						}
					}
				}
			]
		},
		"CityType": {
			"type": "object",
			"properties": {
				"CompanyId": {
					"description": "Código da empresa",
					"type": "string",
					"x-totvs": [
						{
							"product": "PROTHEUS",
							"field": "",
							"required": false,
							"type": "Char",
							"length": "2",
							"note": "Gestão de Empresa ativado",
							"available": true,
							"canUpdate": false
						}
					]
				},
				"BranchId": {
					"description": "Código da Filial",
					"type": "string",
					"x-totvs": [
						{
							"product": "PROTHEUS",
							"field": "",
							"required": false,
							"type": "Char",
							"length": "2",
							"note": "pode ter tamanho variável até 8 com Gestão de Empresa ativado",
							"available": true,
							"canUpdate": false
						}
					]
				},
				"CompanyInternalId": {
					"description": "InternalId da chave completa da empresa",
					"type": "string",
					"x-totvs": [
						{
							"product": "PROTHEUS",
							"field": "",
							"required": false,
							"type": "",
							"length": "",
							"note": "para o Protheus é esperado que com o cabeçalho da mensagem seja identificado empresa e filial",
							"available": true,
							"canUpdate": false
						}
					]
				},
				"Code": {
					"description": "Código da Cidade",
					"type": "string",
					"x-totvs": [
						{
							"product": "PROTHEUS",
							"field": "G5S.G5S_CODIGO",
							"required": true,
							"type": "Char",
							"length": "6",
							"note": "",
							"available": true,
							"canUpdate": false
						},
						{
							"product": "logix",
							"field": "cidades.cod_cidade",
							"required": true,
							"type": "Char",
							"length": "5",
							"note": "",
							"available": true,
							"canUpdate": false
						}
					]
				},
				"InternalId": {
					"description": "InternalId da Cidade",
					"type": "string",
					"x-totvs": [
						{
							"product": "PROTHEUS",
							"field": "cEmpAnt+G5S.G5S_FILIAL+G5S.G5S_CODIGO",
							"required": true,
							"type": "Char",
							"length": "136",
							"note": "",
							"available": true,
							"canUpdate": false
						}
					]
				},
				"Type": {
					"description": "Tipo da Cidade",
					"type": "string",
					"x-totvs": [
						{
							"product": "PROTHEUS",
							"field": "",
							"required": true,
							"type": "Char",
							"length": "1",
							"note": "Informará qual tipo de cadastro de cidade deverá ser utilizado",
							"available": true,
							"canUpdate": false
						}
					]
				},
				"OfficialCode": {
					"description": "Código oficial da Cidade (IBGE) no padrão de 7 digitos",
					"type": "string",
					"minLength": 7,
					"maxLength": 7,
					"x-totvs": [
						{
							"product": "logix",
							"field": "obf_cidade_ibge.cidade_ibge",
							"required": true,
							"type": "Char",
							"length": "7",
							"note": "",
							"available": true,
							"canUpdate": true
						}
					]
				},
				"Description": {
					"description": "Nome da Cidade",
					"type": "string",
					"x-totvs": [
						{
							"product": "PROTHEUS",
							"field": "G5S.G5S_CIDADE",
							"required": true,
							"type": "Char",
							"length": "50",
							"note": "",
							"available": true,
							"canUpdate": false
						},
						{
							"product": "logix",
							"field": "cidades.den_cidade",
							"required": true,
							"type": "Char",
							"length": "30",
							"note": "",
							"available": true,
							"canUpdate": true
						}
					]
				},
				"IATANumber": {
					"description": "Código IATA da cidade que está sendo cadastrada",
					"type": "string",
					"x-totvs": [
						{
							"product": "PROTHEUS",
							"field": "G5S.G5S_IATA",
							"required": false,
							"type": "Char",
							"length": "10",
							"note": "",
							"available": true,
							"canUpdate": false
						}
					]
				},
				"FederalUnit": {
					"description": "Identificação da Unidade Federativa",
					"type": "string",
					"x-totvs": [
						{
							"product": "PROTHEUS",
							"field": "G5S.G5S_SIGLA",
							"required": true,
							"type": "Char",
							"length": "2",
							"note": "Seu conteúdo é pré determinado por lista do SX5, mas pode ser incrementado com informações pelo cliente",
							"available": true,
							"canUpdate": false
						},
						{
							"product": "logix",
							"field": "cidades.cod_uni_feder",
							"required": true,
							"type": "Char",
							"length": "2",
							"note": "Sigla da unidade federativa. Para cidades estrangeiras enviar o valor 'EX'.",
							"available": true,
							"canUpdate": true
						}
					]
				},
				"CountryCode": {
					"description": "Código do País",
					"type": "string",
					"x-totvs": [
						{
							"product": "PROTHEUS",
							"field": "G5S.G5S_PAIS",
							"required": true,
							"type": "Char",
							"length": "2",
							"note": "Seu conteúdo é pré determinado pelo cadastro de Paises (SYA)",
							"available": true,
							"canUpdate": false
						}
					]
				},
				"Situation": {
					"description": "Situação? (\"1-Ativo / 2-Bloqueado\")",
					"type": "string",
					"enum": [
						"1",
						"2"
					],
					"x-totvs": [
						{
							"product": "PROTHEUS",
							"field": "G5S.G5S_MSBLQL",
							"required": false,
							"type": "Char",
							"length": "1",
							"note": "Campo Opcional, se no cadastro esse campo existir, será enviado a TAG",
							"available": true,
							"canUpdate": false
						}
					]
				}
			}
		}
