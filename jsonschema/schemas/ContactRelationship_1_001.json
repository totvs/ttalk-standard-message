{
	"$schema": "https://raw.githubusercontent.com/totvs/ttalk-standard-message/master/jsonschema/schemas/ContactRelationship_1_6901.json#",
	"info": {
		"description": "API para a entidade de Relacionamento de Contatos x Entidades (ContactRelationship) para produtos TOTVS",
		"version": "1.001",
		"title": "ContactRelationship",
		"contact": {
			"name": "T-Talk",
			"url": "https://api.totvs.com.br",
			"email": "comiteintegracao@totvs.com.br"
		},
		"x-totvs": {
			"transactionDefinition": {
				"subType": "event",
				"businessContentType": {
					"type": "object",
					"$ref": "#/definitions/BusinessContentType"
				},
				"returnContentType": {
					"type": "object",
					"$ref": "#/definitions/ReturnContentType"
				}
			},
			"messageDocumentation": {
				"name": "ContactRelationship",
				"description": "Cadastro de Relacionamento Entidade x Contato",
				"segment": "Serviços"
			},
			"productInformation": [
				{
					"product": "protheus",
					"contact": "squad.crm@totvs.com.br",
					"note": "Cadastro de Relacionamento de Contato x Entidade",
					"adapter": "CRMS060.PRW"
				}
			]
		}
	},
	"definitions": {
		"PagedContactRelationship": {
			"type": "object",
			"allOf": [
				{
					"$ref": "https://raw.githubusercontent.com/totvs/ttalk-standard-message/master/jsonschema/apis/types/totvsApiTypesBase.json#/definitions/Paging"
				},
				{
					"$ref": "#/definitions/ContactRelationship"
				}
			]
		},
		"ContactRelationship": {
			"type": "object",
			"properties": {
				"items": {
					"type": "array",
					"items": {
						"$ref": "#/definitions/BusinessContentType"
					}
				}
			}
		},
		"BusinessContentType": {
			"type": "object",
			"properties": {
				"CompanyId": {
					"description": "Código da Empresa ou Código do Grupo da Empresa",
					"type": "string",
					"example": "T1",
					"maxLength": 3,
					"x-totvs": [
						{
							"product": "protheus",
							"field": "",
							"required": false,
							"type": "Char",
							"length": "2",
							"note": "Gestão de Empresa ativado",
							"available": true,
							"canUpdate": false
						}
					]
				},
				"CompanyInternalId": {
					"description": "InternalId da chave completa da empresa",
					"type": "string",
					"x-totvs": [
						{
							"product": "protheus",
							"field": "",
							"required": false,
							"type": "",
							"length": "",
							"note": "para o protheus é esperado que com o cabeçalho da mensagem seja identificado empresa e filial",
							"available": true,
							"canUpdate": false
						}
					]
				},
				"InternalId": {
					"type": "string",
					"description": "InternalId da Relacionamento do Contato x Entidade no ERP",
					"example": "D MG    SA1D MG    00000101",
					"x-totvs": [
						{
							"product": "protheus",
							"field": "AC8_FILIAL|AC8_ENTIDA|AC8_FILENT|AC8_CODENT",
							"type": "varchar",
							"length": "74",
							"available": true,
							"note": "Este dado e formado pelos campos: Filial, Cod. do Contato, Entidade, Filial da Entidade e Cod. Entidade."
						}
					]
				},
				"BranchId": {
					"description": "Código da Filial",
					"type": "string",
					"example": "D MG",
					"maxLength": 12,
					"x-totvs": [
						{
							"product": "protheus",
							"field": "AC8_FILIAL",
							"required": false,
							"type": "Char",
							"length": "8",
							"note": "pode ter tamanho variável de 2 até 8 com Gestão de Empresa ativado",
							"available": true,
							"canUpdate": false
						}
					]
				},
				"Code": {
					"description": "Código da Entidade",
					"type": "string",
					"example": "00000101",
					"maxLength": 55,
					"x-totvs": [
						{
							"product": "protheus",
							"field": "AC8.AC8_CODENT",
							"required": true,
							"type": "Char",
							"length": "55",
							"available": true,
							"canUpdate": false
						}
					]
				},
				"Entity": {
					"description": "Entidade",
					"type": "string",
					"example": "SA1",
					"maxLength": 3,
					"x-totvs": [
						{
							"product": "protheus",
							"field": "AC8.AC8_ENTIDA",
							"required": true,
							"type": "Char",
							"length": "3",
							"available": true,
							"canUpdate": false
						}
					]
				},
				"ListOfContacts": {
					"type": "object",
					"description": "Lista os Contatos",
					"properties": {
						"ListOfContactsType": {
							"type": "array",
							"items": {
								"$ref": "#/definitions/ContactType"
							}
						}
					}
				}
			}
		},
		"PagedContactType": {
			"type": "object",
			"allOf": [
				{
					"$ref": "https://raw.githubusercontent.com/totvs/ttalk-standard-message/master/jsonschema/apis/types/totvsApiTypesBase.json#/definitions/Paging"
				},
				{
					"$ref": "#/definitions/ListofContact"
				}
			]
		},
		"ListofContact": {
			"type": "object",
			"properties": {
				"items": {
					"type": "array",
					"items": {
						"$ref": "#/definitions/ContactType"
					}
				}
			}
		},
		"ContactType": {
			"type": "object",
			"properties": {
				"ContactCode": {
					"description": "Código do Contato",
					"type": "string",
					"example": "000001",
					"maxLenght": 6,
					"x-totvs": [
						{
							"product": "protheus",
							"field": "AC8.AC8_CODCON",
							"required": true,
							"type": "Char",
							"length": "6",
							"note": "Seu conteúdo é pré determinado pelo cadastro de Contatos (TMKA070 - SU5)",
							"available": true,
							"canUpdate": false
						}
					]
				},
				"ContactInternalId": {
					"description": "InternalId do Contato",
					"type": "string",
					"x-totvs": [
						{
							"product": "protheus",
							"field": "AC8_FILIAL|AC8_CODCON|AC8_ENTIDA|AC8_FILENT|AC8_CODENT",
							"required": true,
							"type": "Char",
							"length": "80",
							"note": "Este dado e formado pelos campos: Filial, Cod. do Contato, Entidade, Filial da Entidade e Cod. Entidade.",
							"available": true,
							"canUpdate": false
						}
					]
				}
			}
		},
		"ReturnContentType": {
			"type": "object",
			"properties": {
				"ListOfInternalId": {
					"type": "array",
					"items": {
						"$ref": "https://raw.githubusercontent.com/totvs/ttalk-standard-message/master/jsonschema/schemas/types/ListOfInternalId_1_6900.json#/definitions/ListOfInternalIdType",
						"type": "object"
					}
				}
			}
		}
	}
}