{
	"$schema": "https://raw.githubusercontent.com/totvs/ttalk-standard-message/master/jsonschema/schemas/CustomerVendor_2_006.json#",
	"info": {
		"description": "API para a entidade cliente/fornecedor (customerVendor) para produtos TOTVS",
		"version": "2.008",
		"title": "CustomerVendor",
		"contact": {
			"name": "T-Talk",
			"url": "api.Totvs.com.br",
			"email": "comiteintegracao@totvs.com.br"
		},
		"x-totvs": {
			"transactionDefinition": {
				"subType": "event",
				"businessContentType": {
					"type": "object",
					"$ref": "#/definitions/customerVendorInfo"
				},
				"returnContentType": {
					"type": "object",
					"$ref": "https://raw.githubusercontent.com/totvs/ttalk-standard-message/master/jsonschema/schemas/types/ListOfInternalId_1_000.json#/definitions/ReturnContentWithModelType"
				}
			},
			"messageDocumentation": {
				"name": "Customervendor",
				"description": "Cliente/Fornecedor",
				"segment": "Serviços"
			},
			"productInformation": [
				{
					"product": "protheus",
					"contact": "squad.crm@totvs.com.br",
					"description": "Cadastro de Cliente/Fornecedor",
					"adapter": "crms980.prw"
				},
				{
					"product": "RM",
					"contact": "TribeBO.Squad.Financeiro",
					"description": "Cadastro de Tabela de Cliente/Fornecedor",
					"adapter": ""
				}
			]
		}
	},
	"definitions": {
		"PagedCustomerVendors": {
			"type": "object",
			"allOf": [
				{
					"$ref": "#/definitions/Paging"
				},
				{
					"$ref": "#/definitions/CustomerVendors"
				}
			]
		},
		"CustomerVendors": {
			"type": "object",
			"allOf": [
				{
					"$ref": "https://raw.githubusercontent.com/totvs/ttalk-standard-message/master/jsonschema/apis/types/totvsApiTypesBase.json#/definitions/Paging"
				},
				{
					"type": "object",
					"properties": {
						"items": {
							"type": "array",
							"items": {
								"$ref": "#/definitions/customerVendorInfo"
							}
						}
					}
				}
			]
		},
		"customerVendorInfo": {
			"type": "object",
			"properties": {
				"customerVendorId": {
					"description": "customerVendorId",
					"type": "string"
				},
				"companyId": {
					"type": "string",
					"description": "Empresa",
					"x-totvs": [
						{
							"product": "RM",
							"available": true,
							"required": true,
							"note": "Código da Coligada",
							"field": "FCFO.CODCOLIGADA",
							"type": "integer"
						}
					]
				},
				"currentCompanyId": {
					"type": "string",
					"description": "Código de Contexto da Coligada",
					"x-totvs": [
						{
							"product": "RM",
							"available": true,
							"required": true,
							"note": "Código de Contexto da Coligada",
							"type": "integer"
						}
					]
				},
				"branchId": {
					"type": "string",
					"example": "D MG 01",
					"description": "Filial",
					"x-totvs": [
						{
							"product": "protheus",
							"field": "SA1.A1_FILIAL / SA2.A2_FILIAL",
							"required": true,
							"type": "char",
							"length": "12",
							"available": true,
							"canUpdate": false
						}
					]
				},
				"companyInternalId": {
					"type": "string",
					"description": "InternalId da chave completa de empresa do produto",
					"x-totvs": [
						{
							"product": "protheus",
							"field": "EMPRESA|A1_FILIAL|SA1.A1_COD|SA1.A_LOJA / EMPRESA|A2_FILIAL|SA2.A2_COD|SA2.A2_LOJA",
							"required": true,
							"type": "char",
							"length": "12",
							"available": true,
							"canUpdate": false
						},
						{
							"product": "RM",
							"field": "FCFO.CODCOLIGADA | FCFO.CODCFO",
							"type": "string",
							"required": true,
							"available": true
						}
					]
				},
				"code": {
					"type": "string",
					"example": "000001",
					"description": "Código do Cliente/Fornecedor",
					"x-totvs": [
						{
							"product": "protheus",
							"field": "SA1.A1_COD/SA2.A2_COD",
							"required": true,
							"type": "varChar",
							"length": "6",
							"available": true,
							"canUpdate": false
						},
						{
							"product": "datasul",
							"field": "emitente.cod-emitente",
							"required": true,
							"type": "integer",
							"length": "9",
							"available": true,
							"canUpdate": false,
							"note": "Código do Cliente/Fornecedor Formato do Campo: >>>>>>>>9"
						},
						{
							"product": "RM",
							"available": true,
							"required": true,
							"note": "Código do Cliente Fornecedor",
							"field": "FCFO.CODCFO",
							"type": "string"
						}
					]
				},
				"storeId": {
					"type": "string",
					"example": "01",
					"description": "Loja do cliente/fornecedor",
					"x-totvs": [
						{
							"product": "protheus",
							"field": "SA1.A1_LOJA / SA2.A2_LOJA",
							"required": true,
							"type": "char",
							"length": "02",
							"available": true,
							"canUpdate": false
						},
						{
							"product": "RM",
							"available": true,
							"required": true,
							"note": "Código da Loja",
							"field": "FCFO.CODLOJA",
							"type": "integer"
						}
					]
				},
				"internalId": {
					"type": "string",
					"description": "InternalId da entidade"
				},
				"shortName": {
					"type": "string",
					"example": "JOHN M. CO",
					"description": "Nome reduzido ",
					"x-totvs": [
						{
							"product": "protheus",
							"field": "SA1.A1_NREDUZ / SA2.A2_NREDUZ",
							"required": false,
							"type": "char",
							"length": "20",
							"available": true,
							"canUpdate": true
						},
						{
							"product": "datasul",
							"field": "emitente.nome-abrev",
							"required": true,
							"type": "String",
							"length": "12",
							"available": true
						},
						{
							"product": "RM",
							"available": true,
							"required": true,
							"note": "Nome Fantasia do Cliente Fornecedor",
							"field": "FCFO.NOMEFANTASIA",
							"type": "string"
						}
					]
				},
				"name": {
					"type": "string",
					"example": "JOHN & MARY CO",
					"description": "Nome da entidade",
					"maxLength": 40,
					"x-totvs": [
						{
							"product": "protheus",
							"field": "SA1.A1_NOME / SA2.A2_NOME",
							"required": true,
							"type": "char",
							"length": "40",
							"available": true,
							"canUpdate": true
						},
						{
							"product": "datasul",
							"field": "emitente.nome-emit",
							"note": "Nome Completo do Cliente/Fornecedor",
							"required": true,
							"available": true,
							"type": "String",
							"length": "60"
						},
						{
							"product": "RM",
							"available": true,
							"required": true,
							"note": "Razão Social do Cliente Fornecedor",
							"field": "FCFO.NOME",
							"type": "string"
						}
					]
				},
				"type": {
					"type": "string",
					"example": "1",
					"description": "Tipo de Entidade",
					"enum": [
						"1",
						"2",
						"3"
					],
					"x-totvs": [
						{
							"product": "protheus",
							"required": true,
							"type": "integer",
							"length": "1",
							"available": true,
							"canUpdate": false,
							"note": "Identifica se o emitente é apenas Cliente, apenas Fornecedor ou Ambos 1 – Cliente / 2 – Fornecedor / 3 – Ambos"
						},
						{
							"product": "datasul",
							"field": "emitente.identific",
							"required": true,
							"type": "integer",
							"length": "9",
							"note": "Identifica se o emitente é apenas Cliente, apenas Fornecedor ou Ambos 1 – Cliente / 2 – Fornecedor / 3 – Ambos",
							"available": true
						},
						{
							"product": "RM",
							"available": true,
							"required": true,
							"field": "FCFO.TIPOCLIENTE",
							"type": "integer",
							"note": "1=CUSTOMER; 2=VENDOR; 3=BOTH"
						}
					]
				},
				"entityType": {
					"type": "string",
					"example": "1-Company",
					"description": "Situação Jurídica da Entidade",
					"enum": [
						"1",
						"2",
						"3",
						"4"
					],
					"x-totvs": [
						{
							"product": "protheus",
							"field": "SA1.A1_PESSOA",
							"required": false,
							"type": "char",
							"length": "1",
							"available": true,
							"canUpdate": true,
							"note": "F=Fisica; J=Juridica"
						},
						{
							"product": "datasul",
							"field": "emitente.natureza",
							"note": "Identifica se o emitente é Pessoa Física, Jurídica, Estrangeiro ou Trading. 1 – Pessoa Física / 2 – Pessoa Jurídica / 3 – Estrangeiro / 4 – Trading",
							"required": true,
							"type": "integer",
							"length": ">9",
							"available": true
						},
						{
							"product": "RM",
							"available": true,
							"required": true,
							"note": "Identifica se o emitente é Pessoa Física, Jurídica",
							"field": "FCFO.PESSOAFISOUJUR",
							"type": "char"
						}
					]
				},
				"marketsegment": {
					"type": "object",
					"description": "Segmento de Mercado",
					"properties": {
						"marketSegmentCode": {
							"type": "string",
							"description": "Código do segmento",
							"x-totvs": [
								{
									"product": "protheus",
									"field": "A1_CODSEG",
									"required": false,
									"type": "varchar",
									"length": "6",
									"note": "Segmento Principal",
									"available": true
								}
							]
						},
						"marketSegmentInternalId": {
							"type": "string",
							"description": "InternalId do MarketSegmentCode"
						},
						"marketSegmentDescription": {
							"type": "string",
							"description": "Descrição do segmento",
							"x-totvs": [
								{
									"product": "protheus",
									"field": "A1_DESSEG",
									"required": false,
									"type": "varchar",
									"length": "40",
									"note": "Segmento Principal",
									"available": true
								}
							]
						}
					}
				},
				"registerdate": {
					"type": "string",
					"format": "date-time",
					"example": "2018-06-29T09:34:30Z",
					"description": "Data da implantação do registro",
					"x-totvs": [
						{
							"product": "datasul",
							"field": "emitente.data-implant",
							"required": true,
							"type": "date",
							"length": "99/99/9999",
							"available": true,
							"canUpdate": true,
							"note": "Data em que o Cliente/Fornecedor foi cadastrado no sistema"
						},
						{
							"type": "string",
							"format": "date-time",
							"example": "2018-06-29T09:34:30Z",
							"product": "RM",
							"available": true,
							"required": true,
							"note": "Data de Criação",
							"field": "FCFO.DATACRIACAO"
						}
					]
				},
				"registerSituation": {
					"type": "string",
					"example": "1",
					"description": "Status: 1 - Ativo, 2 - Inativo, 3 - Cancelado, 4 - Pendente, 5 -Suspenso",
					"enum": [
						"1",
						"2",
						"3",
						"4",
						"5"
					],
					"x-totvs": [
						{
							"product": "protheus",
							"field": "A1_MSBLQL / A2_MSBLQL",
							"required": false,
							"type": "char",
							"available": true,
							"canUpdate": true,
							"note": "No Protheus Status Ativo/Inativo serão convertidos em: '1' = Inativo; '2' = Ativo"
						},
						{
							"product": "RM",
							"available": true,
							"required": true,
							"note": "Situação do Cliente Fornecedor 0 = inativo, 1 = ativo",
							"field": "FCFO.ATIVO",
							"type": "integer"
						}
					]
				},
				"comments": {
					"type": "string",
					"example": "Lorem ipsum dolor sit amet, consectetur adipiscing elit.",
					"description": "Observações",
					"x-totvs": [
						{
							"product": "datasul",
							"field": "emitente.observacoes",
							"required": true,
							"type": "string",
							"length": "2000",
							"available": true,
							"canUpdate": true,
							"note": "Informações adicionais relacionadas ao Cliente/Fornecedor"
						}
					]
				},
				"governmentalInformation": {
					"type": "array",
					"items": {
						"type": "object",
						"$ref": "https://raw.githubusercontent.com/totvs/ttalk-standard-message/master/jsonschema/schemas/types/GovernmentalInformation_1_000.json#/definitions/GovernmentalInformationType"
					}
				},
				"address": {
					"type": "object",
					"description": "Endereço Principal",
					"$ref": "https://raw.githubusercontent.com/totvs/ttalk-standard-message/master/jsonschema/schemas/types/Address_2_000.json#/definitions/AddressType"
				},
				"shippingAddress": {
					"type": "object",
					"description": "Endereço de entrega",
					"$ref": "https://raw.githubusercontent.com/totvs/ttalk-standard-message/master/jsonschema/schemas/types/Address_2_000.json#/definitions/AddressType"
				},
				"listOfCommunicationInformation": {
					"type": "array",
					"description": "Informações de comunicação",
					"items": {
						"type": "object",
						"$ref": "https://raw.githubusercontent.com/totvs/ttalk-standard-message/master/jsonschema/schemas/types/CommunicationInformation_1_002.json#/definitions/CommunicationInformationType"
					}
				},
				"listOfContacts": {
					"type": "array",
					"description": "Informações de Contato",
					"items": {
						"type": "object",
						"$ref": "https://raw.githubusercontent.com/totvs/ttalk-standard-message/master/jsonschema/schemas/types/ContactInformation_1_000.json#/definitions/ContactInformationType"
					}
				},
				"listOfBankingInformation": {
					"type": "array",
					"items": {
						"type": "object",
						"$ref": "#/definitions/bankingInformation"
					}
				},
				"billingInformation": {
					"type": "object",
					"properties": {
						"billingCustomerCode": {
							"type": "integer",
							"format": "int32",
							"description": "Código do usuário de cobrança",
							"x-totvs": [
								{
									"product": "datasul",
									"field": "emitente.cod-cobranca ou pessoa_jurid.num_pessoa_jurid_cobr",
									"available": true
								}
							]
						},
						"billingCustomerInternalId": {
							"type": "string",
							"description": "Billing Customer InternalId"
						},
						"address": {
							"type": "object",
							"$ref": "https://raw.githubusercontent.com/totvs/ttalk-standard-message/master/jsonschema/schemas/types/Address_2_000.json#/definitions/AddressType"
						}
					}
				},
				"vendorInformation": {
					"type": "object",
					"description": "Dados do Vendedor vinculado ao Cliente",
					"properties": {
						"vendorClassification": {
							"type": "string",
							"description": "1 - Raw Material, 2 - Service, 3 - Both, 4 - Generic",
							"enum": [
								"1",
								"2",
								"3",
								"4"
							]
						},
						"vendorTypeCode": {
							"type": "string",
							"description": "Código do Vendedor",
							"x-totvs": [
								{
									"product": "protheus",
									"field": "SA1.A1_VEND",
									"required": false,
									"type": "string",
									"length": "06",
									"note": "Código do Vendedor vinculado ao Cliente",
									"available": true,
									"canUpdate": true
								}
							]
						},
						"vendorTypeInternalId": {
							"type": "string",
							"description": "InternalId do VendorTypeCode"
						},
						"vendorTypeDescription": {
							"type": "string",
							"description": "Nome/Descrição do Vendedor"
						}
					},
					"required": [
						"vendorTypeCode",
						"vendorTypeInternalId"
					]
				},
				"fiscalInformation": {
					"type": "object",
					"description": "Informações Fiscais",
					"properties": {
						"category": {
							"type": "string",
							"maxLength": 3,
							"description": "Categoria da informação fiscal"
						},
						"isRetentionAgent": {
							"type": "boolean",
							"description": "Retem impostos"
						},
						"taxPayer": {
							"type": "array",
							"items": {
								"type": "object",
								"$ref": "#/definitions/taxPayerType"
							}
						}
					}
				},
				"creditInformation": {
					"type": "object",
					"description": "Informações de Crédito",
					"properties": {
						"creditIndicator": {
							"type": "integer",
							"format": "int32",
							"description": "Indicador de Crédito"
						},
						"creditEvaluation": {
							"type": "integer",
							"format": "int32",
							"description": "Avaliação de Crédito"
						},
						"shipmentCreditEvaluation": {
							"type": "integer",
							"format": "int32",
							"description": "Avaliação de Crédito para venda"
						},
						"creditLimit": {
							"type": "number",
							"format": "double",
							"description": "Limite de Crédito",
							"x-totvs": [
								{
									"product": "protheus",
									"field": "SA1.A1_LC",
									"required": false,
									"type": "Integer",
									"format": "int32",
									"length": "14",
									"note": "Lim. Credito",
									"available": true,
									"canUpdate": true
								}
							]
						},
						"creditLimitCurrency": {
							"type": "integer",
							"format": "int32",
							"description": "Moeda do Limite de Crédito",
							"x-totvs": [
								{
									"product": "protheus",
									"field": "SA1.A1_MOEDALC",
									"required": false,
									"type": "Integer",
									"length": "02",
									"note": "Moeda do LC",
									"available": true,
									"canUpdate": true
								}
							]
						},
						"creditLimitDate": {
							"type": "string",
							"format": "date-time",
							"example": "2018-06-29T09:34:30Z",
							"description": "Data de vencimento do Limite de Crédito",
							"x-totvs": [
								{
									"product": "protheus",
									"field": "SA1.A1_VENCLC",
									"required": false,
									"type": "Date-Time",
									"length": "08",
									"note": "Moeda do LC",
									"available": true,
									"canUpdate": true
								}
							]
						},
						"additionalCreditLimit": {
							"type": "number",
							"format": "double",
							"description": "Limite de Crédito adicional",
							"x-totvs": [
								{
									"product": "protheus",
									"field": "SA1.A1_LCFIN",
									"required": false,
									"type": "Integer",
									"format": "int32",
									"length": "14",
									"note": "Lim Credito Secundário",
									"available": true,
									"canUpdate": true
								}
							]
						},
						"additionalCreditLimitCurrency": {
							"type": "string",
							"description": "Moeda do Limite de Crédito adicional"
						},
						"additionalCreditLimitDate": {
							"type": "string",
							"format": "date-time",
							"example": "2018-06-29T09:34:30Z",
							"description": "Data de vencimento do Limite de Crédito adicional"
						},
						"latePeriods": {
							"type": "integer",
							"format": "int32"
						},
						"balanceOfCredit": {
							"type": "number",
							"format": "double",
							"description": "Valor do saldo restante do Cliente"
						}
					}
				},
				"paymentConditionCode": {
					"type": "string",
					"description": "Código do Prazo de Pagamento",
					"maxLength": 3,
					"x-totvs": [
						{
							"product": "protheus",
							"field": "SA1.A1_COND | SA2.A2_COND",
							"required": false,
							"type": "char",
							"length": "03",
							"note": "Condição de Pagamento",
							"available": true,
							"canUpdate": true
						}
					]
				},
				"paymentConditionInternalId": {
					"type": "string",
					"description": "InternalId do PaymentConditionCode"
				},
				"priceListHeaderItemCode": {
					"type": "string",
					"description": "Código da Tabela de Preço",
					"maxLength": 3,
					"x-totvs": [
						{
							"product": "protheus",
							"field": "SA1.A1_TABELA",
							"required": false,
							"type": "char",
							"length": "03",
							"note": "Tabela de preço padrão",
							"available": true,
							"canUpdate": true
						}
