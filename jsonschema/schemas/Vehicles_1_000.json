{
  "$schema": "https://raw.githubusercontent.com/totvs/ttalk-standard-message/master/jsonschema/schemas/Vehicles_1_000.json#",
  "info": {
    "description": "",
    "version": "1.000",
    "title": "Vehicles",
    "contact": {},
    "x-totvs": {
      "messageDocumentation": {
        "name": "Vehicles",
        "description": "Veículos",
        "segment": ""
      },
      "transactionDefinition": {
        "subType": "event",
        "businessContentType": {
          "$ref": "#/definitions/BusinessContentType",
          "type": "object"
        }
      }
    }
  },
  "definitions": {
    "BusinessContentType": {
      "type": "object",
      "properties": {
        "BranchId": {
          "type": "string",
          "maxLength": 12
        },
        "Code": {
          "description": "Codigo do Veiculo",
          "type": "string",
          "maxLength": 6
        },
        "Description": {
          "description": "Modelo",
          "type": "string",
          "maxLength": 30
        },
        "Plate": {
          "$ref": "#/definitions/PlateType",
          "type": "object"
        },
        "BrandCode": {
          "description": "Para o codigo de marca deve haver um De/Para",
          "type": "string"
        },
        "ColorCode": {
          "description": "Para o codigo da cor deve haver um De/Para",
          "type": "string"
        },
        "TypeCode": {
          "description": "Para o codigo do tipo de veiculo deve haver um De/Para",
          "type": "string"
        },
        "Chassis": {
          "description": "Número Chassi",
          "type": "string",
          "maxLength": 20
        },
        "RenavamCode": {
          "description": "Codigo RENAVAM",
          "type": "string",
          "maxLength": 10
        },
        "ListOfTrackers": {
          "description": "Rastreadores vinculados ao veiculo",
          "type": "array",
          "items": {
            "$ref": "#/definitions/ListOfTrackerType",
            "type": "object"
          }
        }
      },
      "required": [
        "Code"
      ]
    },
    "PlateType": {
      "type": "object",
      "properties": {
        "Number": {
          "description": "Placa",
          "type": "string",
          "maxLength": 8
        },
        "State": {
          "$ref": "#/definitions/StateType",
          "type": "object"
        },
        "City": {
          "$ref": "#/definitions/CityType",
          "type": "object"
        }
      }
    },
    "StateType": {
      "type": "object",
      "properties": {
        "Code": {
          "type": "string"
        },
        "Description": {
          "type": "string"
        }
      }
    },
    "CityType": {
      "type": "object",
      "properties": {
        "Code": {
          "type": "string"
        },
        "Description": {
          "type": "string"
        }
      }
    },
    "ListOfTrackerType": {
      "type": "array",
      "items": {
        "$ref": "#/definitions/TrackerType",
        "type": "object"
      }
    },
    "TrackerType": {
      "type": "object",
      "properties": {
        "Code": {
          "description": "Código do rastreador",
          "type": "string",
          "maxLength": 15
        },
        "Description": {
          "description": "Descricao do rastreador",
          "type": "string",
          "maxLength": 30
        },
        "ManufacturerID": {
          "description": "CNPJ fabricante do rastreador",
          "type": "string",
          "maxLength": 15
        }
      }
    }
  }
}