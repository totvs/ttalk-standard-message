        }
      }
    }
  },
  "definitions": {
    "RequestType": {
      "type": "object",
      "properties": {
        "CompanyId": {
          "description": "Código da Empresa",
          "type": "string",
          "maxLength": 3,
          "x-totvs": [
            {
              "product": "PROTHEUS",
              "field": "",
              "required": true,
              "type": "varchar",
              "length": "2",
              "note": "Será enviado o valor da variável cEmpAnt( empresa)",
              "available": true,
              "canUpdate": false
            }
          ]
        },
        "BranchId": {
          "description": "Código da Filial",
          "type": "string",
          "x-totvs": [
            {
              "product": "PROTHEUS",
              "field": "",
              "required": false,
              "type": "char",
              "length": "12",
              "note": "Será enviado o valor da variável cFilAnt",
              "available": true,
              "canUpdate": false
            }
          ]
        },
        "CompanyInternalId": {
          "description": "InternalId das informações da empresa",
          "type": "string",
          "x-totvs": [
            {
              "product": "PROTHEUS",
              "field": "cEmpAnt|xFilial(\"SE1\")",
              "required": true,
              "type": "varchar",
              "length": "50",
              "note": "o InternalID da empresa é formado empresa|E1_FILIAL. Tamanho entre 05 e 15",
              "available": true,
              "canUpdate": false
            },
            {
              "product": "RM",
              "field": "CODCOLIGADA+CODFILIAL",
              "required": false,
              "type": "string",
              "length": "",
              "note": "",
              "available": true,
              "canUpdate": false
            }
          ]
        },
        "DocumentInternalId": {
          "description": "Identificados Interno do Título",
          "type": "string",
          "x-totvs": [
            {
              "product": "PROTHEUS",
              "field": "tabela.campo",
              "required": false,
              "type": "char",
              "length": "10",
              "note": "Código que irá identificar o titulo a receber.",
              "available": true,
              "canUpdate": false
            }
          ]
        },
        "DueDate": {
          "description": "Data de Vencimento",
          "type": "string",
          "format": "date",
          "x-totvs": [
            {
              "product": "PROTHEUS",
              "field": "SE1XX0.E1_VENCREA",
              "required": true,
              "type": "date",
              "length": "8",
              "note": "",
              "available": true,
              "canUpdate": false
            }
          ]
        },
        "BankingCode": {
          "description": "Código do banco",
          "type": "string",
          "x-totvs": [
            {
              "product": "PROTHEUS",
              "field": "SE1.E1_PORTADO",
              "required": true,
              "type": "varchar",
              "length": "3",
              "note": "Código do Banco portador do título",
              "available": true,
              "canUpdate": false
            }
          ]
        },
        "Agency": {
          "description": "Agencia do banco",
          "type": "string",
          "x-totvs": [
            {
              "product": "PROTHEUS",
              "field": "SE1XX0.E1_AGEDEP",
              "required": true,
              "type": "varchar",
              "length": "5",
              "note": "",
              "available": true,
              "canUpdate": false
            }
          ]
        },
        "Account": {
          "description": "Conta bancária",
          "type": "string",
          "x-totvs": [
            {
              "product": "PROTHEUS",
              "field": "SE1XX0.E1_CONTA",
              "required": true,
              "type": "varchar",
              "length": "5",
              "note": "",
              "available": true,
              "canUpdate": false
            }
          ]
        },
        "Contract": {
          "description": "Contrato bancário (Convênio)",
          "type": "string",
          "x-totvs": [
            {
              "product": "PROTHEUS",
              "field": "SE1XX0.E1_CONTRAT",
              "required": true,
              "type": "varchar",
              "length": "15",
              "note": "",
              "available": true,
              "canUpdate": false
            }
          ]
        }
      }
    },
    "ReturnContentType": {
      "type": "object",
      "properties": {
        "DocumentInternalId": {
          "description": "Identificados Interno do Título",
          "type": "array",
          "minItems": 1,
          "items": {
            "$ref": "https://raw.githubusercontent.com/totvs/ttalk-standard-message/master/jsonschema/schemas/types/ListOfInternalId_1_000.json#/definitions/InternalIdType",
