          },
          "x-totvs": [
            {
              "product": "PROTHEUS",
              "field": "tabela.campo",
              "required": false,
              "type": "char",
              "length": "10",
              "note": "Código que irá identificar o titulo a receber.",
              "available": true,
              "canUpdate": false
            }
          ]
        },
        "OurNumber": {
          "description": "Nosso Número gerado",
          "type": "array",
          "minItems": 1,
          "items": {
            "type": "string"
          },
          "x-totvs": [
            {
              "product": "PROTHEUS",
              "field": "SE1XX0.E1_NUMBCO",
              "required": true,
              "type": "varchar",
              "length": "15",
              "note": "",
              "available": true,
              "canUpdate": false
            }
          ]
        },
        "Barcode": {
          "description": "Código de Barras",
          "type": "string",
          "x-totvs": [
            {
              "product": "PROTHEUS",
              "field": "SE1XX0.E1_CODBAR",
              "required": true,
              "type": "varchar",
              "length": "44",
              "note": "",
              "available": true,
              "canUpdate": false
            }
          ]
        }
      },
      "required": [
        "DocumentInternalId",
        "OurNumber"
      ]
