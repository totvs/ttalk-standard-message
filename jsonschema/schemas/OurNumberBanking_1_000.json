    "x-totvs": {
      "messageDocumentation": {
        "name": "OurNumberBanking",
        "description": "Requisição de Nosso Número bancário",
        "segment": "Controladoria"
      },
      "productInformation": [
        {
          "product": "PROTHEUS",
          "contact": "GDP - Rogerio Nagy",
          "note": "",
          "adapter": "FINI150a"
        }
      ],
      "transactionDefinition": {
