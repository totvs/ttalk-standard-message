{
  "$schema": "https://raw.githubusercontent.com/totvs/ttalk-standard-message/appointment/V1/1_000/jsonschema/schemas/Practitioner_1_000.json#",
  "info": {
    "description": "API para a entidade prestador (médicos) para produtos TOTVS",
    "version": "1.000",
    "title": "practitioner",
    "contact": {
      "name": "T-Talk",
      "url": "https://api.totvs.com.br",
      "email": "comiteintegracao@totvs.com.br"
    },
    "x-totvs": {
      "messageDocumentation": {
        "name": "Practitioner",
        "description": "Informações de prestadores de saúde (médicos)",
        "segment": "Saúde"
      },
      "productInformation": [
        {
          "product": "RM",
          "contact": "TOTVS.SAUDE.SQUAD.GHASSIST@totvs.com.br",
          "description": "Cadastro de prestadores (médicos)",
          "adapter": ""
        }
      ]
    }
  },
  "definitions": {
    "PagedPractitioner": {
      "type": "object",
      "allOf": [
        {
          "$ref": "https://raw.githubusercontent.com/totvs/ttalk-standard-message/master/jsonschema/apis/types/totvsApiTypesBase.json#/definitions/Paging"
        },
        {
          "$ref": "#/definitions/Practitioner"
        }
      ]
    },
    "Practitioner": {
            "type": "object",
            "properties": {
                "items": {
                    "type": "array",
                    "items": {
                     "$ref": "#/definitions/PractitionerInfo"
					}
               }
            }
    },    
    "PractitionerInfo": {
      "type": "object",
      "properties": {
        "code": {
          "description": "Identificador do registro",
          "required": false,
          "type": "string",
          "example": "1$_$1",
          "x-totvs": [
            {
              "product": "RM",
              "field": "SZCADGERAL.CODCOLIGADA | SZCADGERAL.IDENTIDADEPROF",
              "required": false,
              "type": "String",
              "available": true,
              "canUpdate": false,
              "note": "Este dado e formado pelos campos Código da Coligada e Código do Prestador"
            }
          ]
        },
		"practitionerId": {
          "description": "Código do prestador",
          "required": true,
          "type": "integer",
          "exemple": 1,
          "x-totvs": [
            {
              "product": "RM",
              "field": "SZCADGERAL.CODGERAL",
              "required": true,
              "type": "integer",
              "available": true,
              "canUpdate": true,
              "note": "Código do prestador"
            }
          ]
        },
		"companyId": {
          "description": "Código da coligada",
          "required": true,
          "type": "integer",
          "exemple": 1,
          "x-totvs": [
            {
              "product": "RM",
              "field": "SZCADGERAL.CODCOLIGADA",
              "required": true,
              "type": "integer",
              "available": true,
              "canUpdate": true,
              "note": "Código da Coligada"
            }
          ]
        },
        "socialName": {
          "description": "Razão Social",
          "required": false,
          "type": "string",
          "example": "Lucas Pereira",
          "x-totvs": [
            {
              "product": "RM",
              "field": "SZCADGERAL.RAZAOSOCIAL",
              "required": false,
              "type": "string",
              "available": true,
              "canUpdate": true,
              "note": "Razão Social"
            }
          ]
        },
        "councilName": {
          "description": "Nome do conselho profissional",
          "required": false,
          "type": "string",
          "example": "CRN",
          "x-totvs": [
            {
              "product": "RM",
              "field": "SZCONSPROF.NOMECONSPROF",
              "required": false,
              "type": "string",
              "available": true,
              "canUpdate": true,
              "note": "Nome do conselho"
            }
          ]
        },
        "professionalId": {
          "description": "Identidade profissional do prestador",
          "required": false,
          "type": "string",
          "exemple": 123456,
          "x-totvs": [
            {
              "product": "RM",
              "field": "SZCADGERAL.IDENTIDADEPROF",
              "required": false,
              "type": "integer",
              "available": true,
              "canUpdate": true,
              "note": "Identidade profissional do prestador"
            }
          ]
        },
        "workPhone": {
          "description": "Telefone comercial",
          "required": false,
          "type": "string",
          "exemple": "984842255",
          "x-totvs": [
            {
              "product": "RM",
              "field": "SZCADGERAL.TELCOM",
              "required": false,
              "type": "string",
              "available": true,
              "canUpdate": true,
              "note": "Número do telefone celular"
            }
          ]
        },
		"cellPhone": {
          "description": "Telefone celular",
          "required": false,
          "type": "string",
          "exemple": "984842255",
          "x-totvs": [
            {
              "product": "RM",
              "field": "SZCADGERAL.CELULAR",
              "required": false,
              "type": "string",
              "available": true,
              "canUpdate": true,
              "note": "Número do telefone celular"
            }
          ]
        },
		"email": {
          "description": "email para contato",
          "required": false,
          "type": "string",
          "exemple": "lucas@totvs.com.br",
          "x-totvs": [
            {
              "product": "RM",
              "field": "SZCADGERAL.EMAIL",
              "required": false,
              "type": "string",
              "available": true,
              "canUpdate": true,
              "note": "email para contato"
            }
          ]
        },
		"sex": {
          "description": "Sexo do prestador",
          "required": false,
          "type": "string",
          "exemple": "M",
          "x-totvs": [
            {
              "product": "RM",
              "field": "SZCADGERAL.SEXO",
              "required": false,
              "type": "string",
              "available": true,
              "canUpdate": true,
              "note": "Sexo do prestador(M para masculino, F para feminino)"
            }
          ]
        },
		"address": {
          "description": "endereço do prestador",
          "required": false,
          "type": "string",
          "exemple": "rua ouro preto",
          "x-totvs": [
            {
              "product": "RM",
              "field": "SZCADGERAL.ENDERECO",
              "required": false,
              "type": "string",
              "available": true,
              "canUpdate": true,
              "note": "endereço do prestador"
            }
          ]
        },
        "number": {
          "description": "número do endereço",
          "required": false,
          "type": "string",
          "exemple": "334",
          "x-totvs": [
            {
              "product": "RM",
              "field": "SZCADGERAL.NUMERO",
              "required": false,
              "type": "string",
              "available": true,
              "canUpdate": true,
              "note": "número do endereço"
            }
          ]
        },
        "complement": {
          "description": "complemento do endereço",
          "required": false,
          "type": "string",
          "exemple": "ap 301",
          "x-totvs": [
            {
              "product": "RM",
              "field": "SZCADGERAL.COMPLEMENTO",
              "required": false,
              "type": "string",
              "available": true,
              "canUpdate": true,
              "note": "complemento do endereço"
            }
          ]
        },
		"neighborhood": {
          "description": "bairro",
          "required": false,
          "type": "string",
          "exemple": "cristo redentor",
          "x-totvs": [
            {
              "product": "RM",
              "field": "SZCADGERAL.BAIRRO",
              "required": false,
              "type": "string",
              "available": true,
              "canUpdate": true,
              "note": "bairro do endereço"
            }
          ]
        },
		"city": {
          "description": "cidade",
          "required": false,
          "type": "string",
          "exemple": "porto alegre",
          "x-totvs": [
            {
              "product": "RM",
              "field": "SZCADGERAL.CIDADE",
              "required": false,
              "type": "string",
              "available": true,
              "canUpdate": true,
              "note": "cidade"
            }
          ]
        },
		"state": {
          "description": "UF do prestador",
          "required": false,
          "type": "string",
          "exemple": "RS",
          "x-totvs": [
            {
              "product": "RM",
              "field": "SZCADGERAL.UF",
              "required": false,
              "type": "string",
              "available": true,
              "canUpdate": true,
              "note": "UF do prestador"
            }
          ]
        },
		"postalCode": {
          "description": "código postal / CEP",
          "required": false,
          "type": "string",
          "exemple": "91040600",
          "x-totvs": [
            {
              "product": "RM",
              "field": "SZCADGERAL.CEP",
              "required": false,
              "type": "string",
              "available": true,
              "canUpdate": true,
              "note": "código postal / CEP"
            }
          ]
        }                          
      }
    }
  }
}