{
	"$schema": "https://raw.githubusercontent.com/totvs/ttalk-standard-message/openapi-dereferenciator/jsonschema/schemas/CurrencyQuote_1_001.json#",
	"info": {
		"description": "",
		"version": "1.001",
		"title": "CurrencyQuote",
		"contact": {},
		"x-totvs": {
			"messageDocumentation": {
				"name": "CurrencyQuote",
				"description": "Mensagem de integração de cotação de moedas.",
				"segment": "Geral"
			},
			"productInformation": [
				{
					"product": "PROTHEUS",
					"contact": "Rogerio Nagy",
					"note": "PO Contas a Receber.",
					"adapter": "MATA090"
				}
			],
			"transactionDefinition": {
				"subType": "event",
				"businessContentType": {
					"$ref": "#/definitions/CurrencyQuoteInfo",
					"type": "object"
				},
				"returnContentType": {
					"$ref": "#/definitions/ReturnContentType",
					"type": "object"
				}
			}
		}
	},
	"definitions": {
		"PagedCurrencyQuote": {
			"type": "object",
			"allOf": [
				{
					"$ref": "https://raw.githubusercontent.com/totvs/ttalk-standard-message/openapi-dereferenciator/jsonschema/apis/types/totvsApiTypesBase.json#/definitions/Paging"
				},
				{
					"$ref": "#/definitions/CurrencyQuotes"
				}
			]
		},
		"CurrencyQuotes": {
			"type": "object",
			"properties": {
				"items": {
					"type": "array",
					"items": {
						"$ref": "#/definitions/CurrencyQuoteInfo"
					}
				}
			}
		},
		"CurrencyQuoteInfo": {
			"type": "object",
			"properties": {
				"CompanyId": {
					"type": "string",
					"description": "Código da Empresa",
					"example": "T1 ",
					"maxLength": 3
				},
				"BranchId": {
					"type": "string",
					"description": "Código da Filial",
					"example": "D MG 01 ",
					"maxLength": 12,
					"x-totvs": [
						{
							"product": "Protheus",
							"field": "SM2.M2_FILIAL",
							"required": false,
							"length": "8",
							"available": false,
							"canUpdate": false
						}
					]
				},
				"CompanyInternalId": {
					"type": "string",
					"description": "Identificador da Empresa",
					"x-totvs": [
						{
							"product": "protheus",
							"field": "EMPRESA|SM2.M2_DATA",
							"required": true,
							"type": "varchar",
							"length": "16",
							"available": true,
							"canUpdate": false
						}
					]
				},
				"Date": {
					"description": "Data da cotação",
					"type": "string",
					"format": "date",
					"x-totvs": [
						{
							"product": "PROTHEUS",
							"field": "M2_DATA",
							"required": true,
							"type": "Date",
							"length": "10",
							"note": "",
							"available": true,
							"canUpdate": false
						}
					]
				},
				"ListOfCurrency": {
					"description": "Cotações das moedas",
					"type": "array",
					"maxItems": 99,
					"items": {
						"$ref": "#/definitions/CurrencyType",
						"type": "object"
					}
				}
			},
			"required": [
				"CompanyId",
				"BranchId",
				"CompanyInternalId",
				"Date",
				"ListOfCurrency"
			]
		},
		"CurrencyType": {
			"type": "object",
			"properties": {
				"Code": {
					"description": "Código da moeda (número de 1 a 99)",
					"type": "integer",
					"minimum": 1,
					"maximum": 99,
					"format": "int32",
					"x-totvs": [
						{
							"product": "PROTHEUS",
							"field": "",
							"required": true,
							"type": "varchar",
							"length": "2",
							"note": "",
							"available": true,
							"canUpdate": false
						}
					]
				},
				"Name": {
					"description": "Nome da moeda (Real, Dólar, Euro...)",
					"type": "string",
					"x-totvs": [
						{
							"product": "PROTHEUS",
							"field": "MV_MOEDA + código da moeda (ex: MV_MOEDA1, MV_MOEDA2... MV_MOEDA99)",
							"required": false,
							"type": "varchar",
							"length": "250",
							"note": "",
							"available": false,
							"canUpdate": false
						}
					]
				},
				"Symbol": {
					"description": "Súmbolo da moeda (R$, US$, €...)",
					"type": "string",
					"x-totvs": [
						{
							"product": "PROTHEUS",
							"field": "MV_SIMB + código da moeda (ex: MV_SIMB1, MV_SIMB2... MV_SIMB99)",
							"required": false,
							"type": "varchar",
							"length": "250",
							"note": "",
							"available": false,
							"canUpdate": false
						}
					]
				},
				"Quote": {
					"description": "Cotação da moeda",
					"type": "number",
					"format": "double",
					"x-totvs": [
						{
							"product": "PROTHEUS",
							"field": "M2_MOEDA + código da moeda (ex: M2_MOEDA1, M2_MOEDA2... M2_MOEDA99)",
							"required": true,
							"type": "Numérico",
							"length": "11v4",
							"note": "",
							"available": true,
							"canUpdate": true
						}
					]
				}
			},
			"required": [
				"Code",
				"Quote"
			]
		},
		"ReturnContentType": {
			"type": "object",
			"properties": {
				"ListOfInternalId": {
					"type": "array",
					"items": {
						"$ref": "https://raw.githubusercontent.com/totvs/ttalk-standard-message/openapi-dereferenciator/jsonschema/schemas/types/ListOfInternalId_1_000.json#/definitions/ListOfInternalIdType",
						"type": "object"
					}
				}
			}
		}
	}
}