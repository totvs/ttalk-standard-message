              "field": "Ordem_sup_txt.tex_observ_oc",
              "required": true,
              "type": "char",
              "length": "60",
              "note": "",
              "available": true,
              "canUpdate": false
            }
          ]
        },
        "Application": {
          "description": "Aplicação da ordem de compra",
          "type": "string",
          "x-totvs": [
            {
              "product": "LOGIX",
              "field": "Grupo_ctr_desp.ies_obj_entrada",
              "required": true,
              "type": "char",
              "length": "20",
              "note": "Industrialização Comercialização Ativo imobilizado Material de consumo Outros Ativo CIAP Material de processo Serviço",
              "available": true,
              "canUpdate": false
            }
          ]
        },
        "ItemInternalId": {
          "description": "InternalId do ItemCOde",
          "type": "string"
        },
        "ItemReferenceCode": {
          "description": "Código de Referência do Produto",
          "type": "string",
          "minLength": 1,
          "maxLength": 20
        },
        "UnitPrice": {
          "description": "Preço Unitário",
          "type": "number",
          "multipleOf": 0.01,
          "minimum": -9999999999.99,
          "maximum": 9999999999.99
        },
        "TotalPrice": {
          "description": "Preço Total do Item",
          "type": "number",
          "multipleOf": 0.01,
          "minimum": -9999999999.99,
          "maximum": 9999999999.99
        },
        "Quantity": {
          "description": "Quantidade",
          "type": "number",
          "multipleOf": 0.01,
          "minimum": -9999999999.99,
          "maximum": 9999999999.99
        },
