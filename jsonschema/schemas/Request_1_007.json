          "type": "array",
          "items": {
            "type": "number"
          }
        },
        "ListOfDeliverySchedule": {
          "description": "Lista de programações de entrega",
          "type": "array",
          "items": {
            "type": "string"
          }
        },
        "Text": {
          "description": "Textos da ordem de compra",
          "type": "string",
