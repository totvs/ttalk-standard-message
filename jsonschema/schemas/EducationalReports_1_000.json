{
	"$schema": "https://raw.githubusercontent.com/totvs/ttalk-standard-message/master/jsonschema/schemas/events/EducationalReports_1_000.json#",
	"info": {
		"description": "API para a entidade relatório educacional (EducationalReports) para produtos TOTVS",
		"version": "1.000",
		"title": "EducationalReports",
		"contact": {
			"name": "T-Talk",
			"url": "API.Totvs.com.br",
			"email": "comiteintegracao@totvs.com.br"
		},
		"x-totvs": {
			"messageDocumentation": {
				"name": "EducationalReports",
				"description": "Relatórios Educacionais",
				"segment": "Educacional"
			},
			"productInformation": [{ 
				"product": "RM",
				"contact": "rm_FW@totvs.com.br",
				"description": "Listagem dos relatórios educacionais"
			}]
		}
	},
	"definitions": {
		"PagedEducationalReports": {
			"type": "object",
			"allOf": [{
				"$ref": "https://raw.githubusercontent.com/totvs/ttalk-standard-message/master/jsonschema/apis/types/totvsApiTypesBase.json#/definitions/Paging"
			},
			{
				"type": "object",
				"properties": {
					"items": {
						"type": "array",
						"items": {
							"$ref": "#/components/schemas/EducationalReportsInfo"
						}
					}
				}
			}]
		}
    },
	"EducationalReportsInfo": {
		"type": "object",
        "properties": {
			"CompanyCode": {
				"type": "string",
				"example": "1",
				"description": "Código da Empresa",
				"x-totvs": [{
					"product": "RM",
					"field": "RRPTREPORT.CODCOLIGADA",
					"required": false,
					"type": "integer",
					"length": 2,
					"available": true,
					"canUpdate": false
				}]
			},
			"CompanyInternalId": {
				"type": "string",
				"description": "InternalId Company",
				"x-totvs": [{ 
					"product": "RM",
					"field": "GCOLIGADA.CODCOLIGADA",
					"required": false,
					"type": "integer",
					"length": 2,
					"available": true,
					"canUpdate": false
				}]
			},
			"BranchCode": {
				"type": "string",
				"example": "2",
				"description": "Código da Filial",
				"x-totvs": [{ 
					"product": "RM",
					"field": "GFILIAL.CODFILIAL",
					"required": true,
					"type": "integer",
					"length": 2,
					"available": true,
					"canUpdate": false
				}]
			},
			"BranchInternalId": {
				"type": "string",
				"description": "InternalId Branch",
				"x-totvs": [{ 
					"product": "RM",
					"field": "GFILIAL.CODCOLIGADA|GFILIAL.CODFILIAL",
					"required": false,
					"available": true,
					"note": "A filial na linha RM é composta por CODCOLIGADA e CODFILIAL"
				}]
			},
			"LevelEducationCode": {
				"type": "integer",
				"format": "int32",
				"example": 1,
				"description": "Nível de ensino",
				"x-totvs": [{
					"product": "RM",
					"field": "STIPOCURSO.CODTIPOCURSO",
					"required": true,
					"type": "integer",
					"length": 2,
					"available": true,
					"canUpdate": false
				}]
			},
			"LevelEducationInternalId": {
				"type": "string",
				"description": "InternalId Level Education",
				"x-totvs": [{
					"product": "RM",
					"field": "STIPOCURSO.CODCOLIGADA|STIPOCURSO.CODTIPOCURSO",
					"required": false,
					"available": true,
					"note": "O nível de ensino na linha RM é composto pelos campos CODCOLIGADA e CODTIPOCURSO"
				}]
			},
			"ReportCompanyCode": {
				"type": "string",
				"example": "1",
				"description": "Coligada do Relatório",
				"x-totvs": [{
					"product": "RM", 
					"field": "RRPTREPORT.CODCOLIGADA",
					"required": true,
					"type": "integer",
					"length": 4,
					"available": true,
					"canUpdate": false
				}]
			},
			"ReportId": {
				"type": "integer",
				"format": "int32",
				"example": 459,
				"description": "Identificador do Relatório",
				"x-totvs": [{
					"product": "RM",
					"field": "RRPTREPORT.ID",
					"required": true,
					"type": "integer",
					"length": 4,
					"available": true,
					"canUpdate": false
				}]
			},
			"ReportInternalId": {
				"type": "string",
				"description": "InternalId Report",
				"x-totvs": [{
					"product": "RM", 
					"field": "GFONTEDADOS.ID",
					"type": "integer",
					"length": 4,
					"required": false,
					"available": true
				}]
			},
			"ApplicationCode": {
				"type": "string",
				"example": "S",
				"description": "Código da Aplicação",
				"x-totvs": [{ 
					"product": "RM", 
					"field": "RRPTREPORT.CODAPLICACAO",
					"required": true,
					"type": "varchar",
					"length": 1,
					"available": true,
					"canUpdate": false
				}]
			},
			"ApplicationInternalId": {
				"type": "string",
				"description": "InternalId Application",
				"x-totvs": [{ 
					"product": "RM", 
					"field": "GSISTEMA.CODSISTEMA",
					"type": "varchar",
					"length": 1,
					"required": false,
					"available": true
				}]
			},
			"ReportCode": {
				"type": "string",
				"example": "EDUREL0049",
				"description": "Código do Relatório",
				"x-totvs": [{
					"product": "RM", 
					"field": "RRPTREPORT.CODIGO",
					"required": true,
					"type": "varchar",
					"length": 30,
					"available": true,
					"canUpdate": false
				}]
			},
			"ReportDescription": {
				"type": "string",
				"example": "Diário de classe - Frequência",
				"description": "Descrição do Relatório",
				"x-totvs": [{
					"product": "RM", 
					"field": "RRPTREPORT.DESCRICAO",
					"required": true,
					"type": "varchar",
					"length": 60,
					"available": true,
					"canUpdate": false
				}]
			},
			"ReportTypeCode": {
				"type": "integer",
				"format": "int32",
				"example": 1,
				"description": "Código do Tipo do Relatório",
				"x-totvs": [{
					"product": "RM", 
					"field": "RRPTREPORT.TIPO",
					"required": true,
					"type": "integer",
					"length": 4,
					"available": true,
					"canUpdate": false
				}]
			},
			"ReportTypeInternalId": {
				"type": "string",
				"description": "InternalId ReportType",
				"x-totvs": [{ 
					"product": "RM", 
					"field": "RRELTIP.ID",
					"type": "integer",
					"length": 4,
					"required": false,
					"available": true
				}]
			},
			"ReportTypeDescription": {
				"type": "string",
				"example": "GENERICO",
				"description": "Descrição do Tipo do Relatório",
				"x-totvs": [{
					"product": "RM", 
					"field": "RRELTIP.CODTIPO",
					"required": true,
					"type": "varchar",
					"length": 30,
					"available": true,
					"canUpdate": false
				}]
			},
			"InternalId": {
				"type": "string",
				"description": "InternalId da mensagem",
				"x-totvs": [{
					"product": "RM", 
					"note": "Esta listagem considera sempre a parametrização, por isso é necessário que o internalid tenha os dados do contexto da parametrização",
					"field": "CODCOLIGADA|CODFILIAL|CODTIPOCURSO|CODCOLIGADARELATORIO|IDRELATORIO",
					"required": false,
					"available": true
				}]
			}
        }
    }	
}