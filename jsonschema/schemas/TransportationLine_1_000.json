{
   "$schema":"https://raw.githubusercontent.com/totvs/ttalk-standard-message/TransportationLine/v1/1_000/jsonschema/schemas/TransportationLine_1_000.json#",
   "info":{
      "description":"Contrato de Mensagem Padronizada para linhas de transporte de produtos TOTVS",
      "version":"1.000",
      "title":"TransportationLine",
      "contact":{
         "name":"T-Talk",
         "url":"API.Totvs.com.br",
         "email":"comiteintegracao@totvs.com.br"
      },
      "x-totvs":{
         "messageDocumentation":{
            "name":"TransportationLine",
            "description":"Linhas de Transporte",
            "segment":"RH"
         },
         "productInformation":[
            {
               "product":"RM",
               "contact":"tiago.santana@totvs.com.br",
               "description":"Linhas de Transporte",
               "adapter":"",
               "helpUrl":"link aqui"
            }
         ]
      }
   },
   "definitions":{
      "PagedTransportationLine":{
         "type":"object",
         "allOf":[
            {
               "$ref":"https://raw.githubusercontent.com/totvs/ttalk-standard-message/master/jsonschema/apis/types/totvsApiTypesBase.json#/definitions/Paging"
            },
            {
               "type":"object",
               "properties":{
                  "items":{
                     "type":"array",
                     "items":{
                        "$ref":"#/definitions/TransportationLineInfo"
                     }
                  }
               }
            }
         ]
      },
      "TransportationLineInfo":{
         "description":"Descreve uma linha de transporte",
         "properties":{
            "id":{
               "description":"Identificador da linha de transporte",
               "type":"string"
            },
            "companyId": {
			  "description": "Empresa",
			  "type": "int"
			},
			"code": {
			  "description": "Código da linha de transporte",
			  "type": "string"
			},
			"name": {
			  "description": "Nome da linha de transporte",
			  "type": "string"
			},
			"feeCode": {
			  "description": "Código da tarifa adotada na linha de transporte",
			  "type": "string"
			},
            "transportationGroupCode":{
               "description":"Código do grupo de compra de vale transporte. No RM, o código do grupo deve corresponder ao código de um registro da tabela dinâmica INT20",
               "type":"string"
            },
			"transportationType":{
               "description":"Tipo de transporte. Pode assumir os valores B, I, M, N, O, T, que representam os transportes Barca, Integração, Metrô, Bonde, Ônibus e Trem, respectivamente",
               "type":"string"
            },
			"itineraryCode":{
               "description":"Código do itinerário da linha",
               "type":"string"
            },
			"single":{
               "description":"Utiliza apenas um vale transporte no mês",
               "type":"bool"
            },
			"active":{
               "description":"A linha está ativa",
               "type":"bool"
            }
         }
      }
   }
}