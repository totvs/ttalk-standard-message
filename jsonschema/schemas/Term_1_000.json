					"description": "Código da empresa",
					"type": "string",
					"example": 1,
					"x-totvs": [
						{
							"product": "RM",
							"available": true,
							"required": true,
							"note": "Código da empresa",
							"field": "SPLETIVO.CODCOLIGADA",
							"type": "integer"
						}
					]
				},
				"CompanyInternalId": {
					"description": "InternalId da empresa",
					"type": "string",
					"example": 1,
