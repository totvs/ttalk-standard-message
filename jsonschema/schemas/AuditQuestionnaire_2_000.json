{
  "$schema": "https://raw.githubusercontent.com/totvs/ttalk-standard-message/AuditQuestionnaire/V1/1_000/jsonschema/schemas/AuditQuestionnaire_2_000.json",
  "info": {
    "description": "Contrato de Mensagem Padronizada para Questionário de Auditorias para os produtos TOTVS",
    "version": "2.000",
    "title": "Audit Questionnaire",
    "contact": {
      "name": "T-Talk",
      "url": "API.Totvs.com.br",
      "email": "comiteintegracao@totvs.com.br"
    },
    "x-totvs": {
      "messageDocumentation": {
        "name": "AuditQuestionnaire",
        "description": "Questionário de Auditoria",
        "segment": "Manufatura"
      },
      "productInformation": [
        {
          "product": "protheus",
          "contact": "SUPPLY.ML.MAN.ESTOQUE@totvs.com.br",
          "description": "Contrato de Mensagem Padronizada para Questionário de Auditoria",
          "adapter": "",
          "helpUrl": ""
        }
      ]
    }
  },
  "definitions": {
    "PagedAuditQuest": {
      "type": "object",
      "allOf": [
        {
          "$ref": "https://raw.githubusercontent.com/totvs/ttalk-standard-message/master/jsonschema/apis/types/totvsApiTypesBase.json#/definitions/Paging"
        },
        {
          "type": "object",
          "properties": {
            "items": {
              "type": "array",
              "items": {
                "$ref": "#/definitions/auditQuestType"
              }
            }
          }
        }
      ]
    },
    "auditQuestType": {
      "type": "object",
      "properties": {
        "companyId": {
          "type": "string",
          "description": "Empresa",
          "maxLength": 3
        },
        "branchId": {
          "description": "Filial",
          "type": "string",
          "example": "D MG 01",
          "x-totvs": [
            {
              "product": "PROTHEUS",
              "field": "QU4.QU4_FILIAL",
              "required": true,
              "type": "varchar",
              "available": true,
              "canUpdate": false
            }
          ]
        },
        "checklist": {
          "description": "Número do Checklist",
          "type": "string",
          "maximum": 6,
          "x-totvs": [
            {
              "product": "PROTHEUS",
              "field": "QU4.QU4_CHKLST",
              "required": true,
              "type": "char",
              "length": 6,
              "available": true,
              "canUpdate": false
            }
          ]
        },
        "revision": {
          "description": "Revisão do Checklist",
          "type": "string",
          "maxLength": 2,
          "x-totvs": [
            {
              "product": "PROTHEUS",
              "field": "QU4.QU4_REVIS",
              "required": true,
              "type": "char",
              "length": 2,
              "available": true,
              "canUpdate": false
            }
          ]
        },
        "topic": {
          "description": "Topico do Checklist",
          "type": "string",
          "maxLength": 10,
          "x-totvs": [
            {
              "product": "PROTHEUS",
              "field": "QU4.QU4_CHKITE",
              "required": true,
              "type": "char",
              "length": 10,
              "available": true,
              "canUpdate": false
            }
          ]
        },
        "questionNumber": {
          "description": "Número da Questão",
          "type": "string",
          "maxLength": 3,
          "x-totvs": [
            {
              "product": "PROTHEUS",
              "field": "QU4.QU4_QSTITE",
              "required": true,
              "type": "char",
              "length": 3,
              "available": true,
              "canUpdate": false
            }
          ]
        },
        "questionText": {
          "description": "Texto da Questão",
          "type": "string",
          "x-totvs": [
            {
              "product": "PROTHEUS",
              "field": "QU4.QU4_TXTQS1",
              "required": true,
              "type": "memo",
              "available": true,
              "canUpdate": true
            }
          ]
        },
        "observations": {
          "description": "Observações",
          "type": "string",
          "x-totvs": [
            {
              "product": "PROTHEUS",
              "field": "QU4.QU4_OBSER1",
              "required": false,
              "type": "memo",
              "available": true,
              "canUpdate": true
            }
          ]
        },
        "requirements": {
          "description": "Requisitos",
          "type": "string",
          "x-totvs": [
            {
              "product": "PROTHEUS",
              "field": "QU4.QU4_REQQS1",
              "required": false,
              "type": "memo",
              "available": true,
              "canUpdate": true
            }
          ]
        },
        "initRange": {
          "description": "Faixa Inicial",
          "type": "number",
          "format": "double",
          "x-totvs": [
            {
              "product": "PROTHEUS",
              "field": "QU4.QU4_FAIXIN",
              "required": false,
              "type": "decimal",
              "length\"": "6,2",
              "note": "Contém a máscara numérica @E 999.99",
              "available": true,
              "canUpdate": true
            }
          ]
        },
        "finRange": {
          "description": "Faixa Final",
          "type": "number",
          "format": "double",
          "x-totvs": [
            {
              "product": "PROTHEUS",
              "field": "QU4.QU4_FAIXFI",
              "required": false,
              "type": "decimal",
              "length\"": "6,2",
              "note": "Contém a máscara numérica @E 999.99",
              "available": true,
              "canUpdate": true
            }
          ]
        },
        "lastRevis": {
          "description": "Ultima Revisão",
          "type": "string",
          "format": "date",
          "x-totvs": [
            {
              "product": "PROTHEUS",
              "field": "QU4.QU4_ULTREV",
              "required": false,
              "type": "date",
              "length": 8,
              "available": true,
              "canUpdate": true
            }
          ]
        },
        "questionValue": {
          "description": "Peso da Questão",
          "type": "number",
          "format": "double",
          "x-totvs": [
            {
              "product": "PROTHEUS",
              "field": "QU4.QU4_PESO",
              "required": false,
              "type": "decimal",
              "length\"": "6,2",
              "note": "Contém a máscara numérica @E 999.99",
              "available": true,
              "canUpdate": true
            }
          ]
        },
        "useOption": {
          "description": "Usa Alternativa (1-Sim/2-Não)",
          "type": "string",
          "enum": [
            "1",
            "2"
          ],
          "x-totvs": [
            {
              "product": "PROTHEUS",
              "field": "QU4.QU4_USAALT",
              "required": true,
              "type": "char",
              "length": 1,
              "available": true,
              "canUpdate": true
            }
          ]
        },
        "internalId": {
          "description": "Chave do Questionário de Auditoria",
          "type": "string",
          "x-totvs": [
            {
              "product": "PROTHEUS",
              "field": "QU4_FILIAL|QU4_CHKLST|QU4_REVIS|QU4_CHKITE|QU4_QSTITE",
              "required": false,
              "type": "char",
              "available": true,
              "canUpdate": false
            }
          ]
        }
      }
    }
  }
}