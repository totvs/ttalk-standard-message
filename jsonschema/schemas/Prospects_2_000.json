					"x-totvs": [
						{
							"product": "protheus",
							"field": "SUS.US_PESSOA",
							"required": false,
							"type": "varchar",
							"length": "1",
							"available": true,
							"canUpdate": true,
							"note": "1=Física;2=Jurídica"
