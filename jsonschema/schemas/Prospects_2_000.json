					"x-totvs": [
						{
							"product": "protheus",
							"field": "SUS.US_ORIGEM",
							"required": false,
							"type": "varchar",
							"length": "1",
							"available": true,
							"canUpdate": true,
							"note": "1=Mailing; 2=Campanha; 3=Web; 4=Indicação; 5=Evento; 6=Anúncio; 7=Parceiro; 8=Relações públicas; 9=Seminário; 10=Boca-a-boca; 11=Outros"
