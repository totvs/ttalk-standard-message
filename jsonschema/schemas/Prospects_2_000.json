								{
									"product": "datasul",
									"available": false
								}
							]
						},
						"marketSegmentDescription": {
							"type": "string",
							"description": "Descrição do segmento",
							"x-totvs": [
								{
									"product": "protheus",
									"field": "SUS.US_DESSEG",
									"required": false,
									"type": "varchar",
									"length": "40",
									"note": "Segmento Principal",
									"available": true
								},
								{
									"product": "datasul",
									"available": false
								}
							]
						},
						"PublicSector": {
							"type": "string",
							"description": "Informa se a empresa é do setor Público",
							"enum": [
								"1",
								"2"
							],
							"x-totvs": [
								{
									"product": "protheus",
									"field": "SUS.US_SETPUBL",
									"required": false,
									"type": "varchar",
									"length": "1",
									"available": true
								},
								{
									"product": "datasul",
									"available": false
								}
							]
						}
					}
				},
				"RegisterSituation": {
					"type": "string",
					"example": "1",
					"description": "Status: 1 - Ativo, 2 - Inativo",
					"enum": [
						"1",
						"2"
					],
					"x-totvs": [
						{
							"product": "protheus",
							"field": "SUS.US_MSBLQL",
							"required": false,
							"type": "varchar",
							"canUpdate": true,
							"note": "No Protheus Status active/inactive serão convertidos em: active='2',inactive='1'",
							"available": true
						},
						{
							"product": "datasul",
							"available": false
						}
					]
				},
				"ProspectSituation": {
					"type": "string",
					"example": "1",
					"maxLength": 9,
					"description": "Status Atual.",
					"x-totvs": [
						{
							"product": "protheus",
							"field": "SUS.US_STATUS",
							"required": false,
							"length": 1,
							"type": "varchar",
							"available": true,
							"canUpdate": true,
							"note": " O Protheus só trabalha com as seguintes opções: 1=Classificado; 2=Desenvolvimento; 3=Gerente; 4=StandBy; 5=Desqualificado; 6=Cliente. Qualquer opção diferente será desconsiderada."
						},
						{
							"product": "datasul",
							"field": "crm_pessoa.num_id_classif",
							"required": false,
							"type": "integer",
							"length": "9",
							"available": true,
							"canUpdate": true
						}
					]
