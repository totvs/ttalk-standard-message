					"x-totvs": [
						{
							"product": "protheus",
							"field": "SUS.US_TIPO",
							"required": false,
							"type": "varchar",
							"length": "1",
							"available": true,
							"canUpdate": true,
							"note": "1=Cons.Final;2=Produtor Rural;3=Revendedor;4=Solidario;5=Exportacao"
