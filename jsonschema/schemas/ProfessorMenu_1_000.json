{
	"$schema": "https://raw.githubusercontent.com/totvs/ttalk-standard-message/education/jsonschema/schemas/ProfessorMenu_1_000.json#",
	"info": {
		"description": "Contrato de Mensagem Padronizada para listagem de menus do professor dos produtos TOTVS",
		"version": "1.000",
		"title": "ProfessorMenu",
		"contact": {
			"name": "T-Talk",
			"url": "API.Totvs.com.br",
			"email": "comiteintegracao@totvs.com.br"
		},
		"x-totvs": {
			"messageDocumentation": {
				"name": "ProfessorMenu",
				"description": "Dados do menu do professor do educacional",
				"segment": "Educacional"
			},
			"productInformation": [
				{
					"product": "RM",
					"contact": "TribeEdu.Squad.EduPortalMobile@totvs.com.br",
					"description": "Menus do professor do Educacional"
				}
			]
		}
	},
	"definitions": {
		"PagedProfessorMenu": {
			"type": "array",
			"items": {
				"$ref": "#/definitions/ProfessorMenu"
			}
		},
		"ProfessorMenu": {
			"description": "Descreve um menu do professor do Educacional",
			"properties": {
				"ParentId": {
					"description": "Identificador do menu pai",
					"type": "string",
					"example": "EDU_PORTAL",
					"x-totvs": [
						{
							"product": "RM",
							"available": true,
							"required": false,
							"note": "Identificador do menu pai",
							"field": "GMENUHTML.PARENTID",
							"type": "string"
						}
					]
				},
				"SistemCode": {
					"description": "Código do Sistema",
					"type": "string",
					"example": "S",
					"x-totvs": [
						{
							"product": "RM",
							"note": "Código do Sistema",
							"field": "CODSISTEMA",
							"type": "string",
							"required": false,
							"available": true
						}
					]
				},
				"Caption": {
					"description": "Descrição do menu",
					"type": "string",
					"example": "Aula do dia",
					"x-totvs": [
						{
							"product": "RM",
							"available": true,
							"required": false,
							"note": "Descrição do menu",
							"field": "GMENUHTML.CAPTION",
							"type": "string"
						}
					]
				},
				"Path": {
					"description": "Caminho virtual do menu",
					"type": "string",
					"example": "#/RM/Edu-Educacional/diariodeclasse",
					"x-totvs": [
						{
							"product": "RM",
							"field": "GMENUHTML.PATH",
							"required": false,
							"available": true
						}
					]
				},
				"SecurityId": {
					"type": "string",
					"example": "808100395",
					"description": "Identificador de segurança da rotina",
					"x-totvs": [
						{
							"product": "RM",
							"field": "GMENUHTML.SECURITYID",
							"required": false,
                            "Type": "string",
                            "note": "Similar a tag de segurança da action",
							"available": true
						}
					]
				},
				"ExecutionId": {
					"type": "string",
                    "description": "Identificador de execução, utilizado nos casos de policy.",
                    "example": "8",
					"x-totvs": [
						{
							"product": "RM",
                            "note": "Por exemplo: permissões para incluir/excluir/editar em uma rotina",
                            "field": "GMENUHTML.EXECUTIONID",
							"required": false,
							"available": true
						}
					]
				},
				"IsPolicy": {
					"type": "string",
					"example": "0",
					"description": "Indica se o item de menu é uma policy",
					"x-totvs": [
						{
							"product": "RM",
                            "field": "GMENUHTML.ISPOLICY",
                            "note": "Se for 0 ou NULL é um item de menu, caso seja 1 é uma policy",
							"required": false,
							"Type": "string",
							"available": true
						}
					]
				},
				"InternalId": {
					"type": "string",
					"description": "InternalId do menu do professor",
					"x-totvs": [
						{
							"product": "RM",
							"note": "GMENUHTML.ID",
							"required": true,
							"available": true
						}
					]
				},
				"Order": {
					"type": "integer",
					"example": "1",
					"description": "Ordem do menu do professor",
					"x-totvs": [
						{
							"product": "RM",
							"field": "GMENUHTML.ORDER",
							"note": "Ordem do menu do professor",
							"required": false,
							"Type": "integer",
							"available": true
						}
					]
				},
				"PortalId": {
					"type": "integer",
					"example": "1",
					"description": "Identificador do Portal",
					"x-totvs": [
						{
							"product": "RM",
							"field": "GMENUHTML.IDPORTAL",
							"note": "Identificador do Portal",
							"required": false,
							"Type": "integer",
							"available": true
						}
					]
				},
				"Items": {
					"type": "array",
					"items": {
						"$ref": "#/definitions/ProfessorMenu"
					}
				}
			}
		}
	}
}