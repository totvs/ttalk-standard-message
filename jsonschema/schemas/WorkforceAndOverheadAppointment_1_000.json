{
  "$schema": "https://raw.githubusercontent.com/totvs/ttalk-standard-message/master/jsonschema/schemas/WorkforceAndOverheadAppointment_1_000.json#",
  "info": {
    "description": "",
    "version": "1.000",
    "title": "WorkforceAndOverheadAppointment",
    "contact": {},
    "x-totvs": {
      "messageDocumentation": {
        "name": "Apontamento MOB/GGF",
        "description": "Realizar apontamento de MOB e GGF por operação",
        "segment": "Manufatura"
      },
      "productInformation": [
        {
          "product": "Datasul",
          "contact": "André Luís Oliveira Pereira",
          "note": "",
          "adapter": "WorkforceAndOverheadAppointmentAdapter"
        }
      ],
      "transactionDefinition": {
        "subType": "event",
        "businessContentType": {
          "$ref": "#/definitions/BusinessContentType",
          "type": "object"
        },
        "returnContentType": {
          "$ref": "#/definitions/ReturnContentType",
          "type": "object"
        }
      }
    }
  },
  "definitions": {
    "BusinessContentType": {
      "type": "object",
      "properties": {
        "ListOfProductionOrders": {
          "type": "array",
          "multipleOf": 0.0001,
          "minItems": 1,
          "items": {
            "type": "object",
            "properties": {
              "ProductionOrderNumber": {
                "description": "Número Ordem Produção",
                "type": "string",
                "maxLength": 20
              },
              "ListOfManufactureAppointments": {
                "description": "Sequencia do reporte de horas",
                "type": "array",
                "minimum": -999999999999,
                "maximum": 999999999999,
                "minItems": 1,
                "items": {
                  "type": "object",
                  "properties": {
                    "InternalId": {
                      "type": "string"
                    },
                    "AppointmentType": {
                      "type": "integer",
                      "format": "int32"
                    },
                    "DocumentCode": {
                      "type": "string"
                    },
                    "DocumentSerie": {
                      "type": "string"
                    },
                    "ItemCode": {
                      "type": "string"
                    },
                    "ActivityID": {
                      "type": "string",
                      "maxLength": 10
                    },
                    "ActivityCode": {
                      "type": "string"
                    },
                    "RoutingCode": {
                      "type": "string"
                    },
                    "WorkCenterCode": {
                      "type": "string"
                    },
                    "AppointmentDate": {
                      "type": "string",
                      "format": "date"
                    },
                    "AmountAppointed": {
                      "type": "number"
                    },
                    "OperatorID": {
                      "type": "string",
                      "maxLength": 10
                    },
                    "LaborCode": {
                      "type": "string"
                    },
                    "ReversedReport": {
                      "type": "boolean"
                    },
                    "IntegrationReport": {
                      "type": "string",
                      "maxLength": 10
                    }
                  },
                  "required": [
                    "InternalId",
                    "AppointmentDate"
                  ]
                }
              }
            },
            "required": [
              "ListOfManufactureAppointments"
            ]
          }
        }
      },
      "required": [
        "ListOfProductionOrders"
      ]
    },
    "ReturnContentType": {
      "type": "object",
      "properties": {
        "ListOfInternalId": {
          "type": "array",
          "items": {
            "$ref": "https://raw.githubusercontent.com/totvs/ttalk-standard-message/master/jsonschema/schemas/types/ListOfInternalId_1_000.json#/definitions/ListOfInternalIdType",
            "type": "object"
          }
        }
      }
    },
    "ProductionOrderNumberType": {
      "type": "string"
    }
  }
}