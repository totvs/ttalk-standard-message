{
	"$schema": "https://raw.githubusercontent.com/totvs/ttalk-standard-message/ProfessorTerm/v1/1_000/jsonschema/schemas/ProfessorTerm_1_000.json#",
	"info": {
		"description": "Contrato de Mensagem Padronizada para listagem dos períodos letivos do professor dos produtos TOTVS",
		"version": "1.000",
		"title": "ProfessorTerm",
		"contact": {
			"name": "T-Talk",
			"url": "API.Totvs.com.br",
			"email": "comiteintegracao@totvs.com.br"
		},
		"x-totvs": {
			"messageDocumentation": {
				"name": "ProfessorTerm",
				"description": "Dados do período letivo do professor do educacional",
				"segment": "Educacional"
			},
			"productInformation": [
				{
					"product": "RM",
					"contact": "TribeEdu.Squad.EduPortalMobile@totvs.com.br",
					"description": "Períodos letivos do professor do Educacional"
				}
			]
		}
	},
	"definitions": {
		"PagedProfessorTerm": {
			"type": "object",
			"allOf": [
				{
					"$ref": "https://raw.githubusercontent.com/totvs/ttalk-standard-message/master/jsonschema/apis/types/totvsApiTypesBase.json#/definitions/Paging"
				},
				{
					"type": "object",
					"properties": {
						"items": {
							"type": "array",
							"items": {
								"$ref": "#/definitions/ProfessorTerm"
							}
						}
					}
				}
			]
		},
		"ProfessorTerm": {
			"description": "Descreve um período letivo do professor do Educacional",
			"properties": {
				"CompanyCode": {
					"description": "Código da empresa",
					"type": "string",
					"example": 1,
					"x-totvs": [
						{
							"product": "RM",
							"available": true,
							"required": true,
							"note": "Código da empresa",
							"field": "CODCOLIGADA",
							"type": "integer"
						}
					]
				},
				"BranchCode": {
					"description": "Código da filial",
					"type": "string",
					"example": 1,
					"x-totvs": [
						{
							"product": "RM",
							"available": true,
							"required": true,
							"note": "Código da filial",
							"field": "CODFILIAL",
							"type": "integer"
						}
					]
				},
				"InternalId": {
					"type": "string",
					"description": "InternalId ProfessorTerm",
					"x-totvs": [
						{
							"product": "RM",
							"note": "SPLETIVO.CODCOLIGADA|SPLETIVO.IDPERLET",
							"required": false,
							"available": true
						}
					]
				},
				"LevelEducationalCode": {
					"type": "string",
					"example": "1",
					"description": "Código do nível de ensino",
					"x-totvs": [
						{
							"product": "RM",
							"field": "STIPOCURSO.CODTIPOCURSO",
							"required": false,
							"Type": "integer",
							"length": 2,
							"available": true
						}
					]
				},
				"TermCodeDescription": {
					"type": "string",
					"example": "2011/1",
					"description": "Código do Perído Letivo",
					"x-totvs": [
						{
							"product": "RM",
							"field": "SPLETIVO.CODPERLET",
							"required": false,
							"Type": "string",
							"length": 10,
							"available": true
						}
					]
				},
				"TermCode": {
					"type": "string",
					"example": "1",
					"description": "ID do Perído Letivo",
					"x-totvs": [
						{
							"product": "RM",
							"field": "SPLETIVO.IDPERLET",
							"required": false,
							"Type": "integer",
							"length": 4,
							"available": true
						}
					]
				},
				"TermDescription": {
					"type": "string",
					"example": "1º Semestre de 20011",
					"description": "Nome do Perído Letivo",
					"x-totvs": [
						{
							"product": "RM",
							"field": "SPLETIVO.DESCRICAO",
							"required": false,
							"Type": "string",
							"length": 60,
							"available": true
						}
					]
				}
			}
		}
	}
}