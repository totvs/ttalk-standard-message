					"x-totvs": [
						{
							"field": "DUE_REGSOL",
							"type": "string",
							"available": true,
							"length": "30",
							"canupdate": false,
							"product": "protheus",
							"required": false
						}
					],
					"type": "string",
					"example": "string",
					"description": "Descrição Grupo de Região"
				},
