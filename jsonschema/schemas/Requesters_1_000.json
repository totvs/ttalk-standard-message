                                            "field": "DUE_COLDOM",
                                            "note": "1=Yes, 2=No                                                                                                                      ,",
                                            "type": "string",
                                            "length": "1",
                                            "available": true,
                                            "canupdate": true,
                                            "product": "protheus",
                                            "required": false
                                        }
                                    ],
                                    "type": "string",
                                    "example": "2",
                                    "enum": [
                                        "1",
                                        "2"
                                    ],
                                    "description": "Pickup on Sunday"
                                }
