                "communicationInfo": {
                    "$ref": "https://raw.githubusercontent.com/totvs/ttalk-standard-message/master/jsonschema/schemas/types/CommunicationInformation_1_002.json#/definitions/CommunicationInformationType",
                    "type": "object"
                },
                "regionGroupCode": {
                    "x-totvs": [
                        {
                            "field": "DUE_CDRSOL",
                            "type": "string",
                            "available": true,
                            "length": "6",
                            "canupdate": true,
                            "product": "protheus",
                            "required": false
                        }
                    ],
                    "type": "string",
                    "example": "Q50308",
                    "description": "Requester Area Code"
                },
                "regionGroup": {
                    "x-totvs": [
                        {
                            "field": "DUE_REGSOL",
                            "type": "string",
                            "available": true,
                            "length": "30",
                            "canupdate": false,
                            "product": "protheus",
                            "required": false
                        }
                    ],
                    "type": "string",
                    "example": "string",
                    "description": "Requester Area"
                },
                "pickupInfo": {
                    "description": "PickUp Info",
                    "type": "object",
                    "properties": {
                        "TransportTypeCode": {
                            "x-totvs": [
                                {
                                    "field": "DUE_TIPTRA",
                                    "type": "string",
                                    "available": true,
                                    "length": "1",
                                    "canupdate": true,
                                    "product": "protheus",
                                    "required": true
                                }
                            ],
                            "type": "string",
                            "example": "1",
                            "description": "Transport Type"
                        },
                        "TransportType": {
                            "x-totvs": [
                                {
                                    "field": "DUE_DESTPT",
                                    "type": "string",
                                    "available": true,
                                    "length": "30",
                                    "canupdate": false,
                                    "product": "protheus",
                                    "required": false
                                }
                            ],
                            "type": "string",
                            "example": "string",
                            "description": "Transport Type Descriptn."
                        },
                        "PickupType": {
                            "x-totvs": [
                                {
                                    "field": "DUE_TIPCOL",
                                    "note": "1=Entered, 2=Automatic                                                                                                           ,",
                                    "type": "string",
                                    "length": "1",
                                    "available": true,
                                    "canupdate": true,
                                    "product": "protheus",
                                    "required": false
                                }
                            ],
                            "type": "string",
                            "example": "1",
                            "enum": [
                                "1",
                                "2"
                            ],
                            "description": "Type of Pickup"
                        },
                        "PickupStart": {
                            "x-totvs": [
                                {
                                    "field": "DUE_HORCOI",
                                    "type": "string",
                                    "available": true,
                                    "length": "4",
                                    "canupdate": true,
                                    "product": "protheus",
                                    "required": false
                                }
                            ],
                            "type": "string",
                            "example": "string",
                            "description": "Time Pickup Begins"
                        },
                        "PickupEnds": {
                            "x-totvs": [
                                {
                                    "field": "DUE_HORCOF",
                                    "type": "string",
                                    "available": true,
                                    "length": "4",
                                    "canupdate": true,
                                    "product": "protheus",
                                    "required": false
                                }
                            ],
                            "type": "string",
                            "example": "string",
                            "description": "Time Pickup Ends"
                        },
                        "pickupDays": {
                            "description": "Requester Area Code",
                            "type": "object",
                            "properties": {
                                "Monday": {
                                    "x-totvs": [
                                        {
                                            "field": "DUE_COLSEG",
                                            "note": "1=Yes, 2=No                                                                                                                      ,",
                                            "type": "string",
                                            "length": "1",
                                            "available": true,
                                            "canupdate": true,
                                            "product": "protheus",
                                            "required": false
                                        }
                                    ],
                                    "type": "string",
                                    "example": "2",
                                    "enum": [
                                        "1",
                                        "2"
                                    ],
                                    "description": "Pickup on Monday"
                                },
                                "Tuesday": {
                                    "x-totvs": [
                                        {
                                            "field": "DUE_COLTER",
                                            "note": "1=Yes, 2=No                                                                                                                      ,",
                                            "type": "string",
                                            "length": "1",
                                            "available": true,
                                            "canupdate": true,
                                            "product": "protheus",
                                            "required": false
                                        }
                                    ],
                                    "type": "string",
                                    "example": "2",
                                    "enum": [
                                        "1",
                                        "2"
                                    ],
                                    "description": "Pickup on Tuesday"
                                },
                                "Wednesday": {
                                    "x-totvs": [
                                        {
                                            "field": "DUE_COLQUA",
                                            "note": "1=Yes, 2=No                                                                                                                      ,",
                                            "type": "string",
                                            "length": "1",
                                            "available": true,
                                            "canupdate": true,
                                            "product": "protheus",
                                            "required": false
                                        }
                                    ],
                                    "type": "string",
                                    "example": "2",
                                    "enum": [
                                        "1",
                                        "2"
                                    ],
                                    "description": "Pickup on Wednesday"
                                },
                                "Thursday": {
                                    "x-totvs": [
                                        {
                                            "field": "DUE_COLQUI",
                                            "note": "1=Yes, 2=No                                                                                                                      ,",
                                            "type": "string",
                                            "length": "1",
                                            "available": true,
                                            "canupdate": true,
                                            "product": "protheus",
                                            "required": false
                                        }
                                    ],
                                    "type": "string",
                                    "example": "2",
                                    "enum": [
                                        "1",
                                        "2"
                                    ],
                                    "description": "Pickup on Thursday"
                                },
                                "Friday": {
                                    "x-totvs": [
                                        {
                                            "field": "DUE_COLSEX",
                                            "note": "1=Yes, 2=No                                                                                                                      ,",
                                            "type": "string",
                                            "length": "1",
                                            "available": true,
                                            "canupdate": true,
                                            "product": "protheus",
                                            "required": false
                                        }
                                    ],
                                    "type": "string",
                                    "example": "2",
                                    "enum": [
                                        "1",
                                        "2"
                                    ],
                                    "description": "Pickup on Friday"
                                },
                                "Saturday": {
                                    "x-totvs": [
                                        {
                                            "field": "DUE_COLSAB",
                                            "note": "1=Yes, 2=No                                                                                                                      ,",
                                            "type": "string",
                                            "length": "1",
                                            "available": true,
                                            "canupdate": true,
                                            "product": "protheus",
                                            "required": false
                                        }
                                    ],
                                    "type": "string",
                                    "example": "2",
                                    "enum": [
                                        "1",
                                        "2"
                                    ],
                                    "description": "Pickup on Saturday"
                                },
                                "Sunday": {
                                    "x-totvs": [
