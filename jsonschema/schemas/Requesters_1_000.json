									"length": "4",
									"canupdate": true,
									"product": "protheus",
									"required": false
								}
							],
							"type": "string",
							"example": "string",
							"description": "Horário de Fim de Coleta"
						},
						"pickupDays": {
							"description": "Requester Area Code",
							"type": "object",
							"properties": {
								"Monday": {
									"x-totvs": [
										{
											"field": "DUE_COLSEG",
											"note": "1=Sim, 2=Não",
											"type": "string",
											"length": "1",
											"available": true,
											"canupdate": true,
											"product": "protheus",
											"required": false
										}
									],
									"type": "boolean",
									"example": false,
									"description": "Coleta segunda-feira?"
								},
								"Tuesday": {
									"x-totvs": [
										{
											"field": "DUE_COLTER",
											"note": "1=Sim, 2=Não",
											"type": "string",
											"length": "1",
											"available": true,
											"canupdate": true,
											"product": "protheus",
											"required": false
										}
									],
									"type": "boolean",
									"example": false,
									"description": "Coleta terça-feira?"
								},
								"Wednesday": {
									"x-totvs": [
										{
											"field": "DUE_COLQUA",
											"note": "1=Sim, 2=Não",
											"type": "string",
											"length": "1",
											"available": true,
											"canupdate": true,
											"product": "protheus",
											"required": false
										}
									],
									"type": "boolean",
									"example": false,
									"description": "Coleta quarta-feira?"
								},
								"Thursday": {
									"x-totvs": [
										{
											"field": "DUE_COLQUI",
											"note": "1=Sim, 2=Não",
											"type": "string",
											"length": "1",
											"available": true,
											"canupdate": true,
											"product": "protheus",
											"required": false
										}
									],
									"type": "boolean",
									"example": false,
									"description": "Coleta quinta-feira?"
								},
								"Friday": {
									"x-totvs": [
										{
											"field": "DUE_COLSEX",
											"note": "1=Sim, 2=Não",
											"type": "string",
											"length": "1",
											"available": true,
											"canupdate": true,
											"product": "protheus",
											"required": false
										}
									],
									"type": "boolean",
									"example": false,
									"description": "Coleta sexta-feira?"
								},
								"Saturday": {
									"x-totvs": [
										{
											"field": "DUE_COLSAB",
											"note": "1=Sim, 2=Não",
											"type": "string",
											"length": "1",
											"available": true,
											"canupdate": true,
											"product": "protheus",
											"required": false
										}
									],
									"type": "boolean",
									"example": false,
									"description": "Coleta sábado?"
								},
								"Sunday": {
									"x-totvs": [
										{
											"field": "DUE_COLDOM",
											"note": "1=Sim, 2=Não",
											"type": "string",
											"length": "1",
											"available": true,
											"canupdate": true,
											"product": "protheus",
											"required": false
										}
									],
									"type": "boolean",
									"example": false,
									"description": "Coleta Domingo?"
								}
							}
						}
					}
				},
				"Products": {
					"type": "array",
					"items": {
						"$ref": "#/definitions/RequestersItemsInfo"
					}
				}
			}
		},
