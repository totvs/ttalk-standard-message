					"properties": {
						"TransportTypeCode": {
							"x-totvs": [
								{
									"field": "DUE_TIPTRA",
									"type": "string",
									"available": true,
									"length": "1",
									"canupdate": true,
									"product": "protheus",
									"required": true
								}
							],
							"type": "string",
							"example": "1",
							"description": "Código de Tipo de Transporte"
						},
						"TransportType": {
							"x-totvs": [
								{
									"field": "DUE_DESTPT",
									"type": "string",
									"available": true,
									"length": "30",
									"canupdate": false,
									"product": "protheus",
									"required": false
								}
							],
							"type": "string",
							"example": "string",
							"description": "Descrição de Tipo de Transporte"
						},
						"PickupType": {
							"x-totvs": [
								{
									"field": "DUE_TIPCOL",
									"note": "1=Entered, 2=Automatic                                                                                                           ,",
									"type": "string",
									"length": "1",
									"available": true,
									"canupdate": true,
									"product": "protheus",
									"required": false
								}
							],
							"type": "string",
							"example": "1",
							"enum": [
								"1",
								"2"
							],
							"description": "Tipo de Coleta "
						},
						"PickupStart": {
							"x-totvs": [
								{
									"field": "DUE_HORCOI",
									"type": "string",
									"available": true,
									"length": "4",
									"canupdate": true,
									"product": "protheus",
									"required": false
								}
							],
							"type": "string",
							"example": "string",
