{
	"$schema": "mastermaster/schemas/Requesters_1_000.json",
	"info": {
		"description": "API para a entidade Solicitante (Requesters) para produtos TOTVS",
		"version": "1.000",
		"title": "Requesters",
		"contact": {
			"name": "T-Talk",
			"url": "api.Totvs.com.br",
			"email": "comiteintegracao@totvs.com.br"
		},
		"x-totvs": {
			"transactionDefinition": {
				"subType": "event",
