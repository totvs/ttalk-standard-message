{
    "$schema": "https://raw.githubusercontent.com/izacsc/ttalk-standard-message/Requesters/V1/1_000/jsonschema/schemas/Requesters_1_000.json",
    "info": {
        "description": "API para a entidade Solicitante (Requesters) para produtos TOTVS",
        "version": "1.000",
        "title": "Requesters",
        "contact": {
            "name": "T-Talk",
            "url": "api.Totvs.com.br",
            "email": "comiteintegracao@totvs.com.br"
        },
        "x-totvs": {
            "transactionDefinition": {
                "subType": "event",
                "businessContentType": {
                    "type": "object",
                    "$ref": "#/definitions/RequestersInfo"
                },
                "returnContentType": {
                    "type": "object",
                    "$ref": "https://raw.githubusercontent.com/totvs/ttalk-standard-message/master/jsonschema/schemas/ListOfInternalId_1_000.json#/definitions/ReturnContentWithModelType"
                }
            },
            "messageDocumentation": {
                "name": "Requesters",
                "description": "Cadastro de Solicitante",
                "segment": "TMS"
            },
            "productInformation": [
                {
                    "product": "protheus",
                    "contact": "supply.ml.log.tms-operacional@totvs.com.br",
                    "description": "Cadastro de Solicitante",
                    "adapter": "TMSA440I.prw"
                }
            ]
        }
    },
    "definitions": {
        "PagedRequesters": {
            "type": "object",
            "allOf": [
                {
                    "$ref": "https://raw.githubusercontent.com/totvs/ttalk-standard-message/master/jsonschema/apis/types/totvsApiTypesBase.json#/definitions/Paging"
                },
                {
                    "$ref": "#/definitions/Requesters"
                }
            ]
        },
        "Requesters": {
            "type": "object",
            "properties": {
                "items": {
                    "type": "array",
                    "items": {
                        "$ref": "#/definitions/RequestersInfo"
                    }
                }
            }
        },
        "RequestersItems": {
            "type": "array",
            "items": {
                "$ref": "#/definitions/RequestersItemsInfo"
            }
        },
        "RequestersInfoPost": {
            "allOf": [
                {
                    "$ref": "#/RequestersInfo"
                },
                {
                    "$ref": "#/RequestersItemsInfo"
                }
            ]
        }
    },
    "RequestersInfo": {
        "type": "object",
        "description": "Requesters",
        "properties": {
            "CompanyId": {
                "type": "string",
                "example": "T1",
                "description": "Código da empresa",
                "maxLength": 3
            },
            "BranchId": {
                "type": "string",
                "description": "Código da Filial",
                "example": "D MG 01 ",
                "maxLength": 12,
                "x-totvs": [
                    {
                        "product": "protheus",
                        "field": "DUE.DUE_FILIAL",
                        "required": true,
                        "length": "8",
                        "available": true,
                        "canUpdate": false
                    }
                ]
            },
            "CompanyInternalId": {
                "type": "string",
                "description": "Código da Empresa",
                "x-totvs": [
                    {
                        "product": "protheus",
                        "field": "EMPRESA|DUE.DUE_FILIAL",
                        "required": true,
                        "type": "varchar",
                        "length": "16",
                        "available": true,
                        "canUpdate": false
                    }
                ]
            },
            "InternalId": {
                "type": "string",
                "description": "InternalId do Solicitante no ERP",
                "x-totvs": [
                    {
                        "product": "protheus",
                        "field": "DUE.DUE_FILIAL|DUE.DUE_CODSOL",
                        "type": "varchar",
                        "length": "14",
                        "required": true,
                        "available": true,
                        "note": "O internalID do Solicitante é formado por DUE.DUE_FILIAL|DUE.DUE_CODSOL"
                    }
                ]
            },
            "code": {
                "x-totvs": [
                    {
                        "field": "DUE_CODSOL",
                        "type": "string",
                        "available": true,
                        "length": "9",
                        "canupdate": true,
                        "product": "protheus",
                        "required": false
                    }
                ],
                "type": "string",
                "example": "000000099",
                "description": "Código do Solicitante"
            },
            "requesterCustomer": {
                "type": "object",
                "properties": {
                    "Code": {
                        "$ref": "https://raw.githubusercontent.com/totvs/ttalk-standard-message/master/jsonschema/schemas/CustomerVendor_2_007.json#/definitions/BusinessContentType/properties/Code"
                    },
                    "StoreId": {
                        "$ref": "https://raw.githubusercontent.com/totvs/ttalk-standard-message/master/jsonschema/schemas/CustomerVendor_2_007.json#/definitions/BusinessContentType/properties/StoreId"
                    }
                }
            },
            "name": {
                "x-totvs": [
                    {
                        "field": "DUE_NOME",
