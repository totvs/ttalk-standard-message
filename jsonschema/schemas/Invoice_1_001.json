{
  "$schema": "https://raw.githubusercontent.com/totvs/ttalk-standard-message/master/jsonschema/schemas/Invoice_1_001.json#",
  "info": {
    "description": "",
    "version": "1.001",
    "title": "Invoice",
    "contact": {},
    "x-totvs": {
      "messageDocumentation": {
        "name": "Invoice",
        "description": "Baixa de estoque",
        "segment": ""
      },
      "transactionDefinition": {
        "subType": "event",
        "businessContentType": {
          "$ref": "#/definitions/BusinessContentType",
          "type": "object"
        },
        "returnContentType": {
          "$ref": "#/definitions/ReturnContentType",
          "type": "object"
        }
      }
    }
  },
  "definitions": {
    "BusinessContentType": {
      "type": "object",
      "properties": {
        "CompanyId": {
          "description": "Empresa",
          "type": "string",
          "maxLength": 2
        },
        "BranchId": {
          "description": "Filial",
          "type": "string",
          "maxLength": 2
        },
        "InvoiceNumber": {
          "description": "Número da Nota Fiscal (LOGIX: 10, DataSul: 16, Protheus: 20)",
          "type": "string",
          "maxLength": 20
        },
        "InvoiceSerie": {
          "description": "Serie da Nota Fiscal",
          "type": "string",
          "maxLength": 3
        },
        "InvoiceSubSerie": {
          "description": "Sub-Serie da Nota Fiscal",
          "type": "string",
          "maxLength": 5
        },
        "InvoiceModel": {
          "description": "Modelo da Nota Fiscal (Convênio ICMS 31/99)",
          "type": "string",
          "maxLength": 2
        },
        "InvoiceSituation": {
          "description": "Situação da Nota Fiscal (1-Normal 2-Cancelada) / 1 - 1-Normal / 2 - 2-Cancelada",
          "type": "string",
          "enum": [
            "1",
            "2"
          ]
        },
        "IssueDate": {
          "description": "Data de Emissão",
          "type": "string",
          "format": "date"
        },
        "InvoiceAmount": {
          "description": "Valor da Nota Fiscal",
          "type": "number",
          "format": "double"
        },
        "ValueofGoods": {
          "description": "Valor da Mercadoria",
          "type": "number",
          "format": "double"
        },
        "FreightAmount": {
          "description": "Valor do frete",
          "type": "number",
          "format": "double"
        },
        "InsuranceAmount": {
          "description": "Valor do Seguro",
          "type": "number",
          "format": "double"
        },
        "DiscountAmount": {
          "description": "Valor do Desconto",
          "type": "number",
          "format": "double"
        },
        "CurrencyRate": {
          "description": "Taxa da Moeda Utilizada para Calcular o Valor da Nota Fiscal",
          "type": "number",
          "format": "double"
        },
        "ListOfItens": {
          "description": "Itens da Nota",
          "type": "array",
          "items": {
            "$ref": "#/definitions/ListOfItensType",
            "type": "object"
          }
        }
      }
    },
    "ListOfItensType": {
      "type": "array",
      "items": {
        "$ref": "#/definitions/ItemType",
        "type": "object"
      }
    },
    "ItemType": {
      "type": "object",
      "properties": {
        "InvoiceSequence": {
          "description": "Sequência da Nota Fiscal",
          "type": "string",
          "maxLength": 4
        },
        "OrderNumber": {
          "description": "Número do Pedido que Originou o Item da Nota",
          "type": "string",
          "maxLength": 6
        },
        "OrdemItem": {
          "description": "Sequência do Item do Pedido que Gerou o Item da Nota",
          "type": "string",
          "maxLength": 4
        },
        "ItemCode": {
          "description": "Código do Item",
          "type": "string",
          "maxLength": 15
        },
        "Quantity": {
          "description": "Quantidade do Item",
          "type": "number",
          "format": "double"
        },
        "UnitofMeasureCode": {
          "description": "Código da Unidade de Medida",
          "type": "string",
          "maxLength": 6
        },
        "UnityPrice": {
          "description": "Preço Unitário do Item x Taxa da Moeda)",
          "type": "number",
          "format": "double"
        },
        "GrossValue": {
          "description": "Valor Bruto do Item. (Quantidade x Preço Unitário x Taxa da Moeda)",
          "type": "number",
          "format": "double"
        },
        "FreightValue": {
          "description": "Valor do Frete Unitário",
          "type": "number",
          "format": "double"
        },
        "InsuranceValue": {
          "description": "Valor do Seguro Unitário",
          "type": "number",
          "format": "double"
        },
        "DiscountValue": {
          "description": "Valor do Desconto Unitário",
          "type": "number",
          "format": "double"
        },
        "NetValue": {
          "description": "Valor Líquido (Valor Bruto + Frete + Seguro - Desconto)",
          "type": "number",
          "format": "double"
        },
        "AreAndLineOfBusinessCode": {
          "description": "Código da área e linha.             No Logix será uma string que poderá ter 4 (área/linha de negócio com 2 níveis)             ou 8 caracteres (área/linha de negócio com 4 níveis).             Os campos serão concatenados sem separador, respeitando a sequencia da chave da tabela linha_prod.",
          "type": "string"
        }
      }
    },
    "ReturnContentType": {
      "type": "object",
      "properties": {
        "CompanyId": {
          "description": "Empresa",
          "type": "string",
          "maxLength": 2
        },
        "BranchId": {
          "description": "Filial",
          "type": "string",
          "maxLength": 2
        },
        "InvoiceNumber": {
          "description": "Número da Nota Fiscal",
          "type": "string",
          "maxLength": 20
        },
        "InvoiceSerie": {
          "description": "Série da Nota Fiscal",
          "type": "string",
          "maxLength": 3
        },
        "InvoiceSubSerie": {
          "description": "Sub-Serie da Nota Fiscal",
          "type": "string",
          "maxLength": 5
        }
      }
    }
  }
}