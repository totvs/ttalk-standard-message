{
    "$schema": "https://raw.githubusercontent.com/totvs/ttalk-standard-message/master/jsonschema/schemas/EsocialAudit_1_000.json#",
    "info": {
        "description": "API para conferência de envio dentro/ fora do prazo do governo",
        "version": "1.000",
        "title": "Painel de Auditoria do eSocial",
        "contact": {
            "name": "T-Talk",
            "url": "api.Totvs.com.br",
            "email": "comiteintegracao@totvs.com.br"
        },
        "x-totvs": {
            "messageDocumentation": {
                "name": "Painel de Auditoria do eSocial",
                "description": "API para conferência de envio dentro/ fora do prazo do governo",
                "segment": "Seviços"
            },
            "productInformation": [
                {
                    "product": "protheus",
                    "contact": "silas.gomes@totvs.com.br",
                    "description": "API para conferência de envio dentro/ fora do prazo do governo",
                    "adapter": "WSTAF039.prw"
                }
            ]
        }
    },
    "definitions": {
        "PagedItens": {
            "type": "object",
            "allOf": [
                {
                    "$ref": "https://raw.githubusercontent.com/totvs/ttalk-standard-message/master/jsonschema/apis/types/totvsApiTypesBase.json#/definitions/Paging"
                },
                {
                    "type": "object",
                    "properties": {
                        "items": {
                            "type": "array",
                            "items": {
                                "$ref": "#/definitions/ListAudit"
                            }
                        }
                    }
                }
            ]
        },
        "ListAudit": {
            "type": "object",
            "properties": {
                "branch": {
                    "name": "branch",
                    "description": "Código da filial",
                    "example": "D MG 01",
                    "type": "string",
                    "required": true,
                    "maxLength": 10
                },
                "eventDescription": {
                    "name": "eventDescription",
                    "description": "Evento",
                    "example": "S-1200 - Folha de Pagamento",
                    "type": "string",
                    "required": true,
                    "maxLength": 20
                },
                "typeOrigin": {
                    "name": "typeOrigin",
                    "description": "Tipo",
                    "example": "Original",
                    "type": "string",
                    "required": true,
                    "maxLength": 10
                },
                "indApur": {
                    "name": "indApur",
                    "description": "Indicador de Apuração",
                    "example": "Mensal",
                    "type": "string",
                    "required": true,
                    "maxLength": 10
                },
                "periodEvent": {
                    "name": "periodEvent",
                    "description": "Data do Evento",
                    "example": "03/2021",
                    "type": "string",
                    "required": true,
                    "maxLength": 10
                },
                "cpf": {
                    "name": "cpf",
                    "description": "CPF",
                    "example": "111.111.111-11",
                    "type": "string",
                    "required": true,
                    "maxLength": 10
                },
                "registration": {
                    "name": "registration",
                    "description": "Matricula",
                    "example": "123456789-00",
                    "type": "string",
                    "required": true,
                    "maxLength": 10
                },
                "name": {
                    "name": "name",
                    "description": "Nome",
                    "example": "Fulano da Silva",
                    "type": "string",
                    "required": true,
                    "maxLength": 10
                },
                "periodTrans": {
                    "name": "periodTrans",
                    "description": "Data de Transmissão",
                    "example": "15/03/2021",
                    "type": "string",
                    "required": true,
                    "maxLength": 10
                },
                "periodLimit": {
                    "name": "periodLimit",
                    "description": "Data Limite de Transmissão",
                    "example": "20/03/2021",
                    "type": "string",
                    "required": true,
                    "maxLength": 10
                },
                "receipt": {
                    "name": "receipt",
                    "description": "Recibo",
                    "example": "1.2.23145615616",
                    "type": "string",
                    "required": true,
                    "maxLength": 20
                }
            },
            "Paging": {
                "type": "object",
                "properties": {
                    "hasNext": {
                        "type": "boolean",
                        "example": false
                    }
                }
            }
        }
    },
    "parameters": {
        "companyId": {
            "name": "companyId",
            "description": "Código da empresa e código da filial",
            "example": "T1D MG 01",
            "in": "query",
            "required": true,
            "schema": {
                "type": "string"
            }
        }
    }
}