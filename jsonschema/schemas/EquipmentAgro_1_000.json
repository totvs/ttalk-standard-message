         "url":"https://tdn.totvs.com/display/PA/PIMS+AGRO+HOME",
         "email":"adauto.matuzaki@totvs.com.br"
      },
      "x-totvs":{
         "messageDocumentation":{
            "description":"APIs T-Pesag-Server Service | Equipment",
            "name":"Equipment",
            "segment":"Agroindústria"
         },
         "productInformation":[
            {
               "product":"pims",
               "adapter":"Equipment",
               "contact":"adauto.matuzaki@totvs.com.br",
               "description":"TOTVS Agro - PIMS",
               "helpUrl":"https://tdn.totvs.com/display/PA/PIMS+AGRO+HOME"
            }
         ],
         "transactionDefinition":{
            "subType":"event",
            "businessContentType":{
               "$ref":"#/definitions/EquipmentDTO",
               "type":"object"
            },
            "returnContentType":{
               "$ref":"#/definitions/ReturnContentType",
               "type":"object"
            }
         }
      }
   },
   "definitions":{
      "requestBodies":{
         "EquipmentDTO":{
            "content":{
               "application/json":{
                  "schema":{
                     "$ref":"#/definitions/EquipmentDTO"
                  }
               }
            },
            "description":"EquipmentDTO",
            "required":true
         }
      },
      "CollectionResultResponseDTO":{
         "type":"object",
         "properties":{
            "hasNext":{
               "type":"boolean"
            },
            "items":{
               "type":"array",
               "items":{
                  "$ref":"#/definitions/DataTransferObject"
               }
            },
            "length":{
               "type":"integer",
               "format":"int32"
            }
         },
         "title":"CollectionResultResponseDTO"
      },
      "DataTransferObject":{
         "type":"object",
         "title":"DataTransferObject"
      },
      "EquipmentDTO":{
         "type":"object",
         "properties":{
            "_expandables":{
               "type":"array",
               "items":{
                  "type":"string"
               }
            },
            "availabilityFlag":{
               "type":"string",
               "example":1,
               "allowEmptyValue":false,
               "x-totvs":[
                  {
                     "product":"pims",
                     "available":true,
                     "canUpdate":true,
                     "field":"fg_disponib",
                     "length":"1",
                     "note":"Equipment availability.(Can use 1-Disponível/2-Não disponível/3-Vendido)",
                     "required":true,
                     "type":"String"
                  }
               ]
            },
            "equipmentCode":{
               "type":"string",
               "example":1234,
               "allowEmptyValue":false,
               "x-totvs":[
                  {
                     "product":"pims",
                     "available":true,
                     "canUpdate":true,
                     "field":"cd_equipamento",
                     "length":"8",
                     "note":"Equipment Code",
                     "required":true,
                     "type":"String"
                  }
               ]
            },
            "idEquipment":{
               "type":"string",
               "example":19356178954,
               "allowEmptyValue":false,
               "x-totvs":[
                  {
                     "product":"pims",
                     "available":true,
                     "canUpdate":false,
                     "field":"id_equipamento",
                     "length":"30",
                     "note":"Id.Primary Key",
                     "required":true,
                     "type":"String"
                  }
               ]
            },
            "lastUpdate":{
               "type":"string",
               "example":"2018-08-10T10:14:22-0300",
               "allowEmptyValue":false,
               "x-totvs":[
                  {
                     "product":"pims",
                     "available":true,
                     "canUpdate":true,
                     "field":"LAST_UPDATE",
                     "length":"10",
                     "note":"Last Update.",
                     "required":false,
                     "type":"Date"
                  }
               ]
            },
            "operatingVolume":{
               "type":"number",
               "example":60,
               "allowEmptyValue":false,
               "x-totvs":[
                  {
                     "product":"pims",
                     "available":true,
                     "canUpdate":true,
                     "field":"qt_volume",
                     "length":"8",
                     "note":"Operating Volume",
                     "required":false,
                     "type":"Long"
                  }
               ]
            },
            "plateNumber":{
               "type":"string",
               "example":"ABC1234",
               "allowEmptyValue":false,
               "x-totvs":[
                  {
                     "product":"pims",
                     "available":true,
                     "canUpdate":true,
                     "field":"no_placa",
                     "length":"8",
                     "note":"Plate Number",
                     "required":false,
                     "type":"String"
                  }
               ]
            },
            "resourceType":{
               "type":"string",
               "example":"CM",
               "allowEmptyValue":false,
               "x-totvs":[
                  {
                     "product":"pims",
                     "available":true,
                     "canUpdate":true,
                     "field":"cd_tp_recurso",
                     "length":"2",
                     "note":"Resource Type",
                     "required":false,
                     "type":"String"
                  }
               ]
            },
            "standardTareWeight":{
               "type":"integer",
               "format":"int64",
               "example":12250,
               "allowEmptyValue":false,
               "x-totvs":[
                  {
                     "product":"pims",
                     "available":true,
                     "canUpdate":true,
                     "field":"qt_tara_padr",
                     "length":"5",
                     "note":"Standard Tare Weight",
                     "required":false,
                     "type":"Long"
                  }
               ]
            },
            "typeEquipmentFlag":{
               "type":"string",
               "example":1,
               "allowEmptyValue":false,
               "x-totvs":[
                  {
                     "product":"pims",
                     "available":true,
                     "canUpdate":true,
                     "field":"fg_tp_equip",
                     "length":"1",
                     "note":"Equipment Type.(Can use 1-Proprio/2-Terceiros/3-Fornecedor/4-Alugado)",
                     "required":true,
                     "type":"String"
                  }
               ]
            }
         },
         "title":"EquipmentDTO"
      },
      "ReturnContentType":{
         "type":"object",
         "properties":{
            "ListOfInternalId":{
               "type":"array",
               "items":{
                  "$ref":"https://raw.githubusercontent.com/totvs/ttalk-standard-message/master/jsonschema/schemas/types/ListOfInternalId_1_000.json#/definitions/ListOfInternalIdType",
                  "type":"object"
               }
            }
         }
      }
   }
}
