{
	"$schema": "https://raw.githubusercontent.com/totvs/ttalk-standard-message/ScheduleTime/V1/1_000/jsonschema/schemas/ScheduleTime_1_000.json#",
	"info": { 
		"description": "API para a entidade de Horários de agendamento para os produtos TOTVS",
		"version": "1.000",
		"title": "Horários de Agendamento",
		"contact": {
			"name": "T-Talk",
			"url": "https://api.totvs.com.br",
			"email": "comiteintegracao@totvs.com.br"
		},
		"x-totvs": {
			"messageDocumentation": {
				"name": "ScheduleTime",
				"description": "Informações dos Horários de Agendamento",
				"segment": "Saúde"
			},
			"productInformation": [
				{
					"product": "RM",
					"contact": "TribeSau.Squad.Assistencial@totvs.com.br",
					"description": "Horários de Agendamento",
					"adapter": ""
				}
			]
		}
	},
	"definitions": {
		"PagedScheduleTime": {
			"type": "object",
			"allOf": [
				{
					"$ref": "https://raw.githubusercontent.com/totvs/ttalk-standard-message/master/jsonschema/apis/types/totvsApiTypesBase.json#/definitions/Paging"
				},
				{
					"$ref": "#/definitions/ScheduleTime"
				}
			]
		},
		"ScheduleTime": {
			"type": "object",
			"properties": {
				"items": {
					"type": "array",
					"items": {
						"$ref": "#/definitions/ScheduleTimeInfo"
					}
				}
			}
		},
		"ScheduleTimeInfo": {
			"type": "object",
			"properties": {
				"code": {
					"description": "Identificador do registro",
					"required": true,
					"type": "string",
					"example": "1$_$2_$3_$4_$0500",
					"x-totvs": [
						{
							"product": "RM",
							"field": "SZHORARIO.CODCOLIGADA|SZHORARIO.IDPERIODO|SZHORARIO.CODIGO|SZHORARIO.DIA|SZHORARIO.HORAINI",
							"required": true,
							"type": "string",
							"available": true,
							"canUpdate": false,
							"note": "Este dado e formado pelos campos Código da Coligada, Código do Período, Código da agenda, Dia da semana, Hora início"
						}
					]
				},
				"companyId": {
					"description": "Código da coligada",
					"required": true,
					"type": "integer",
					"exemple": 1,
					"x-totvs": [
						{
							"product": "RM",
							"field": "SZHORARIO.CODCOLIGADA",
							"required": true,
							"type": "integer",
							"available": true,
							"canUpdate": false,
							"note": "Código da Coligada"
						}
					]
				},
				"scheduleId": {
					"description": "Código da Agenda",
					"required": true,
					"type": "integer",
					"exemple": 100,
					"x-totvs": [
						{
							"product": "RM",
							"field": "SZHORARIO.CODIGO",
							"required": true,
							"type": "integer",
							"available": true,
							"canUpdate": false,
							"note": "Código da Agenda"
						}
					]
                },
                "scheduleCode": {
					"description": "Código da Agenda",
					"required": true,
					"type": "string",
					"example": "1$_$1",
					"x-totvs": [
						{
							"product": "RM",
							"field": "SZHORARIO.CODCOLIGADA|SZHORARIO.CODIGO",
							"required": true,
							"type": "String",
							"available": true,
							"canUpdate": false,
							"note": "Este dado e formado pelos campos Código da Coligada e Código da Agenda"
						}
					]
				},
                "periodId": {
                    "description": "Código do Período",
					"required": true,
                    "type": "integer",                    
					"exemple": 10,
					"x-totvs": [
						{
							"product": "RM",
							"field": "SZHORARIO.IDPERIODO",
                            "required": true,
							"type": "integer",
							"available": true,
							"canUpdate": false,
							"note": "Código do Período"
						}
					]
                },
				"periodCode": {
					"description": "Código do Período",
					"required": false,
					"type": "string",
					"exemple": "1$_2",
					"x-totvs": [
						{
							"product": "RM",
							"field": "SZHORARIO.CODCOLIGADA|SZHORARIO.IDPERIODO",
							"required": false,
							"type": "string",
							"available": true,
                            "canUpdate": true,
							"note": "Código do Período"
						}
					]
				},
				"day": {
					"description": "Dia da Semana",
					"required": true,
					"type": "integer",
					"exemple": 2,
					"x-totvs": [
						{
							"product": "RM",
							"field": "SZHORARIO.DIA",
							"required": true,
							"type": "integer",
							"available": true,
                            "canUpdate": true,                            
							"note": "Dia da Semana"
						}
					]
                },
                "timeStart": {
					"description": "Hora Início",
					"required": true,
                    "type": "string",
                    "maxLength": 5,
					"exemple": "05:00",
					"x-totvs": [
						{
							"product": "RM",
							"field": "SZHORARIO.HORAINI",
                            "required": true,
                            "maxLength": 5,
							"type": "string",
							"available": true,
                            "canUpdate": true,                            
							"note": "Hora Início"
						}
					]                    
                },
                "timeEnd": {
					"description": "Hora Fim",
					"required": true,
                    "type": "string",
                    "maxLength": 5,
					"exemple": "05:00",
					"x-totvs": [
						{
							"product": "RM",
							"field": "SZHORARIO.HORAFIM",
                            "required": true,
                            "maxLength": 5,
							"type": "string",
							"available": true,
                            "canUpdate": true,                            
							"note": "Hora Fim"
						}
					]                    
                },
                "maxTimeSlot": {
					"description": "Encaixe Máximo",
					"required": false,
                    "type": "integer",                    
					"exemple": 5,
					"x-totvs": [
						{
							"product": "RM",
							"field": "SZHORARIO.MAXENCAIXE",
                            "required": false,
							"type": "integer",
							"available": true,
                            "canUpdate": true,                            
							"note": "Encaixe Máximo"
						}
					]
                },
                "weekDayDuration": {
                    "description": "Período",
					"required": false,
                    "type": "integer",               
					"exemple": 7,
					"x-totvs": [
						{
							"product": "RM",
							"field": "SZHORARIO.DURACAODIASEMANA",
                            "required": false,
							"type": "integer",
							"available": true,
                            "canUpdate": true,          
							"note": "Período"
						}
					]
                },
                "status": {
                    "description": "Status",
					"required": true,
                    "type": "string",
                    "maxLength": "1",
					"exemple": "T",
					"x-totvs": [
						{
							"product": "RM",
							"field": "SZHORARIO.STATUS",
                            "required": true,
							"type": "string",
							"available": true,
                            "canUpdate": true,          
							"note": "Período"
						}
					]
                }
			}
		}
	}
}
