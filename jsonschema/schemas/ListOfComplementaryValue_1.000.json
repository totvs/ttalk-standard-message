            {
              "product": "PROTHEUS",
              "field": "FKC_PERIOD",
              "required": true,
              "type": "varchar",
              "length": "1",
              "note": "1=Único;2=Diário;3=Mensal;4=Anual",
              "available": true,
              "canUpdate": false
            }
          ]
        },
