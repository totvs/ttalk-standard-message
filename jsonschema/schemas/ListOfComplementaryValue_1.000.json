            {
              "product": "PROTHEUS",
              "field": "FKC_CODIGO",
              "required": false,
              "type": "varchar",
              "length": "6",
              "note": "Código do valor acessório",
              "available": true,
              "canUpdate": false
            }
          ]
        },
        "Description": {
          "description": "Descrição",
          "type": "string",
          "x-totvs": [
            {
              "product": "RM",
              "field": "FPARAM.TEXTO",
              "required": true,
              "type": "varchar",
              "length": "30",
              "note": "Descrição do Valor Complementar",
              "available": true,
              "canUpdate": false
            },
            {
              "product": "PROTHEUS",
              "field": "FKC_DESC",
              "required": true,
              "type": "varchar",
              "length": "30",
              "note": "Descrição do tipo de Valor Acessório",
              "available": true,
              "canUpdate": false
            }
          ]
        },
