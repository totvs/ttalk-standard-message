              "required": true,
              "type": "integer",
              "length": "",
              "note": "Determina o efeito do valor acessório sobre o saldo do valor do lançamento financeiro",
              "available": true,
              "canUpdate": false
            },
            {
              "product": "PROTHEUS",
              "field": "FKC_ACAO",
              "required": true,
              "type": "varchar",
              "length": "1",
              "note": "Tipo de ação do Valor Acessório (1=Soma;2=Subtrai)",
              "available": true,
              "canUpdate": false
            }
          ]
