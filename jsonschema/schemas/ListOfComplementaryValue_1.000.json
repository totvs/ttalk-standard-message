          "type": "object",
          "x-totvs": [
            {
              "product": "RM",
              "field": "",
              "required": false,
              "type": "",
              "length": "",
              "note": "não tem equivalência no RM, será informado sempre \"single\"",
              "available": true,
              "canUpdate": false
