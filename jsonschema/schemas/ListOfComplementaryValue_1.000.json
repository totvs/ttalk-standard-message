              "product": "RM",
              "field": "FPARAM.INTEIRO",
              "required": true,
              "type": "integer",
              "length": "",
              "note": "Determina que o efeito do Valor de Integração deve ter efeito até a data de vencimento",
              "available": true,
              "canUpdate": false
            },
            {
              "product": "PROTHEUS",
              "field": "FKC_APLIC",
              "required": true,
              "type": "varchar",
              "length": "1",
              "note": "Aplicação do VA quanto a data de vencimento (1=Percentual;2=Valor)",
              "available": true,
              "canUpdate": false
