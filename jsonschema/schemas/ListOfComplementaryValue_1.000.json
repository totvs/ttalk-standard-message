          "type": "object",
          "x-totvs": [
            {
              "product": "RM",
              "field": "",
              "required": false,
              "type": "",
              "length": "",
              "note": "não tem equivalência no RM, será informado sempre \"Value\"",
              "available": true,
              "canUpdate": false
            },
            {
              "product": "PROTHEUS",
              "field": "FKC_TPVAL",
              "required": true,
              "type": "varchar",
              "length": "1",
              "note": "Tipo de Valor Acessório (1=Percentual;2=Valor)",
              "available": true,
              "canUpdate": false
            }
          ]
        },
