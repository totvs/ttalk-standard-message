          "x-totvs": [
            {
              "product": "RM",
              "field": "FPARAM.INTEIRO",
              "required": true,
              "type": "integer",
              "length": "",
              "note": "Informado se o valor opcional está ativo, para valores fixos da baixa é informado sempre \"true\"",
              "available": true,
              "canUpdate": false
            },
            {
              "product": "PROTHEUS",
              "field": "FKC_ATIVO",
              "required": true,
              "type": "varchar",
              "length": "1",
              "note": "1=Sim;2=Não",
              "available": true,
              "canUpdate": false
            }
          ]
        },
        "Portfolio": {
          "$ref": "#/definitions/PortfolioType",
          "description": "Carteira a que se aplica a regra de valor complementar",
          "type": "object",
          "x-totvs": [
            {
              "product": "RM",
              "field": "",
              "required": false,
              "type": "",
              "length": "",
              "note": "Nos casos de ação Acrescenta/Diminui, é enviado \"Ambos\", nos casos de Acrescenta se a Receber, e Diminui se a Receber, é nenviado \"Receber\"",
              "available": true,
              "canUpdate": false
            },
            {
              "product": "PROTHEUS",
              "field": "FKC_RECPAG",
              "required": true,
              "type": "varchar",
              "length": "1",
              "note": "Aplicação do VA quanto a carteira (1=Pagar;2=Receber;3=Ambas)",
              "available": true,
              "canUpdate": false
            }
          ]
        },
        "DayFactor": {
          "description": "Fator dia - Complementar a aplicação quanto à data de vencimento",
          "type": "integer",
          "format": "int32",
          "x-totvs": [
            {
              "product": "RM",
              "field": "FPARAM.INTEIRO",
              "required": true,
              "type": "integer",
              "length": "",
              "note": "Envia valor complementar do valor opcional, se ação for do tipo Acrescenta (dias em atraso)",
              "available": true,
              "canUpdate": false
