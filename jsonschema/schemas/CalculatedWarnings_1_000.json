{
	"$schema": "https://raw.githubusercontent.com/totvs/ttalk-standard-message/master/jsonschema/schemas/events/CalculatedWarnings_1_000.json#",
	"info": {
		"description": "API para a entidade de aviso calculado (CalculatedWarning) para produtos TOTVS",
		"version": "1.000",
		"title": "Aviso Calculado",
		"contact": {
			"name": "T-Talk",
			"url": "API.Totvs.com.br",
			"email": "comiteintegracao@totvs.com.br"
		},
		"x-totvs": {
			"messageDocumentation": {
				"name": "CalculatedWarnings",
				"description": "Aviso Calculado",
				"segment": "Recursos Humanos"
			},
			"productInformation": [
				{
					"product": "RM",
					"contact": "rm_FW@totvs.com.br",
					"description": "Listagem dos avisos calculados"
				}
			]
		}
	},
	"definitions": {
		"PagedCalculatedWarnings": {
			"type": "object",
			"allOf": [
				{
					"$ref": "https://raw.githubusercontent.com/totvs/ttalk-standard-message/master/jsonschema/apis/types/totvsApiTypesBase.json#/definitions/Paging"
				},
				{
					"type": "object",
					"properties": {
						"items": {
							"type": "array",
							"items": {
								"$ref": "#/definitions/CalculatedWarningsInfo"
							}
						}
					}
				}
			]
		},
		"CalculatedWarningsInfo": {
			"type": "object",
			"properties": {
				"CompanyCode": {
					"type": "integer",
					"example": "1",
					"description": "Código da Empresa",
					"x-totvs": [
						{
							"product": "RM",
							"field": "AAVISOCALCULADO.CODCOLIGADA",
							"required": true,
							"type": "integer",
							"available": true,
							"canUpdate": false
						}
					]
				},
				"EmployeeId": {
					"type": "string",
					"example": "00007",
					"description": "Chapa",
					"x-totvs": [
						{
							"product": "RM",
							"field": "AAVISOCALCULADO.CHAPA",
							"required": true,
							"type": "string",
							"available": true,
							"canUpdate": false
						}
					]
				},
				"ReferenceDate": {
					"type": "datetime",
					"example": "1999-11-24 16:00:39",
					"description": "Data de referência da jornada",
					"x-totvs": [
						{
							"product": "RM",
							"field": "AAVISOCALCULADO.DATAREFERENCIA",
							"required": true,
							"type": "datetime",
							"available": true,
							"canUpdate": false
						}
					]
				},
				"WorkingDayId": {
					"type": "integer",
					"description": "ID da jornada",
					"x-totvs": [
						{
							"product": "RM",
							"field": "AAVISOCALCULADO.IDJORNADA",
							"required": true,
							"type": "integer",
							"available": true,
							"canUpdate": false
						}
					]
				},
				"WarningCode": {
					"type": "integer",
					"description": "Código do aviso",
					"x-totvs": [
						{
							"product": "RM",
							"field": "AAVISOCALCULADO.CODAVISO",
							"required": true,
							"type": "integer",
							"available": true,
							"canUpdate": false
						}
					]
				},
				"Description": {
					"type": "string",
					"description": "Descrição do aviso",
					"x-totvs": [
						{
							"product": "RM",
							"field": "AAVISOCALCULADO.DESCRICAO",
							"required": false,
							"type": "string",
							"available": true,
							"canUpdate": false
						}
					]
				},
				"Validated": {
					"type": "integer",
					"description": "Parâmetro booleano para verificar se o aviso foi validado",
					"x-totvs": [
						{
							"product": "RM",
							"field": "AAVISOCALCULADO.VALIDADO",
							"required": false,
							"type": "integer",
							"available": true,
							"canUpdate": false
						}
					]
				},
				"InitialDate": {
					"type": "datetime",
					"example": "1999-11-24 16:00:39",
					"description": "Data de início",
					"x-totvs": [
						{
							"product": "RM",
							"field": "AAVISOCALCULADO.DATAINICIO",
							"required": false,
							"type": "datetime",
							"available": true,
							"canUpdate": false
						}
					]
				},
				"EndDate": {
					"type": "datetime",
					"example": "1999-11-24 16:00:39",
					"description": "Data fim",
					"x-totvs": [
						{
							"product": "RM",
							"field": "AAVISOCALCULADO.DATAFIM",
							"required": false,
							"type": "datetime",
							"available": true,
							"canUpdate": false
						}
					]
				},
				"id": {
					"type": "string",
					"description": "Identificador do aviso calculado",
					"x-totvs": [
						{
							"product": "RM",
							"available": true,
							"note": "Código do Aviso Calculado",
							"field": "calculatedwarning_id",
							"type": "string"
						}
					]
				}
			}
		}
	}
}