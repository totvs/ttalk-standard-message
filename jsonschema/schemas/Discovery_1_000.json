{
    "$schema": "https://raw.githubusercontent.com/totvs/ttalk-standard-message/master/jsonschema/schemas/DiscoveryEnvironment_1_000.json#",
    "info": {
        "description": "Contrato de Mensagem Padronizada para a entidade ambiente do produto Totvs Discovery",
        "version": "1.000",
        "title": "Discovery",
        "contact": {
            "name": "T-Talk",
            "url": "API.Totvs.com.br",
            "email": "comiteintegracao@totvs.com.br"
        },
        "x-totvs": {
            "transactionDefinition": {
                "subType": "event",
                "businessContentType": {
                    "type": "object",
                    "$ref": "#/definitions/GetEnvironmentsType"
                },
                "returnContentType": {
                    "type": "object",
                    "$ref": "https://raw.githubusercontent.com/totvs/ttalk-standard-message/master/jsonschema/schemas/types/ListOfInternalId_1_000.json#/definitions/ReturnContentWithModelType"
                }
            },
            "messageDocumentation": {
                "name": "Discovery",
                "description": "Ambiente Discovery",
                "segment": "Foundation"
            },
            "productInformation": [{
                "product": "Protheus",
                "contact": "Protheus_FW@totvs.com.br",
                "description": "Ambiente Discovery",
                "adapter": "FWRestEnvInfo.prw",
                "helpUrl": "link aqui"
            }]
        }
    },
    "definitions": {
        "PagedEnvironments": {
            "type": "object",
            "allOf": [{
                    "$ref": "https://raw.githubusercontent.com/totvs/ttalk-standard-message/master/jsonschema/apis/types/totvsApiTypesBase.json#/definitions/Paging"
                },
                {
                    "type": "object",
                    "properties": {
                        "items": {
                            "type": "array",
                            "items": {
                                "$ref": "#/definitions/GetEnvironmentsType"
                            }
                        }
                    }
                }
            ]
        },
        "PagedResources": {
            "type": "object",
            "allOf": [{
                    "$ref": "https://raw.githubusercontent.com/totvs/ttalk-standard-message/master/jsonschema/apis/types/totvsApiTypesBase.json#/definitions/Paging"
                },
                {
                    "type": "object",
                    "properties": {
                        "category": {
                            "type": "string",
                            "example": "server",
                            "description": "Categoria (server, database, gateway)"
                        },
                        "port": {
                            "type": "string",
                            "example": "8075",
                            "description": "porta"
                        }
                    }
                }
            ]
        },
        "PagedRefreshing": {
            "type": "object",
            "allOf": [{
                    "$ref": "https://raw.githubusercontent.com/totvs/ttalk-standard-message/master/jsonschema/apis/types/totvsApiTypesBase.json#/definitions/Paging"
                },
                {
                    "type": "object",
                    "properties": {
                        "items": {
                            "type": "array",
                            "items": {
                                "$ref": "#/definitions/RefreshingType"
                            }
                        }
                    }
                }
            ]
        },
        "PagedIdReceiver": {
            "type": "object",
            "allOf": [{
                    "$ref": "https://raw.githubusercontent.com/totvs/ttalk-standard-message/master/jsonschema/apis/types/totvsApiTypesBase.json#/definitions/Paging"
                },
                {
                    "type": "object",
                    "properties": {
                        "items": {
                            "type": "array",
                            "items": {
                                "$ref": "#/definitions/IDReceiverType"
                            }
                        }
                    }
                }
            ]
        },
        "PagedTotvs": {
            "type": "object",
            "allOf": [{
                    "$ref": "https://raw.githubusercontent.com/totvs/ttalk-standard-message/master/jsonschema/apis/types/totvsApiTypesBase.json#/definitions/Paging"
                },
                {
                    "type": "object",
                    "properties": {
                        "items": {
                            "type": "array",
                            "items": {
                                "$ref": "#/definitions/TotvsType"
                            }
                        }
                    }
                }
            ]
        },
        "PagedAuthentication": {
            "type": "object",
            "allOf": [{
                    "$ref": "https://raw.githubusercontent.com/totvs/ttalk-standard-message/master/jsonschema/apis/types/totvsApiTypesBase.json#/definitions/Paging"
                },
                {
                    "type": "object",
                    "properties": {
                        "avatar": {
                            "description": "avatar",
                            "type": "string",
                            "maxLength": 100,
                            "example": ""
                        },
                        "dialect": {
                            "description": "idioma do ambiente",
                            "example": "pt-br",
                            "type": "string",
                            "maxLength": 100
                        },
                        "fullName": {
                            "description": "fullName",
                            "example": "Administrador",
                            "type": "string",
                            "maxLength": 100
                        },
                        "username": {
                            "description": "username",
                            "example": "admin",
                            "type": "string",
                            "maxLength": 100
                        },
                        "email": {
                            "description": "email",
                            "example": "admin@totvs.com.br",
                            "type": "string",
                            "maxLength": 100
                        },
                        "token": {
                            "description": "token de acesso",
                            "type": "string",
                            "maxLength": 250,
                            "example": ""
                        }
                    }
                }
            ]
        },
        "PagedConfig": {
            "type": "object",
            "allOf": [{
                    "$ref": "https://raw.githubusercontent.com/totvs/ttalk-standard-message/master/jsonschema/apis/types/totvsApiTypesBase.json#/definitions/Paging"
                },
                {
                    "type": "object",
                    "properties": {
                        "items": {
                            "type": "array",
                            "items": {
                                "$ref": "#/definitions/ConfigType"
                            }
                        }
                    }
                }
            ]
        },
        "RefreshingType": {
            "type": "object",
            "properties": {
                "refresh": {
                    "description": "Esta sendo atualizado",
                    "type": "boolean",
                    "maxLength": 1
                }
            }
        },
        "GetEnvironmentsType": {
            "type": "object",
            "properties": {
                "name": {
                    "description": "nome do ambiente",
                    "example": "P12127_BRASIL",
                    "type": "string",
                    "maxLength": 100
                }
            }
        },
        "TotvsType": {
            "type": "object",
            "properties": {
                "totvs": {
                    "description": "totvs",
                    "type": "boolean",
                    "maxLength": 1
                }
            }
        },
        "ConfigType": {
            "type": "object",
            "properties": {
                "sendDataTOTVS": {
                    "description": "Envia dados para a TOTVS",
                    "type": "boolean",
                    "maxLength": 1
                },
                "historyDays": {
                    "type": "numeric",
                    "example": "30",
                    "description": "Quantidade de dias que o histórico é mantido"
                },
                "monitorActive": {
                    "type": "boolean",
                    "description": "Monitoramento dos recursos infra-estrutura estão ativos"
                },
                "monitorInterval": {
                    "type": "numeric",
                    "example": "15",
                    "description": "Intervalo em minutos de monitoramento dos recursos"
                }
            }
        },
        "IDReceiverType": {
            "type": "object",
            "properties": {
                "alias": {
                    "type": "string",
                    "example": "TOTVS",
                    "description": "Alias"
                },
                "TOTVSID": {
                    "description": "Id de instalacao TOTVS",
                    "type": "string",
                    "maxLength": 32,
                    "example": "xxxx9999x9x999999999x9999x9999xx"
                },
                "name": {
                    "type": "string",
                    "example": "TOTVS S.A",
                    "description": "Nome da instalacao"
                },

                "receive": {
                    "type": "boolean",
                    "description": "Receptor"
                }
            }
        },
        "PostEnvironmentType": {
            "type": "object",
            "properties": {
                "host": {
                    "type": "string",
                    "example": "localhost",
                    "description": "host"
                },
                "port": {
                    "type": "string",
                    "example": "1234",
                    "description": "port"
                },
                "name": {
                    "type": "string",
                    "example": "TOTVS S.A",
                    "description": "Nome da instalacao"
                },
                "category": {
                    "type": "string",
                    "description": "category",
                    "example": "database"
                },
                "TOTVSID": {
                    "type": "string",
                    "description": "identificador da instalação protheus",
                    "example": "xxxx9999x9x999999999x9999x9999xx"
                }
            }
        },
        "ResourcesType": {
            "type": "object",
            "properties": {
                "detail": {
                    "type": "string",
                    "example": "Tipo de execucao do appserver",
                    "description": "detalhes do ambiente"
                },
                "title": {
                    "description": "titulo",
                    "type": "string",
                    "maxLength": 32,
                    "example": "Memoria total"
                }
            },

            "value": {
                "type": "string",
                "example": "8074.54 MB",
                "description": "value",
                "maxLength": 50
            },

            "code": {
                "type": "string",
                "example": "as_physical_memory",
                "description": "codigo da informacao",
                "maxLength": 50
            }
        },
        "HealthType": {
            "type": "array",
            "properties": {
                "message": {
                    "description": "detalhes",
                    "example": "Consumo igual ou superior a 75%. A memoria fisica do computador esta em alto nivel de consumo",
                    "type": "string",
                    "maxLength": 250
                },
                "type": {
                    "type": "string",
                    "example": "success",
                    "description": "informa o status (success, warning, error)"
                }
            }
        },
        "AuthenticationType": {
            "type": "object",
            "properties": {
                "username": {
                    "description": "usuario",
                    "example": "admin",
                    "type": "string",
                    "maxLength": 50
                },
                "password": {
                    "description": "senha",
                    "type": "string",
                    "example": "123",
                    "maxLength": 50
                }
            }
        },
        "DeletedType": {
            "type": "object",
            "properties": {
                "deleted": {
                    "description": "deleted",
                    "type": "boolean"
                }
            }
        }
    }
}