{
    "$schema": "https://raw.githubusercontent.com/totvs/ttalk-standard-message/luciano1/jsonschema/schemas/Seller_3_001.json#",
    "info": {
        "description": "API para a entidade Vendedor (seller) para produtos TOTVS",
		"version": "2.000",
		"title": "Vendedor",
		"contact": {
			"name": "T-Talk",
			"url": "api.totvs.com.br",
			"email": "comiteintegracao@totvs.com.br"
		},
		"x-totvs": {
            "transactionDefinition": {
				"subType": "event",
				"businessContentType": {
					"type": "object",
					"$ref": "#/definitions/SellerInfo"
				},
				"returnContentType": {
					"type": "object",
					"$ref": "https://raw.githubusercontent.com/totvs/ttalk-standard-message/master/jsonschema/schemas/types/ListOfInternalId_1_000.json#/definitions/ReturnContentWithModelType"
				}
			},
			"messageDocumentation": {
				"name": "Seller",
				"description": "API para a entidade Vendedor (seller) para produtos TOTVS",
				"segment": "Serviços"
			},
			"productInformation": [{
					"product": "Protheus",
					"contact": "squad.crm@totvs.com.br",
					"description": "Cadastro de Vendedores",
					"adapter": "mats040.prw"
				}
			]
		}
    },
    "definitions": {
        "PagedSellers": {
            "type": "object",
            "allOf": [
                {
                    "$ref": "https://raw.githubusercontent.com/totvs/ttalk-standard-message/master/jsonschema/apis/types/totvsApiTypesBase.json#/definitions/Paging"
                },
                {
                    "$ref": "#/definitions/Sellers"
                }
            ]
        },
        "Sellers": {
            "type": "object",
            "properties": {
                "Seller": {
                    "type": "array",
                    "items": {
                        "$ref": "#/definitions/SellerInfo"
                    }
                }
            }
        },
        "SellerInfo": {
            "type": "object",
            "properties": {
                "companyId": {
                    "type": "string",
                    "description": "Empresa",
                    "x-totvs": [
                        {
                            "product": "RM",
                            "field": "TVEN.CODCOLIGADA",
                            "required": true,
                            "type": "integer",
                            "lenght": "2",
                            "available": true
                        },
                        {
                            "product": "Datasul",
                            "field": "representante.cod_empresa",
                            "required": true,
                            "type": "varchar",
                            "lenght": "5",
                            "available": true,
                            "description": "Codigo da empresa"
                        }
                    ]
                },
                "branchID": {
                    "type": "string",
                    "description": "Filial",
                    "x-totvs": [
                        {
                            "product": "RM",
                            "field": "TVEN.CODFILIAL",
                            "required": true,
                            "type": "integer",
                            "lenght": "2",
                            "available": true
                        },
                        {
                            "product": "Datasul",
                            "field": "SA3.A3_FILIAL",
                            "required": true,
                            "type": "varchar",
                            "lenght": "8",
                            "available": true,
                            "canUpdate": false
                        }
                    ]
                },
                "CompanyInternalId": {
                    "type": "string",
                    "description": "InternalId da chave completa de empresa do produto",
                    "x-totvs": [
                        {
                            "product": "RM",
                            "field": "TVEN.CODCOLIGADA|TVEN.CODFILIAL",
                            "required": true,
                            "type": "string",
                            "lenght": "10",
                            "available": true,
                            "note": "Concatenação da coligada do registro e filial do contexto"
                        },
                        {
                            "product": "Datasul",
                            "field": "representante.cod_empresa",
                            "required": true,
                            "type": "varchar",
                            "lenght": "5",
                            "available": true,
                            "note": "Código da Empresa"
                        },
                        {
                            "product": "Protheus",
                            "available": false,
                            "note": "Para o Protheus é esperado que com o cabeçalho da mensagem seja identificado empresa e filial já pelo Framework estando no ambiente (Empresa x Filial) correto"
                        }
                    ]
                },
                "code": {
                    "type": "string",
                    "description": "Código do vendedor",
                    "maxLength": 16,
                    "x-totvs": [
                        {
                            "product": "RM",
                            "field": "TVEN.CODVEN",
                            "description": "Código do vendedor",
                            "required": true,
                            "type": "varchar",
                            "lenght": "16",
                            "available": true
                        },
                        {
                            "product": "Datasul",
                            "field": "representante.cdn_repres",
                            "description": "Código do vendedor",
                            "required": true,
                            "type": "integer",
                            "lenght": "5",
                            "available": true
                        },
                        {
                            "product": "Protheus",
                            "field": "SA3.A3_COD",
                            "description": "Código",
                            "required": true,
                            "type": "varchar",
                            "lenght": "6",
                            "available": true,
                            "canUpdate": false
                        }
                    ]
                },
                "internalId": {
                    "type": "string",
                    "description": "InternalId da entidade",
                    "x-totvs": [
                        {
                            "product": "RM",
                            "field": ".",
                            "required": true,
                            "lenght": 18,
                            "description": "O internalID do Vendedor é formado por TVEN.CODCOLIGADA|TVEN.CODVEN",
                            "available": true,
                            "type": "varchar"
                        },
                        {
                            "product": "Protheus",
                            "available": true,
                            "note": "O internalID do Vendedor é formado por SA3.A3_FILIAL|SA3.A3_COD",
                            "type": "varchar"
                        },
                        {
                            "product": "Datasul",
                            "field": "representante.cod_empresa | string(representante.cdn_repres)",
                            "required": true,
                            "lenght": 10,
                            "description": "Empresa + Codigo do Representante",
                            "available": true,
                            "type": "char"
                        }
                    ]
                },
                "name": {
                    "type": "string",
                    "description": "Nome do vendedor",
                    "maxLength": 80,
                    "x-totvs": [
                        {
                            "product": "RM",
                            "field": "TVEN.NOME",
                            "description": "Nome do vendedor",
                            "required": true,
                            "type": "varchar",
                            "lenght": "80",
                            "available": true
                        },
                        {
                            "product": "Protheus",
                            "field": "SA3.A3_NOME",
                            "required": true,
                            "type": "varchar",
                            "lenght": "40",
                            "available": true,
                            "canUpdate": true
                        },
                        {
                            "product": "Datasul",
                            "field": "representante.nom_pessoa",
                            "required": true,
                            "type": "char",
                            "lenght": "80",
                            "available": true,
                            "canUpdate": true
                        }
                    ]
                },
                "shortName": {
                    "type": "string",
                    "description": "login do vendedor",
                    "maxLength": 25,
                    "x-totvs": [
                        {
                            "product": "RM",
                            "field": "TVEN.NOME",
                            "description": "O RM considera os primeiros 25 caracteres do campo Nome do Vendedor",
                            "required": false,
                            "type": "varchar",
                            "lenght": "80",
                            "available": true
                        },
                        {
                            "product": "Protheus",
                            "field": "SA3.A3_NREDUZ",
                            "required": false,
                            "type": "varchar",
                            "lenght": "25",
                            "available": true,
                            "canUpdate": true
                        },
                        {
                            "product": "Datasul",
                            "field": "representante.nom_abrev",
                            "description": "Nome Abreviado do Representante",
                            "required": true,
                            "type": "char",
                            "lenght": "12",
                            "available": true,
                            "canUpdate": true
                        }
                    ]
                },
                "personalIdentification": {
                    "type": "string",
                    "description": "CPF do vendedor",
                    "maxLength": 25,
                    "x-totvs": [
                        {
                            "product": "Protheus",
                            "field": "SA3.A3_CGC",
                            "required": false,
                            "type": "varchar",
                            "lenght": "14",
                            "available": true,
                            "canUpdate": true
                        },
                        {
                            "product": "Datasul",
                            "field": "representante.cod_id_feder",
                            "required": false,
                            "type": "char",
                            "lenght": "14",
                            "available": true,
                            "canUpdate": true
                        }
                    ]
                },
                "sellerPassWord": {
                    "type": "string",
                    "description": "Senha do vendedor",
                    "maxLength": 6,
                    "x-totvs": [
                        {
                            "product": "Protheus",
                            "field": "SA3.A3_SENHA",
                            "description": "Senha do Vendedor no Portal do Vendedor",
                            "required": false,
                            "type": "varchar",
                            "lenght": "6",
                            "available": true,
                            "canUpdate": true
                        }
                    ]
                },
                "sellerPhoneDDD": {
                    "type": "string",
                    "description": "DDD do vendedor",
                    "maxLength": 3,
                    "x-totvs": [
                        {
                            "product": "Protheus",
                            "field": "SA3.A3_DDTEL",
                            "required": false,
                            "type": "varchar",
                            "lenght": "3",
                            "available": true,
                            "canUpdate": true
                        }
                    ]
                },
                "sellerPhone": {
                    "type": "string",
                    "description": "Telefone do vendedor",
                    "maxLength": 15,
                    "x-totvs": [
                        {
                            "product": "Protheus",
                            "field": "SA3.A3_TEL",
                            "required": false,
                            "type": "varchar",
                            "lenght": "15",
                            "available": true,
                            "canUpdate": true
                        },
                        {
                            "product": "Datasul",
                            "field": "pessoa_jurid.cod_telefone ou pessoa_fisic.cod_telefone",
                            "required": false,
                            "type": "char",
                            "lenght": "20",
                            "available": true,
                            "canUpdate": true,
                            "description": "Telefone da Pessoa do Representante"
                        }
                    ]
                },
                "sellerEmail": {
                    "type": "string",
                    "description": "E-mail do vendedor",
                    "maxLength": 80,
                    "x-totvs": [
                        {
                            "product": "Protheus",
                            "field": "SA3.A3_EMAIL",
                            "required": false,
                            "type": "varchar",
                            "lenght": "80",
                            "available": true,
                            "canUpdate": true
                        },
                        {
                            "product": "Datasul",
                            "field": "pessoa_jurid.cod_e_mail ou pessoa_fisic.cod_e_mail",
                            "required": false,
                            "type": "char",
                            "lenght": "80",
                            "available": true,
                            "canUpdate": true,
                            "description": "E-mail da Pessoa do Representante"
                        }
                    ]
                },
                "address": {
                    "type": "object",
                    "description": "Endereço do Vendedor",
                    "$ref": "https://raw.githubusercontent.com/totvs/ttalk-standard-message/master/jsonschema/schemas/types/Address_2_000.json#/definitions/AddressInfo"
                },
                "salesChargeInformation": {
                    "type": "object",
                    "description": "Informações de comissões",
                    "properties": {
                        "customerVendorInternalId": {
                            "type": "string",
                            "description": "Código de fornecedor do vendedor para processamento de comissões",
                            "x-totvs": [
                                {
                                    "product": "Protheus",
                                    "field": "SA3.A3_FORNECE",
                                    "required": false,
                                    "note": "required true quando integração com Newhotel",
                                    "description": "SA2.A2_FILIAL|SA2.A2_COD|SA2.A2_LOJA",
                                    "type": "varchar",
                                    "available": true,
                                    "lenght": "50"
                                }
                            ]
                        },
                        "salesChargeInterface": {
                            "type": "string",
                            "description": "Formato de cobrança de comissões: S-Contas a Pagar;F-Folha de Pagamento;N-Sem Interface",
                            "enum": [
                                "S",
                                "F",
                                "N"
                            ],
                            "x-totvs": [
                                {
                                    "product": "Protheus",
                                    "field": "SA3.A3_GERASE2",
                                    "required": false,
                                    "available": true,
                                    "type": "varchar",
                                    "lenght": "1",
                                    "example": "S=Contas a Pagar;F=Folha de Pagamento;N=Sem Interface"
                                }
                            ]
                        },
                        "indirectSeller": {
                            "type": "string",
                            "description": "Código do vendedor indireto",
                            "maxLength": 16,
                            "x-totvs": [
                                {
                                    "product": "Datasul",
                                    "description": "Código do Representante Indireto",
                                    "field": "repres_financ.cdn_repres_indir",
                                    "required": false,
                                    "type": "integer",
                                    "available": true,
                                    "lenght": "5"
                                }
                            ]
                        },
                        "indirectSellerInternalId": {
                            "type": "string",
                            "description": "InternalId do Vendedor Indireto",
                            "x-totvs": [
                                {
                                    "product": "Datasul",
                                    "description": "Empresa + Codigo do Representante Indireto",
                                    "field": "repres_financ.cod_empresa|string(repres_financ.cdn_repres_indir)",
                                    "required": false,
                                    "available": true,
                                    "type": "char",
                                    "lenght": "10"
                                }
                            ]
                        }
                    }
                },
                "active": {
                    "type": "boolean",
                    "description": "Vendedor ativo?",
                    "example": true,
                    "x-totvs": [
                        {
                            "product": "Protheus",
                            "field": "A3_MSBLQL",
                            "required": false,
                            "type": "char",
                            "available": true,
                            "canUpdate": true,
                            "description": "No Protheus Status active/inactive serão convertidos em: active='2',inactive='1'"
                        },
                        {
                            "product": "RM",
                            "field": "TVEN.INATIVO",
                            "required": false,
                            "available": true,
                            "type": "boolean",
                            "description": "Status do Vendedor: 0-Ativo 1-Inativo"
                        },
                        {
                            "product": "Datasul",
                            "field": "representante.ind_sit_repres = 'Ativo'",
                            "required": false,
                            "available": true,
                            "type": "char",
                            "lenght": "10",
                            "description": "Situação do Representante"
                        }
                    ]
                },
                "representativeType": {
                    "type": "string",
                    "description": "Tipo de representate: I=Interno;E=Externo;P=Parceiro",
                    "enum": [
                        "I",
                        "E",
                        "P"
                    ],
                    "x-totvs": [
                        {
                            "product": "Protheus",
                            "field": "SA3.A3_TIPO",
                            "required": false,
                            "available": true,
                            "type": "varchar",
                            "lenght": "1"
                        }
                    ]
                }
            }
        }
    }
}