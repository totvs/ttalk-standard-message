{
	"$schema": "https://raw.githubusercontent.com/totvs/ttalk-standard-message/master/jsonschema/schemas/Classes_1_000.json#",
	"info": {
		"description": "API para a entidade turmas (Classes) para produtos TOTVS",
		"version": "1.000",
		"title": "Turmas",
		"contact": {
			"name": "T-Talk",
			"url": "API.Totvs.com.br",
			"email": "comiteintegracao@totvs.com.br"
		},
		"x-totvs": {
			"messageDocumentation": {
				"name": "Classes",
				"description": "Turmas",
				"segment": "Recursos Humanos"
			},
			"productInformation": [
				{
					"product": "RM",
					"contact": "rm_FW@totvs.com.br",
					"description": "Listagem das turmas"
				}
			]
		}
	},
	"definitions": {
		"ClassesInfo": {
			"type": "object",
			"description": "Tabela de turmas",
			"properties": {
				"id": {
					"description": "Código do coligada e código da turma",
					"type": "string",
					"x-totvs": [
						{
							"product": "RM",
							"available": true,
							"note": "Código do coligada e código da turma",
							"field": "classes_id",
							"type": "string",
							"required": true
						}
					]
				},
				"CompanyCode": {
					"type": "string",
					"example": "1",
					"description": "Código da Empresa",
					"x-totvs": [
						{
							"product": "RM",
							"field": "VTURMAS.CODCOLIGADA",
							"required": true,
							"type": "integer",
							"length": 2,
							"available": true,
							"canUpdate": false
						}
					]
				},
				"CompanyCodeInternalId": {
					"type": "string",
					"description": "InternalId Company",
					"x-totvs": [
						{
							"product": "RM",
							"field": "GCOLIGADA.CODCOLIGADA",
							"required": false,
							"type": "integer",
							"length": 2,
							"available": true,
							"canUpdate": false
						}
					]
				},
				"ClassCode": {
					"type": "string",
					"example": "1",
					"description": "Código da turma",
					"x-totvs": [
						{
							"product": "RM",
							"field": "VTURMAS.CODTURMA",
							"required": true,
							"type": "varchar",
							"length": "tamanho máximo de 5",
							"available": true,
							"canUpdate": false
						}
					]
				},
				"ClassCodeInternalId": {
					"type": "string",
					"description": "InternalId Código turma",
					"x-totvs": [
						{
							"product": "RM",
							"field": "VTURMAS.CODCOLIGADA|VTURMAS.CODTUMA",
							"required": false,
							"available": true,
							"note": "A turma na linha RM é composta pelos campos CODCOLIGADA e CODTURMA"
						}
					]
				},
				"EntityCode": {
					"type": "string",
					"example": "1",
					"description": "Código da entidade que vai ministrar a turma",
					"x-totvs": [
						{
							"product": "RM",
							"field": "VTURMAS.CODENTIDADE",
							"required": true,
							"type": "varchar",
							"length": "tamanho máximo de 16",
							"available": true,
							"canUpdate": true
						}
					]
				},
				"EntityCodeInternalId": {
					"type": "string",
					"description": "InternalId Código da Entidade",
					"x-totvs": [
						{
							"product": "RM",
							"field": "VENTIDADES.CODENTIDADE",
							"required": false,
							"available": true
						}
					]
				},
				"Name": {
					"type": "string",
					"example": "Turma do RM",
					"description": "Nome da turma",
					"x-totvs": [
						{
							"product": "RM",
							"field": "VTURMAS.NOME",
							"required": false,
							"type": "varchar",
							"length": "tamanho máximo de 150",
							"available": true,
							"canUpdate": true
						}
					]
				},
				"Active": {
					"type": "boolean",
					"example": "0",
					"description": "Identifica se a turma está ativa ou inativa",
					"x-totvs": [
						{
							"product": "RM",
							"field": "VTURMAS.ATIVO",
							"required": false,
							"type": "smallint",
							"available": true,
							"canUpdate": true
						}
					]
				},
				"NumberOfOpenings": {
					"type": "integer",
					"format": "int32",
					"example": "5",
					"description": "Identifica o número de vagas da turma",
					"x-totvs": [
						{
							"product": "RM",
							"field": "VTURMAS.VAGAS",
							"required": true,
							"type": "int",
							"available": true,
							"canUpdate": true
						}
					]
				},
				"IsLocal": {
					"type": "string",
					"example": "Totvs",
					"description": "Informa o local que será ministrada a turma",
					"x-totvs": [
						{
							"product": "RM",
							"field": "VTURMAS.LOCAL",
							"required": false,
							"type": "varchar",
							"length": "tamanho máximo de 100",
							"available": true,
							"canUpdate": true
						}
					]
				},
				"BeginTime": {
					"type": "number",
					"example": "5",
					"description": "Informa a hora de inicio da turma",
					"x-totvs": [
						{
							"product": "RM",
							"field": "VTURMAS.HORAINICIO",
							"required": false,
							"type": "int",
							"available": true,
							"canUpdate": true
						}
					]
				},
				"EndTime": {
					"type": "number",
					"example": "10",
					"description": "Informa a hora de termino da turma",
					"x-totvs": [
						{
							"product": "RM",
							"field": "VTURMAS.HORATERMINO",
							"required": false,
							"type": "int",
							"available": true,
							"canUpdate": true
						}
					]
				},
				"CourseCode": {
					"type": "string",
					"example": "1",
					"description": "Informa o código do curso",
					"x-totvs": [
						{
							"product": "RM",
							"field": "VTURMAS.CODCURSO",
							"required": true,
							"type": "varchar",
							"available": true,
							"canUpdate": true
						}
					]
				},
				"CourseCodeInternalId": {
					"type": "string",
					"description": "InternalId Código do curso",
					"x-totvs": [
						{
							"product": "RM",
							"field": "VCURSOS.CODCURSO",
							"required": false,
							"available": true
						}
					]
				},
				"DateLastValidation": {
					"type": "string",
					"format": "date-time",
					"example": "1999-11-24 16:00:39",
					"description": "Informa a data e hora da ultima validação",
					"x-totvs": [
						{
							"product": "RM",
							"field": "VTURMAS.DTULTVALIDACAO",
							"required": false,
							"type": "datetime",
							"available": true,
							"canUpdate": true
						}
					]
				},
				"MinimumAttendance": {
					"type": "number",
					"example": "80",
					"description": "Informa a frequencia minima requerida na turma",
					"x-totvs": [
						{
							"product": "RM",
							"field": "VTURMAS.FREQUENCIAMINIMA",
							"required": false,
							"type": "smallint",
							"available": true,
							"canUpdate": false
						}
					]
				},
				"Courseload": {
					"type": "number",
					"example": "840",
					"description": "Informa a carga horaria da turma",
					"x-totvs": [
						{
							"product": "RM",
							"field": "VTURMAS.CARGAHORARIA",
							"required": false,
							"type": "int",
							"available": true,
							"canUpdate": true
						}
					]
				},
				"beginDate": {
					"type": "string",
					"format": "date-time",
					"example": "1999-11-24 16:00:39",
					"description": "Informa a data e hora de inicio da turma",
					"x-totvs": [
						{
							"product": "RM",
							"field": "VTURMAS.DTINICIO",
							"required": true,
							"type": "datetime",
							"available": true,
							"canUpdate": true
						}
					]
				},
				"EndDate": {
					"type": "string",
					"format": "date-time",
					"example": "1999-11-24 16:00:39",
					"description": "Informa a data e hora de termino da turma",
					"x-totvs": [
						{
							"product": "RM",
							"field": "VTURMAS.DTTERMINO",
							"required": false,
							"type": "dattimee",
							"available": true,
							"canUpdate": true
						}
					]
				},
				"InstructorCode": {
					"type": "number",
					"example": "1",
					"description": "Informa o código do instrutor",
					"x-totvs": [
						{
							"product": "RM",
							"field": "VTURMAS.CODINSTRUTOR",
							"required": false,
							"type": "int",
							"available": true,
							"canUpdate": true
						}
					]
				},
				"InstructorCodeInternalId": {
					"type": "string",
					"description": "InternalId Código do instrutor",
					"x-totvs": [
						{
							"product": "RM",
							"field": "PPESSOA.CODIGO",
							"required": false,
							"available": true
						}
					]
				},
				"showInternet": {
					"type": "boolean",
					"example": "0",
					"description": "Identifica se é pra exibir na internet",
					"x-totvs": [
						{
							"product": "RM",
							"field": "VTURMAS.EXIBEINTERNET",
							"required": false,
							"type": "boolean",
							"available": true,
							"canUpdate": true
						}
					]
				},
				"showAllCompany": {
					"type": "boolean",
					"example": "0",
					"description": "Identifica se é para exibir a turma para todas as coligadas",
					"x-totvs": [
						{
							"product": "RM",
							"field": "VTURMAS.EXIBETODASCOL",
							"required": false,
							"type": "boolean",
							"available": true,
							"canUpdate": true
						}
					]
				},
				"isInternal": {
					"type": "number",
					"example": "1",
					"description": "Informa se a turma é interna da empresa",
					"x-totvs": [
						{
							"product": "RM",
							"field": "VTURMAS.INTERNO",
							"required": false,
							"type": "boolean",
							"length": 22,
							"available": true,
							"canUpdate": true
						}
					]
				},
				"isActiveIMG": {
					"type": "number",
					"example": "1",
					"description": "Informa se a turma é interna da empresa. TurmaAberta se a turma está ativa. TurmaFechada se a turma não está ativa "
				}
			}
		}
	}
}