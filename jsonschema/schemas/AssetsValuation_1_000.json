{
	"$schema": "https://raw.githubusercontent.com/totvs/ttalk-standard-message/master/jsonschema/schemas/AssetsValuation_1_69690.json#",
	"info": {
		"description": "",
		"version": "1.000",
		"title": "AssetsValuation",
		"contact": {},
		"x-totvs": {
			"messageDocumentation": {
				"name": "AssetsValuation",
				"description": "Avaliação de Ativos",
				"segment": ""
			},
			"transactionDefinition": {
				"subType": "event",
				"businessContentType": {
					"$ref": "#/definitions/BusinessContentType",
					"type": "object"
				},
				"returnContentType": {
					"$ref": "#/definitions/ReturnContent",
					"type": "object"
				}
			}
		}
	},
	"definitions": {
		"BusinessContentType": {
			"type": "object",
			"properties": {
				"Code": {
					"description": "Código do movimento de ampliação do valor do Ativo/Bem",
					"type": "string",
					"maxLength": 6
				},
				"InternalId": {
					"description": "InternalId da ampliação patrimonial",
					"type": "string"
				},
				"CompanyId": {
					"description": "Empresa",
					"type": "string"
				},
				"BranchId": {
					"description": "Filial",
					"type": "string"
				},
				"CompanyInternalId": {
					"description": "InternalId da Empresa",
					"type": "string"
				},
				"AssetInternalId": {
					"description": "Código do Ativo que deseja ampliar o valor",
					"type": "string"
				},
				"RegisterDateTime": {
					"description": "Data da ampliação do valor do Ativo",
					"type": "string",
					"format": "date-time"
				},
				"CalculationType": {
					"description": "Forma de calculo do valor a ser acrescido. / 000 - Utilizado Valor nominal a ser multiplicado pela quantidade, caso seja maior que 1 / 001 - Utilizado Valor nominal a rateado pela quantidade, caso seja maior que 1 / 002 - Utilizado Valor percentual a rateado pela quantidade, caso seja maior que 1",
					"type": "string",
					"minLength": 3,
					"maxLength": 3,
					"enum": [
						"000",
						"001",
						"002"
					]
				},
				"AssetAccounting": {
					"description": "Indica se há contabilização da ampliação do valor do Ativo",
					"type": "boolean"
				},
				"Value": {
					"description": "Valor nominal a ser acrescido no valor do ativo",
					"type": "number",
					"multipleOf": 0.01,
					"minimum": -9999999999.99,
					"maximum": 9999999999.99
				},
				"Percentual": {
					"description": "Percentual do valor a ser acrescido no valor do ativo",
					"type": "number",
					"multipleOf": 0.01,
					"minimum": -9999999999.99,
					"maximum": 9999999999.99
				},
				"Quantity": {
					"description": "Quantidade",
					"type": "number",
					"multipleOf": 0.01,
					"minimum": -9999999999.99,
					"maximum": 9999999999.99
				},
				"Observation": {
					"description": "Observação",
					"type": "string"
				}
			}
		},
		"ReturnContent": {
			"type": "object",
			"properties": {
				"ListOfInternalId": {
					"type": "array",
					"items": {
						"$ref": "https://raw.githubusercontent.com/totvs/ttalk-standard-message/master/jsonschema/schemas/types/ListOfInternalId_1_69690.json#/definitions/ListOfInternalIdType",
						"type": "object"
					}
				}
			}
		}
	}
}