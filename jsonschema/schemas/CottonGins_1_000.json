{
    "$schema": "https://raw.githubusercontent.com/totvs/ttalk-standard-message/CottonGins/v1/1_000/jsonschema/schemas/CottonGins_1_000.json#",

	"info": {
		"description": "Contrato de Mensagem Padronizada para a Cadastro de Unidades de Beneficiamento para produtos TOTVS",
		"version": "1.000",
		"title": "CottonGins",
		"contact": {
			"name": "T-Talk",
			"url": "API.Totvs.com.br",
			"email": "comiteintegracao@totvs.com.br"
		},
		"x-totvs": {
			"transactionDefinition": {
				"subType": "event",
				"businessContentType": {
					"type": "object",
					"$ref": "#/definitions/CottonGinsType"
				},
				"returnContentType": {
					"type": "object",
					"$ref": "https://raw.githubusercontent.com/totvs/ttalk-standard-message/master/jsonschema/schemas/types/ListOfInternalId_1_000.json#/definitions/ReturnContentWithModelType"
				}
			},
			"messageDocumentation": {
				"name": "CottonGins",
				"description": "Unidades de Beneficiamento",
				"segment": "Agroindústria"
			},
			"productInformation": [{
					"product": "protheus",
					"contact": "Protheus_FW@totvs.com.br",
					"description": "Unidades de Beneficiamento",
					"adapter": "agra660api.prw",
					"helpUrl": "link aqui"
				}
			]
		}
	},

	"definitions": {
		"PagedBranches": {
			"type": "object",
			"allOf": [{
					"$ref": "https://raw.githubusercontent.com/totvs/ttalk-standard-message/master/jsonschema/apis/types/totvsApiTypesBase.json#/definitions/Paging"
				}, {
					"type": "object",
					"properties": {
						"items": {
							"type": "array",
							"items": {
								"$ref": "#/definitions/CottonGinsType"
							}
						}
					}
				}
			]
		},

		"CottonGinsType": {
			"type": "object",
			"properties": {
				"InternalId":{
                    "type":"string",
                    "description":"Chave da unidade de beneficiamento",
                    "example": "99|01",
                    "x-totvs":[{
                            "product": "protheus",
                            "field": "DX3_FILIAL|DX3_CODIGO",
                            "type": "Char",
                            "length": "5",
							"available": true,
							"canUpdate": false,
							"note": "Este dado é formado pelos campos: Filal do cadastro, Codigo da unidade de beneficiamento"
                        }
					]
				},
				"SourceBranch": {
					"type": "string",
					"example": "99",
					"description": "Filial do Sistema",
					"x-totvs": [{
							"product": "protheus",
							"Field": "DX3.DX3_FILIAL",
							"Required": true,
							"Type": "Char",
							"length": "2",
							"available": true,
							"canUpdate": false
						}
					]
				},
				"Code": {
					"type": "string",
					"example": "01",
					"description": "Código da unidade de beneficiamento'",
					"x-totvs": [{
							"product": "protheus",
							"field": "DX3.DX3_CODIGO",
							"required": false,
							"type": "Char",
							"length": "2",
							"available": true,
							"canUpdate": false
						}
					]
				},
				"Description": {
					"type": "string",
					"example": "Esteira Belo Horizonte",
					"description": "Nome da unidade de beneficiamento",
					"x-totvs": [{
							"product": "protheus",
							"field": "DX3.DX3_NOME",
							"required": true,
							"type": "Char",
							"length": "30",
							"available": true,
							"canUpdate": true
						}
					]
				}
			}
		}
	}
}