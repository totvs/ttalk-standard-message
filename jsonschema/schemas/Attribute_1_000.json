         "email":"adauto.matuzaki@totvs.com.br"
      },
      "x-totvs":{
         "messageDocumentation":{
            "description":"APIs T-Pesag-Server Service | Attribute",
            "name":"Attribute",
            "segment":"Agroind√∫stria"
         },
         "productInformation":[
            {
               "product":"pims",
               "adapter":"Attribute",
               "contact":"adauto.matuzaki@totvs.com.br",
               "description":"TOTVS Agro - PIMS",
               "helpUrl":"https://tdn.totvs.com/display/PA/PIMS+AGRO+HOME"
            }
         ],
         "transactionDefinition":{
            "subType":"event",
            "businessContentType":{
               "$ref":"#/definitions/AttributeDTO",
               "type":"object"
            },
            "returnContentType":{
               "$ref":"#/definitions/ReturnContentType",
               "type":"object"
            }
         }
      }
   },
   "definitions":{
      "AttributeDTO":{
         "type":"object",
         "properties":{
            "_expandables":{
               "type":"array",
               "items":{
                  "type":"string"
               }
            },
            "auxiliaryTable":{
               "type":"string",
               "example":966647755218443400,
               "allowEmptyValue":false,
               "x-totvs":[
                  {
                     "product":"pims",
                     "available":true,
                     "canUpdate":false,
                     "field":"tabela_auxilar",
                     "length":"30",
                     "note":"Auxiliary Table id",
                     "required":false,
                     "type":"String"
                  }
               ]
            },
            "cdAttributeERP":{
               "type":"string",
               "example":"ANO_AGRICOLA",
               "allowEmptyValue":false,
               "x-totvs":[
                  {
                     "product":"pims",
                     "available":true,
                     "canUpdate":false,
                     "field":"cd_atributo_erp",
                     "length":"30",
                     "note":"Attribute ERP code",
                     "required":false,
                     "type":"String"
                  }
               ]
            },
            "deAuxiliaryTable":{
               "type":"string",
               "example":"SILO",
               "description":"Auxiliary Table description",
               "allowEmptyValue":false
            },
            "deEntity":{
               "type":"string",
               "example":"PROPRIET",
               "description":"Entity description",
               "allowEmptyValue":false
            },
            "description":{
               "type":"string",
               "example":"Safra",
               "allowEmptyValue":false,
               "x-totvs":[
                  {
                     "product":"pims",
                     "available":true,
                     "canUpdate":false,
                     "field":"descricao",
                     "length":"30",
                     "note":"Attribute description",
                     "required":true,
                     "type":"String"
                  }
               ]
            },
            "entity":{
               "type":"string",
               "example":"ID_TABELA_SAFRA",
               "allowEmptyValue":false,
               "x-totvs":[
                  {
                     "product":"pims",
                     "available":true,
                     "canUpdate":false,
                     "field":"entidade",
                     "length":"30",
                     "note":"Entity id",
                     "required":false,
                     "type":"String"
                  }
               ]
            },
            "filters":{
               "type":"array",
               "example":"FilterDTO",
               "description":"List of FilterDTO",
               "allowEmptyValue":false,
               "items":{
                  "$ref":"#/definitions/FilterDTO"
               }
            },
            "id":{
               "type":"string",
               "example":8640689860603600000,
               "allowEmptyValue":false,
               "x-totvs":[
                  {
                     "product":"pims",
                     "available":true,
                     "canUpdate":false,
                     "field":"id",
                     "length":"30",
                     "note":"Id. Primary Key.",
                     "required":true,
                     "type":"String"
                  }
               ]
            },
            "key":{
               "type":"string",
               "example":"SAFRA",
               "allowEmptyValue":false,
               "x-totvs":[
                  {
                     "product":"pims",
                     "available":true,
                     "canUpdate":false,
                     "field":"chave",
                     "length":"20",
                     "note":"Attribute key",
                     "required":true,
                     "type":"String"
                  }
               ]
            },
            "nameColumn":{
               "type":"string",
               "example":"CD_SAFRA",
               "description":"Attribute name column",
               "allowEmptyValue":false
            },
            "origin":{
               "type":"string",
               "example":"equipamento",
               "allowEmptyValue":false,
               "x-totvs":[
                  {
                     "product":"pims",
                     "available":true,
                     "canUpdate":false,
                     "field":"origem",
                     "length":"15",
                     "note":"Attribute origin",
                     "required":false,
                     "type":"String"
                  }
               ]
            },
            "primaryKeyColumn":{
               "type":"string",
               "example":"ID_SAFRA_ID_SAFRA",
               "description":"Attribute primary key column",
               "allowEmptyValue":false
            },
            "queryColumn":{
               "type":"string",
               "example":"ID_SAFRA_CD_SAFRA",
               "allowEmptyValue":false,
               "x-totvs":[
                  {
                     "product":"pims",
                     "available":true,
                     "canUpdate":false,
                     "field":"coluna_consulta",
                     "length":"30",
                     "note":"Entity query column. Necessary just when Type is ENTIDADE",
                     "required":false,
                     "type":"String"
                  }
               ]
            },
            "searchType":{
               "type":"string",
               "example":"ENTIDADE",
               "allowEmptyValue":false,
               "x-totvs":[
                  {
                     "product":"pims",
                     "available":true,
                     "canUpdate":false,
                     "field":"tipo_busca",
                     "length":"15",
                     "note":"Entity search type. Necessary just when Type is ENTIDADE or TABELA_AUXILIAR (Can be LOOKUP or COMBOBOX )",
                     "required":false,
                     "type":"String"
                  }
               ]
            },
            "type":{
               "type":"string",
               "example":"ENTIDADE",
               "allowEmptyValue":false,
               "x-totvs":[
                  {
                     "product":"pims",
                     "available":true,
                     "canUpdate":false,
                     "field":"tipo",
                     "length":"15",
                     "note":"Attribute type (Can be TEXT, DATE, NUMBER, SWITCH, LOOKUP, TABELA_AUXILIAR, COMBOBOX, ENTIDADE, PASSWORD)",
                     "required":true,
                     "type":"String"
                  }
               ]
            }
         },
         "title":"AttributeDTO"
      },
      "CollectionResultResponseDTO":{
         "type":"object",
         "properties":{
            "hasNext":{
               "type":"boolean"
            },
            "items":{
               "type":"array",
               "items":{
                  "$ref":"#/definitions/DataTransferObject"
               }
            },
            "length":{
               "type":"integer",
               "format":"int32"
            }
         },
         "title":"CollectionResultResponseDTO"
      },
      "DataTransferObject":{
         "type":"object",
         "title":"DataTransferObject"
      },
      "FilterDTO":{
         "type":"object",
         "properties":{
            "_expandables":{
               "type":"array",
               "items":{
                  "type":"string"
               }
            },
            "field":{
               "type":"string",
               "example":"Campo",
               "allowEmptyValue":false,
               "x-totvs":[
                  {
                     "product":"pims",
                     "available":true,
                     "canUpdate":true,
                     "field":"campo",
                     "length":"30",
                     "note":"Field",
                     "required":false,
                     "type":"String"
                  }
               ]
            },
            "id":{
               "type":"string",
               "example":8640689860603600000,
               "allowEmptyValue":false,
               "x-totvs":[
                  {
                     "product":"pims",
                     "available":true,
                     "canUpdate":false,
                     "field":"id",
                     "length":"30",
                     "note":"Id. Primary Key.",
                     "required":true,
                     "type":"String"
                  }
               ]
            },
            "idAttribute":{
               "type":"string",
               "example":8640689860603600000,
               "allowEmptyValue":false,
               "x-totvs":[
                  {
                     "product":"pims",
                     "available":true,
                     "canUpdate":false,
                     "field":"id_atributo",
                     "length":"30",
                     "note":"Id. Attribute.",
                     "required":true,
                     "type":"String"
                  }
               ]
            },
            "idColumn":{
               "type":"string",
               "example":"ID_COLUNA",
               "description":"Id. Column",
               "allowEmptyValue":false
            },
            "value":{
               "type":"string",
               "example":"Valor",
               "allowEmptyValue":false,
               "x-totvs":[
                  {
                     "product":"pims",
                     "available":true,
                     "canUpdate":true,
                     "field":"valor",
                     "length":"30",
                     "note":"Value",
                     "required":true,
                     "type":"String"
                  }
               ]
            }
         },
         "title":"FilterDTO"
      },
      "ReturnContentType":{
         "type":"object",
         "properties":{
            "ListOfInternalId":{
               "type":"array",
               "items":{
                  "$ref":"https://raw.githubusercontent.com/totvs/ttalk-standard-message/master/jsonschema/schemas/types/ListOfInternalId_1_000.json#/definitions/ListOfInternalIdType",
                  "type":"object"
               }
            }
         }
      }
   }
}
