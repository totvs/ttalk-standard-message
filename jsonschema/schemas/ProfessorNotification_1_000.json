{
	"$schema": "https://raw.githubusercontent.com/totvs/ttalk-standard-message/ProfessorNotification/v1/1_000/jsonschema/schemas/ProfessorNotification_1_000.json#",
	"info": {
		"description": "Contrato de Mensagem Padronizada para listagem das notificações do professor produtos TOTVS",
		"version": "1.000",
		"title": "ProfessorNotification",
		"contact": {
			"name": "T-Talk",
			"url": "API.Totvs.com.br",
			"email": "comiteintegracao@totvs.com.br"
		},
		"x-totvs": {
			"messageDocumentation": {
				"name": "ProfessorNotification",
				"description": "Dados das notificações de um professor",
				"segment": "Educacional"
			},
			"productInformation": [
				{
					"product": "RM",
					"contact": "TribeEdu.Squad.EduPortalMobile@totvs.com.br",
					"description": "Notificações de um professor do Educacional"
				}
			]
		}
	},
	"definitions": {
		"PagedProfessorNotification": {
			"type": "object",
			"allOf": [
				{
					"$ref": "https://raw.githubusercontent.com/totvs/ttalk-standard-message/master/jsonschema/apis/types/totvsApiTypesBase.json#/definitions/Paging"
				},
				{
					"type": "object",
					"properties": {
						"items": {
							"type": "array",
							"items": {
								"$ref": "#/definitions/ProfessorNotification"
							}
						}
					}
				}
			]
		},
		"ProfessorNotification": {
			"description": "Descreve uma notificação de um professor do Educacional",
			"properties": {
				"CompanyCode": {
					"description": "Código da empresa",
					"type": "string",
					"example": 1,
					"x-totvs": [
						{
							"product": "RM",
							"available": true,
							"required": true,
							"note": "Código da empresa",
							"field": "CODCOLIGADA",
							"type": "integer",
							"canUpdate": false
						}
					]
				},		
				"BranchCode": {
					"type": "string",
					"example": "2",
					"description": "Código da Filial",
					"x-totvs": [
						{
							"product": "RM",
							"field": "GFILIAL.CODFILIAL",
							"required": true,
							"type": "integer",
							"length": 2,
							"available": true,
							"canUpdate": false
						}
					]
				},
				"LevelEducationCode": {
					"type": "integer",
					"format": "int32",
					"example": 1,
					"description": "Nível de ensino",
					"x-totvs": [
						{
							"product": "RM",
							"field": "STIPOCURSO.CODTIPOCURSO",
							"required": true,
							"type": "integer",
							"length": 2,
							"available": true,
							"canUpdate": false
						}
					]
				},
				"InternalId": {
					"type": "string",
					"description": "InternalId ProfessorNotification",
					"x-totvs": [
						{
							"product": "RM",
							"note": "SPROFESSORTURMA.CODCOLIGADA | SPROFESSORTURMA.IDTURMADISC | SPROFESSOR.CODPROF",
							"required": true,
							"available": false
						}
					]
				},
				"ClassCode": {
					"description": "Código da turma",
					"type": "string",
					"x-totvs": [
						{
							"product": "RM",
							"field": "STURMADISC.CODTURMA",
							"required": true,
							"type": "string",
							"length": "20",
							"note": "Código da turma",
							"available": true,
							"canUpdate": false
						}
					]
				},
				"SpecializationBranchCode": {
					"description": "Código da matriz aplicada",
					"type": "int",
					"example": "0001",
					"x-totvs": [
						{
							"product": "RM",
							"available": true,
							"required": true,
							"note": "Código da matriz aplicada",
							"field": "IDHABILITACAOFILIAL",
							"type": "int",
							"canUpdate": false
						}
					]
				},
				"DisciplineClassCode": {
					"type": "int",
					"example": "103",
					"description": "Id da turma/disciplina",
					"x-totvs": [
						{
							"product": "RM",
							"field": "STURMADISC.IDTURMADISC",
							"note": "Id da turma/disciplina",
							"required": false,
							"Type": "integer",
							"available": true,
							"canUpdate": false
						}
					]
				},
				"DisciplineCode": {
					"type": "string",
					"example": "001",
					"description": "Código da disciplina",
					"x-totvs": [
						{
							"product": "RM",
							"field": "STURMADISC.CODDISC",
							"required": false,
							"type": "varchar",
							"length": 20,
							"available": true,
							"canUpdate": false
						}
					]
				},				
				"DisciplineDescription": {
					"type": "string",
					"example": "Matemática",
					"description": "Descrição da disciplina",
					"x-totvs": [
						{
							"product": "RM",
							"field": "SDISCIPLINA.NOME",
							"required": false,
							"type": "varchar",
							"length": 20,
							"available": true,
							"canUpdate": false
						}
					]
				},
				"TermCode": {
					"type": "int",
					"example": "12",
					"description": "Id período letivo",
					"x-totvs": [
						{
							"product": "RM",
							"field": "SPLETIVO.IDPERLET",
							"required": false,
							"Type": "integer",
							"length": 5,
							"available": true,
							"canUpdate": false
						}
					]
				},
				"ProfessorCode": {
					"type": "string",
					"example": "PROF-01",
					"description": "Professor associado a notificação",
					"x-totvs": [
						{
							"product": "RM",
							"field": "SPROFESSOR.CODPROF",
							"note": "Professor associado a notificação",
							"required": false,
							"Type": "string",
							"available": true,
							"canUpdate": true
						}
					]
				},
				"EventType": {
					"type": "string",
					"example": "N",
					"enum": [
						"N",
						"F",
						"P",
						"R",
						"A",
						"D"
					],
					"default": "N",
					"description": "Tipo do evento de notificação. [N - Digitação de Notas | F - Digitação de Faltas | P - Plano de aula previstro | R - Plano de aula Realizado | A - Data da avaliação | D - Frequência diária]",
					"x-totvs": [
						{
							"product": "RM",
							"note": "Campo calculado que indica o tipo do evento",
							"required": true,
							"type": "string",
							"available": true,
							"canUpdate": false
						}
					]
				},
				"EventDescription": {
					"type": "string",
					"example": "Período de digitação de notas da etapa corrente",
					"description": "Descrição do evento da notificação",
					"x-totvs": [
						{
							"product": "RM",
							"note": "Descrição do evento da notificação - Campo calculado",
							"required": false,
							"Type": "string",
							"available": true,
							"canUpdate": true
						}
					]
				},
				"Status": {
					"type": "string",
					"example": "B",
					"enum": [
						"B",
						"E"
					],
					"default": "B",
					"description": "Status do evento [B - Em breve | E - Prazo Expirado]",
					"x-totvs": [
						{
							"product": "RM",
							"note": "Campo calculado que indica o status do evento",
							"required": true,
							"type": "string",
							"available": true,
							"canUpdate": false
						}
					]
				},
				"Deadline": {
					"type": "date",
					"example": "06/12/2018",
					"description": "Data limite",
					"x-totvs": [
						{
							"product": "RM",
							"note": "A Data limite vai depender do tipo de notificação (SETAPAS.DTLIMITEDIGITACAO | SETAPAS.DTLIMITECONTPREVISTO | SETAPAS.DTLIMITECONTEFETIVO | SPROVAS.DTPREVISTA | SPLANOAULA.DATA",
							"required": false,
							"Type": "date",
							"available": true,
							"canUpdate": true
						}
					]
				}			
			}
		}
	}
}