            "email": "comiteintegracao@totvs.com.br"
        },
        "x-totvs": {
            "messageDocumentation": {
                "name": "Painel de Impressão da CAT",
                "description": "API do Painel de Impressão da CAT",
                "segment": "Seviços"
            },
            "productInformation": [
                {
                    "product": "protheus",
                    "contact": "squad.taf.esocial@totvs.com.br",
                    "description": "API do Painel de Impressão da CAT",
                    "adapter": "WSTAF052.prw"
                }
            ]
        }
    },
    "definitions": {
        "CatReportItems": {
            "type": "object",
            "allOf": [
                {
                    "$ref": "https://raw.githubusercontent.com/totvs/ttalk-standard-message/master/jsonschema/apis/types/totvsApiTypesBase.json#/definitions/Paging"
                },
                {
                    "type": "object",
                    "properties": {
                        "items": {
                            "type": "array",
                            "items": {
                                "$ref": "#/definitions/CatReportList"
                            }
                        }
                    }
                }
            ]
        },
        "CatReportList": {
            "type": "object",
            "properties": {
                "requestId": {
                    "name": "requestId",
                    "description": "Identificador da Requisição de Processamento",
                    "example": "885ebca6-d865-cea7-de62-e7812e946336",
                    "type": "string",
                    "maxLength": 36
                }                
            },
            "Paging": {
                "type": "object",
                "properties": {
                    "hasNext": {
                        "type": "boolean",
                        "example": false
                    }
                }
            }
        },

        "StatusItems": {
            "type": "object",
            "allOf": [
                {
                    "$ref": "https://raw.githubusercontent.com/totvs/ttalk-standard-message/master/jsonschema/apis/types/totvsApiTypesBase.json#/definitions/Paging"
                },
                {
                    "type": "object",
                    "properties": {
                        "items": {
                            "type": "array",
                            "items": {
                                "$ref": "#/definitions/ListStatus"
                            }
                        }
                    }
                }
            ]
        },
        "ListStatus": {
            "type": "object",
            "properties": {
                "finished": {
                    "name": "finished",
                    "description": "Finalizado",
                    "example": "true",
                    "type": "boolean",
                    "maxLength": 10
                },
                "percent": {
                    "name": "percent",
                    "description": "Percentual",
                    "example": "100%",
                    "type": "string",
                    "maxLength": 20
                }
               
            },
            "Paging": {
                "type": "object",
                "properties": {
                    "hasNext": {
                        "type": "boolean",
                        "example": false
                    }
                }
            }
        },

        "CatValuesItems": {
            "type": "object",
            "allOf": [
                {
                    "$ref": "https://raw.githubusercontent.com/totvs/ttalk-standard-message/master/jsonschema/apis/types/totvsApiTypesBase.json#/definitions/Paging"
                },
                {
                    "type": "object",
                    "properties": {
                        "items": {
                            "type": "array",
                            "items": {
                                "$ref": "#/definitions/CatListValue"
                            }
                        }
                    }
                }
            ]
        },
        "CatListValue": {
            "type": "object",
            "properties": {
                "items": {
                    "branch": {
                    "name": "branch",
                    "description": "Código da filial",
                    "example": "D MG 01 ",
                    "type": "string",
                    "maxLength": 8
                },
                "catNumber": {
                    "name": "catNumber",
                    "description": "Número da CAT",
                    "example": "1.2.0000000000145721641",
                    "type": "string",
                    "maxLength": 23
                },
                "categCode": {
                    "name": "categCode",
                    "description": "Código da Categoria",
                    "example": "101",
                    "type": "string",
                    "maxLength": 3
                },
                "accidentDate": {
                    "name": "accidentDate",
                    "description": "Data do Acidente",
                    "example": "19/07/22",
                    "type": "string",
                    "maxLength": 8
                },
                "accidentTime": {
                    "name": "accidentTime",
                    "description": "Hora do Acidente",
                    "example": "18:04",
                    "type": "string",
                    "maxLength": 5
                },
                "accidentType": {
                    "name": "accidentType",
                    "description": "Tipo do Acidente",
                    "example": "Trajeto",
                    "type": "string",
                    "maxLength": 10
                },
                "catType": {
                    "name": "catType",
                    "description": "Tipo da CAT",
                    "example": "Inicial",
                    "type": "string",
                    "maxLength": 25
                },
                "eventType": {
                    "name": "eventType",
                    "description": "Tipo do Evento",
                    "example": "Inclusão",
                    "type": "string",
                    "maxLength": 10
                },
                "deathDate": {
                    "name": "deathDate",
                    "description": "Data do Óbito",
                    "example": "19/07/22",
                    "type": "string",
                    "maxLength": 8
                },
                "originCatNumber": {
                    "name": "originCatNumber",
                    "description": "Número da CAT de Origem",
                    "example": "1.2.0000000000145721641",
                    "type": "string",
                    "maxLength": 23
                },
                "cpf": {
                    "name": "cpf",
                    "description": "CPF",
                    "example": "418.615.610-75",
                    "type": "string",
                    "maxLength": 14
                },
                "name": {
                    "name": "name",
                    "description": "Nome",
                    "example": "LUIS MARCOS FIGUEIREDO",
                    "type": "string",
                    "maxLength": 255
                },
                "registration": {
                    "name": "registration",
                    "description": "Matrícula",
                    "example": "MATLUIS001",
                    "type": "string",
                    "maxLength": 30
                },
                "predecessorType": {
                    "name": "predecessorType",
                    "description": "Nome do Evento do Trabalhador",
                    "example": "S2200",
                    "type": "string",
                    "maxLength": 5
                }
            }               
            },
            "Paging": {
                "type": "object",
                "properties": {
                    "hasNext": {
                        "type": "boolean",
                        "example": false
                    }
                }
            }
        }
    },
    "parameters": {
        "companyId": {
            "name": "companyId",
            "description": "Código da Empresa e Código da Filial",
            "example": "T1|D MG 01",
            "in": "query",
            "required": true,
            "schema": {
                "type": "string"
            }
        },
        "requestId": {
            "name": "requestId",
            "description": "Código do Ticket para Requisição",
            "example": "857ab68b-b7f5-d1f5-c3f0-bae8de29ebaf",
            "in": "query",
            "required": true,
            "schema": {
                "type": "string"
            }
        }
    }
}