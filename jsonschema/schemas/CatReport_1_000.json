{
    "$schema": "https://raw.githubusercontent.com/totvs/ttalk-standard-message/master/jsonschema/schemas/CatReport_1_000.json#",
    "info": {
        "description": "API do Painel de Impressão da CAT",
        "version": "1.000",
        "title": "Painel de Impressão da CAT",
        "contact": {
            "name": "T-Talk",
