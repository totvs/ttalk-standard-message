{
	"$schema": "https://raw.githubusercontent.com/totvs/ttalk-standard-message/master/jsonschema/schemas/events/BiologicalMonitoringResponsible_1_000.json#",
	"info": {
		"description": "API para a entidade de Responsável pela Monitoração Biológica (BiologicalMonitoringResponsible) para produtos TOTVS",
		"version": "1.000",
		"title": "Responsável pela Monitoração Biológica",
		"contact": {
			"name": "T-Talk",
			"url": "API.Totvs.com.br",
			"email": "comiteintegracao@totvs.com.br"
		},
		"x-totvs": {
			"messageDocumentation": {
				"name": "BiologicalMonitoringResponsible",
				"description": "Responsável pela Monitoração Biológica",
				"segment": "Recursos Humanos"
			},
			"productInformation": [
				{
					"product": "RM",
					"contact": "rm_FW@totvs.com.br",
					"description": "Listagem dos Responsáveis pela Monitoração Biológica"
				}
			]
		}
	},
	"definitions": {
		"BiologicalMonitoringResponsibleGroups": {
			"type": "object",
			"allOf": [
				{
					"$ref": "https://raw.githubusercontent.com/totvs/ttalk-standard-message/master/jsonschema/apis/types/totvsApiTypesBase.json#/definitions/Paging"
				},
				{
					"type": "object",
					"properties": {
						"items": {
							"$ref": "#/definitions/BiologicalMonitoringResponsibleReadView"
						}
					}
				}
			]
		},
		"BiologicalMonitoringResponsibleSave": {
			"type": "object",
			"properties": {
				"CompanyCode": {
					"type": "string",
					"example": "1",
					"description": "Código da Empresa",
					"x-totvs": [
						{
							"product": "RM",
							"field": "VOCORRENCIA.CODCOLIGADA",
							"required": true,
							"type": "integer",
							"length": 2,
							"available": true,
							"canUpdate": false
						}
					]
				},
				"PersonCode": {
					"type": "integer",
					"format": "int32",
					"example": "15",
					"description": "Código da pessoa responsável",
					"x-totvs": [
						{
							"product": "RM",
							"field": "VRESPMONBIOLOGICA.CODPESSOA",
							"required": true,
							"type": "int",
							"available": true,
							"canUpdate": true
						}
					]
				},
				"SeqCode": {
					"description": "Seq. Do Responsável",
					"type": "integer",
					"format": "int32",
					"example": "15",
					"x-totvs": [
						{
							"product": "RM",
							"field": "VRESPMONBIOLOGICA.SEQRESPBIO",
							"required": true,
							"type": "int",
							"available": true,
							"canUpdate": false
						}
					]
				},
				"BranchId": {
					"type": "integer",
					"format": "int16",
					"example": "15",
					"description": "Código Filial",
					"x-totvs": [
						{
							"product": "RM",
							"field": "VRESPMONBIOLOGICA.CODFILIAL",
							"required": false,
							"type": "smallint",
							"available": true,
							"canUpdate": true
						}
					]
				},
				"BeginDate": {
					"type": "string",
					"format": "date-time",
					"description": "Data de Início",
					"x-totvs": [
						{
							"product": "RM",
							"field": "VRESPMONBIOLOGICA.DATAINICIAL",
							"required": true,
							"type": "datetime",
							"available": true,
							"canUpdate": true
						}
					]
				},
				"EndDate": {
					"type": "string",
					"format": "date-time",
					"description": "Data de Término",
					"x-totvs": [
						{
							"product": "RM",
							"field": "VRESPMONBIOLOGICA.DATAFINAL",
							"required": false,
							"type": "datetime",
							"available": true,
							"canUpdate": true
						}
					]
				},
				"Nit": {
					"description": "NIT",
					"type": "string",
					"example": "84818515155",
					"x-totvs": [
						{
							"product": "RM",
							"field": "VRESPMONBIOLOGICA.NIT",
							"required": false,
							"type": "string",
							"length": 11,
							"available": true,
							"canUpdate": true
						}
					]
				},
				"ClassCouncilRegistrationCode": {
					"description": "NIT",
					"type": "string",
					"example": "84818515155",
					"x-totvs": [
						{
							"product": "RM",
							"field": "VRESPMONBIOLOGICA.REGCONSELHOCLASSE",
							"required": false,
							"type": "string",
							"length": 20,
							"available": true,
							"canUpdate": true
						}
					]
				},
				"SectionCode": {
					"description": "Código da sessão",
					"type": "string",
					"example": "1.1.1",
					"x-totvs": [
						{
							"product": "RM",
							"field": "VRESPMONBIOLOGICA.REGCONSELHOCLASSE",
							"required": false,
							"type": "string",
							"length": 35,
							"available": true,
							"canUpdate": true
						}
					]
				},
				"CouncilState": {
					"description": "UF de expedição do CRM/COREN",
					"type": "string",
					"example": "MG",
					"x-totvs": [
						{
							"product": "RM",
							"field": "VRESPMONBIOLOGICA.UFREGCONSELHO",
							"required": false,
							"type": "string",
							"length": 2,
							"available": true,
							"canUpdate": true
						}
					]
				}
			}
		},
		"BiologicalMonitoringResponsibleReadRecord": {
			"type": "object",
			"allOf": [
				{
					"$ref": "#/definitions/BiologicalMonitoringResponsibleSave"
				},
				{
					"type": "object",
					"properties": {
						"id": {
							"description": "Código da coligada, Código Seq. do Responsável",
							"type": "string",
							"x-totvs": [
								{
									"product": "RM",
									"available": true,
									"note": "Código da coligada, Código da ocorrência",
									"field": "VRESPMONBIOLOGICA.CODCOLIGADA|VRESPMONBIOLOGICA.SEQRESPBIO",
									"type": "string"
								}
							]
						}
					}
				}
			]
		},
		"BiologicalMonitoringResponsibleReadView": {
			"type": "object",
			"allOf": [
				{
					"$ref": "#/definitions/BiologicalMonitoringResponsibleReadRecord"
				},
				{
					"type": "object",
					"properties": {
						"PersonName": {
							"type": "string",
							"example": "Itamar",
							"description": "Nome da Pessoa Responsável",
							"x-totvs": [
								{
									"product": "RM",
									"field": "PPESSOA.NOME",
									"required": true,
									"type": "varchar",
									"length": 120,
									"available": true,
									"canUpdate": false
								}
							]
						},
						"BranchDescription": {
							"description": "Descrição da filial",
							"type": "string",
							"x-totvs": [
								{
									"product": "RM",
									"field": "GFILIAL.NOMEFANTASIA",
									"required": false,
									"type": "string",
									"length": 100,
									"available": true,
									"canUpdate": false
								}
							]
						},
						"SectionDescription": {
							"description": "Nome da seção responsável",
							"type": "string",
							"x-totvs": [
								{
									"product": "RM",
									"field": "PSECAO.DESCRICAO",
									"required": false,
									"type": "string",
									"available": true,
									"canUpdate": false
								}
							]
						}
					}
				}
			]
		}
	}
}