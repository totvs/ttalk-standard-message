{
	"$schema": "https://raw.githubusercontent.com/totvs/ttalk-standard-message/master/jsonschema/schemas/events/AuthorizedOvertime_1_000.json#",
	"info": {
		"description": "API para a entidade extra autorizada (AuthorizedOvertime) para produtos TOTVS",
		"version": "1.000",
		"title": "Extra Autorizada",
		"contact": {
			"name": "T-Talk",
			"url": "API.Totvs.com.br",
			"email": "comiteintegracao@totvs.com.br"
		},
		"x-totvs": {
			"messageDocumentation": {
				"name": "AuthorizedOvertime",
				"description": "Extra Autorizada",
				"segment": "Recursos Humanos"
			},
			"productInformation": [
				{
					"product": "RM",
					"contact": "rm_FW@totvs.com.br",
					"description": "Listagem das extras autorizadas"
				}
			]
		}
	},
	"definitions": {
		"PagedAuthorizedOvertime": {
			"type": "object",
			"allOf": [
				{
					"$ref": "https://raw.githubusercontent.com/totvs/ttalk-standard-message/master/jsonschema/apis/types/totvsApiTypesBase.json#/definitions/Paging"
				},
				{
					"type": "object",
					"properties": {
						"items": {
							"type": "array",
							"items": {
								"$ref": "#/definitions/AuthorizedOvertimeInfo"
							}
						}
					}
				}
			]
		},
		"AuthorizedOvertimeInfo": {
			"type": "object",
			"properties": {
				"CompanyCode": {
					"type": "integer",
					"example": "1",
					"description": "Código da Empresa",
					"x-totvs": [
						{
							"product": "RM",
							"field": "AEAUTFUN.CODCOLIGADA",
							"required": true,
							"type": "integer",
							"available": true,
							"canUpdate": false
						}
					]
				},
				"EmployeeId": {
					"type": "string",
					"example": "00001",
					"description": "Chapa",
					"x-totvs": [
						{
							"product": "RM",
							"field": "AEAUTFUN.CHAPA",
							"required": true,
							"type": "string",
							"available": true,
							"canUpdate": false
						}
					]
				},
				"Date": {
					"type": "datetime",
					"example": "1999-11-24 16:00:39",
					"description": "Data de referência da extra autorizada",
					"x-totvs": [
						{
							"product": "RM",
							"field": "AEAUTFUN.DATA",
							"required": true,
							"type": "datetime",
							"available": true,
							"canUpdate": false
						}
					]
				},
				"InitialTime": {
					"type": "integer",
					"example": "1440",
					"description": "Hora início da extra autorizada em minutos",
					"x-totvs": [
						{
							"product": "RM",
							"field": "AEAUTFUN.HORAINICIO",
							"required": true,
							"type": "integer",
							"available": true,
							"canUpdate": false
						}
					]
				},
				"EndTime": {
					"type": "integer",
					"example": "1440",
					"description": "Hora fim da extra autorizada em minutos",
					"x-totvs": [
						{
							"product": "RM",
							"field": "AEAUTFUN.HORAFIM",
							"required": false,
							"type": "integer",
							"available": true,
							"canUpdate": false
						}
					]
				},
				"Authorized": {
					"type": "integer",
					"example": "1440",
					"description": "Horas autorizadas em minutos",
					"x-totvs": [
						{
							"product": "RM",
							"field": "AEAUTFUN.AUTORIZADO",
							"required": false,
							"type": "integer",
							"available": true,
							"canUpdate": false
						}
					]
				},
				"Reason": {
					"type": "string",
					"example": "Teste",
					"description": "Justificativa",
					"x-totvs": [
						{
							"product": "RM",
							"field": "AEAUTFUN.MOTIVO",
							"required": false,
							"type": "string",
							"available": true,
							"canUpdate": false
						}
					]
				},
				"ConflictSolution": {
					"type": "integer",
					"example": "1",
					"description": "Solução conflito",
					"x-totvs": [
						{
							"product": "RM",
							"field": "AEAUTFUN.SOLUCAOCONFLITO",
							"required": false,
							"type": "integer",
							"available": true,
							"canUpdate": false
						}
					]
				},
				"id": {
					"type": "string",
					"description": "Identificador da extra autorizada",
					"x-totvs": [
						{
							"product": "RM",
							"available": true,
							"note": "Código da Extra Autorizada",
							"field": "authorizedovertime_id",
							"type": "string"
						}
					]
				}
			}
		}
	}
}