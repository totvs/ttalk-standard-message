	"definitions": {
		"MRPDemandBase": {
			"type": "object",
			"properties": {
				"branchId": {
					"description": "Código da filial",
					"type": "string",
					"maxLength": 2,
					"x-totvs": [
						{
							"product": "PROTHEUS",
							"field": "T4J.T4J_FILIAL",
							"required": true,
							"type": "Char",
							"length": "2",
							"available": true,
							"canUpdate": false
						}
					]
				},
				"code": {
					"description": "Código da demanda",
					"type": "string",
					"maxLength": 200,
					"x-totvs": [
						{
							"product": "PROTHEUS",
							"field": "T4J.T4J_IDREG",
							"required": true,
							"type": "Char",
							"length": "200",
							"available": true,
							"canUpdate": false
						}
					]
				},
				"date": {
					"description": "Data da demanda",
					"type": "string",
					"format": "date",
					"x-totvs": [
						{
							"product": "PROTHEUS",
							"field": "T4J.T4J_DATA",
							"required": true,
							"type": "Char",
							"length": "8",
							"available": true,
							"canUpdate": true
						}
					]
				},
				"product": {
					"description": "Produto da demanda",
					"type": "string",
					"maxLength": 90,
					"x-totvs": [
						{
							"product": "PROTHEUS",
							"field": "T4J.T4J_PROD",
							"required": true,
							"type": "Char",
							"length": "90",
							"available": true,
							"canUpdate": false
						}
					]
				},
				"quantity": {
					"description": "Quantidade da demanda",
					"type": "number",
					"x-totvs": [
						{
							"product": "PROTHEUS",
							"field": "T4J.T4J_QUANT",
							"required": true,
							"format": "double",
							"length": 14.3,
							"available": true,
							"canUpdate": true
						}
					]
				},
				"warehouse": {
					"description": "Armazém da demanda",
					"type": "string",
					"maxLength": 10,
					"x-totvs": [
						{
							"product": "PROTHEUS",
							"field": "T4J.T4J_LOCAL",
