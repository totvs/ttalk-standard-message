					]
				},
				"revision": {
					"description": "Revisão da demanda",
					"type": "string",
					"maxLength": 3,
					"x-totvs": [
						{
							"product": "PROTHEUS",
							"field": "T4J.T4J_REV",
							"type": "Char",
							"length": "3",
							"required": false,
							"available": true,
							"canUpdate": true
						}
					]
				},
				"document": {
					"description": "Documento da demanda",
					"type": "string",
					"maxLength": 30,
					"x-totvs": [
						{
							"product": "PROTHEUS",
							"field": "T4J.T4J_DOC",
							"type": "Char",
							"length": "30",
							"required": false,
							"available": true,
							"canUpdate": true
						}
					]
				},
				"source": {
					"description": "Origem da demanda.",
					"type": "string",
					"maxLength": 1,
					"enum": [
						"1",
						"2",
						"3",
						"4",
						"5",
						"9"
					],
					"x-totvs": [
						{
							"product": "PROTHEUS",
							"field": "T4J.T4J_ORIGEM",
							"type": "Char",
							"length": "1",
							"required": false,
							"available": true,
							"canUpdate": true,
							"note": "1=Plano Mestre;2=Previsão de Vendas;3=Pedido de Vendas;4=Empenho Projeto;5=Arquivo;9=Manual"
						}
					]
				},
				"processed": {
					"description": "Indicador de demanda já processada pelo MRP.",
					"type": "string",
					"maxLength": 1,
					"enum": [
						"1",
						"2"
					],
					"x-totvs": [
						{
							"product": "PROTHEUS",
							"field": "T4J.T4J_PROC",
							"type": "Char",
							"length": "1",
							"required": false,
							"available": true,
							"canUpdate": true,
							"note": "1=Sim;2=Não"
						}
					]
				},
				"optional": {
					"description": "Opcionais da demanda",
					"type": "array",
					"items": {
						"type": "object",
						"properties": {
							"key": {
								"description": "Path da estrutura onde houve seleção de Opcionais",
								"type": "string",
								"maxLength": 254,
								"x-totvs": [
									{
										"product": "PROTHEUS",
										"field": "T4J.T4J_MOPC",
										"type": "Char",
										"length": "254",
										"required": true,
										"available": true,
										"canUpdate": true
									}
								]
							},
							"value": {
								"description": "Grupo e item de opcionais que foram selecionados para o path da estrutura",
								"type": "string",
								"maxLength": 100,
								"x-totvs": [
									{
										"product": "PROTHEUS",
										"field": "T4J.T4J_MOPC",
										"type": "Char",
										"length": "100",
										"required": true,
										"available": true,
										"canUpdate": true
									}
								]
							}
						}
					},
					"x-totvs": [
						{
							"product": "PROTHEUS",
							"field": "T4J.T4J_MOPC",
							"required": false,
							"available": true,
							"canUpdate": true
						}
					]
				}
			}
