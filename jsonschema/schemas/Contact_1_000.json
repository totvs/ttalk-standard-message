{
	"$schema": "https://raw.githubusercontent.com/totvs/ttalk-standard-message/master/jsonschema/schemas/Contact_1_000.json#",
	"info": {
		"description": "API para a entidade Contatos (Contact) para produtos TOTVS",
		"version": "1.000",
		"title": "Contact",
		"contact": {
			"name": "T-Talk",
			"url": "API.Totvs.com.br",
			"email": "comiteintegracao@totvs.com.br"
		},
		"x-totvs": {
			"transactionDefinition": {
				"subType": "event",
				"businessContentType": {
					"type": "object",
					"$ref": "#/definitions/ContactInfo"
				},
				"returnContentType": {
					"type": "object",
					"$ref": "https://raw.githubusercontent.com/totvs/ttalk-standard-message/master/jsonschema/schemas/types/ListOfInternalId_1_000.json#/definitions/ReturnContentWithModelType"
				}
			},
			"messageDocumentation": {
				"name": "Contact",
				"description": "API para a entidade Contato (contact) para produtos TOTVS",
				"segment": "Services"
			},
			"productInformation": [
				{
					"product": "protheus",
					"contact": "squad.crm@totvs.com.br",
					"description": "Cadastro de Contatos",
					"adapter": "TMKS070.prw"
				}
			]
		}
	},
	"definitions": {
		"PagedContacts": {
			"type": "object",
			"allOf": [
				{
					"$ref": "https://raw.githubusercontent.com/totvs/ttalk-standard-message/master/jsonschema/apis/types/totvsApiTypesBase.json#/definitions/Paging"
				},
				{
					"type": "object",
					"properties": {
						"items": {
							"type": "array",
							"items": {
								"$ref": "#/definitions/ContactInfo"
							}
						}
					}
				}
			]
		},
		"ContactInfo": {
			"type": "object",
			"properties": {
				"contactId": {
					"description": "contactId",
					"type": "string"
				},
				"companyId": {
					"type": "string",
					"description": "Código da Filial",
					"x-totvs": [
						{
							"product": "protheus",
							"required": false,
							"type": "char",
							"length": "2",
							"description": "Gestão de Empresa ativado",
							"available": true
						}
					]
				},
				"branchId": {
					"type": "string",
					"example": "D MG    ",
					"description": "Código da Empresa ou Código do Grupo da Empresa",
					"x-totvs": [
						{
							"product": "protheus",
							"field": "SU5.U5_FILIAL",
							"required": false,
							"type": "char",
							"length": "8",
							"description": "Pode ter tamanho variável até 8 com Gestão de Empresa ativado",
							"note": "Este campo e controlado pelo Framework atravez da função xFilial()",
							"available": true
						}
					]
				},
				"companyInternalId": {
					"type": "string",
					"description": "InternalId da chave completa da empresa",
					"x-totvs": [
						{
							"product": "protheus",
							"description": "Para o Protheus é esperado que com o cabeçalho da mensagem seja identificado empresa e filial",
							"available": true
						}
					]
				},
				"code": {
					"type": "string",
					"example": "000001",
					"description": "Código do Contato",
					"x-totvs": [
						{
							"product": "protheus",
							"field": "SU5.U5_CODCONT",
							"required": true,
							"type": "char",
							"length": "6",
							"available": true
						}
					]
				},
				"internalId": {
					"type": "string",
					"description": "InternalId de Integração",
					"x-totvs": [
						{
							"product": "protheus",
							"field": "cEmpAnt+SU5.U5_FILIAL+SU5.U5_CODCONT",
							"required": true,
							"type": "char",
							"length": "136",
							"available": true
						}
					]
				},
				"name": {
					"type": "string",
					"example": "JOHN SMITH",
					"description": "Nome do Contato",
					"x-totvs": [
						{
							"product": "protheus",
							"field": "SU5.U5_CONTAT",
							"required": true,
							"type": "char",
							"length": "30",
							"available": true
						}
					]
				},
				"governmentalInformation": {
					"type": "object",
					"description": "Informações Governamentais",
					"$ref": "https://raw.githubusercontent.com/totvs/ttalk-standard-message/master/jsonschema/schemas/types/GovernmentalInformation_1_000.json#/definitions/GovernmentalInformation"
				},
				"address": {
					"type": "array",
					"items": {
						"$ref": "https://raw.githubusercontent.com/totvs/ttalk-standard-message/master/jsonschema/schemas/types/Address_2_000.json#/definitions/AddressType"
					}
				},
				"listOfCommunicationInformation": {
					"type": "array",
					"description": "Informações de comunicação",
					"items": {
						"$ref": "https://raw.githubusercontent.com/totvs/ttalk-standard-message/master/jsonschema/schemas/Commons_1_000.json#/definitions/CommunicationInformationType"
					}
				},
				"treatment": {
					"type": "string",
					"example": "01",
					"description": "Tratamento do Contato",
					"x-totvs": [
						{
							"product": "protheus",
							"field": "SU5.U5_TRATA",
							"required": false,
							"type": "char",
							"length": "2",
							"available": true
						}
					]
				},
				"gender": {
					"type": "string",
					"example": "1",
					"description": "Genero : 1= Masculino;2= Feminino",
					"enum": [
						"1",
						"2",
						""
					],
					"x-totvs": [
						{
							"product": "protheus",
							"field": "SU5.U5_SEXO",
							"required": false,
							"type": "char",
							"length": "1",
							"available": true
						}
					]
				},
				"birthday": {
					"type": "string",
					"example": "19000101",
					"description": "Data de Nascimento",
					"x-totvs": [
						{
							"product": "protheus",
							"field": "SU5.U5_NIVER",
							"required": false,
							"type": "char",
							"length": "8",
							"available": true
						}
					]
				},
				"requester": {
					"type": "string",
					"example": "2",
					"description": "Solicitante? : 1-Sim;2-Não",
					"enum": [
						"1",
						"2"
					],
					"x-totvs": [
						{
							"product": "protheus",
							"field": "SU5.U5_SOLICTE",
							"required": false,
							"type": "char",
							"length": "1",
							"available": true
						}
					]
				},
				"situation": {
					"type": "string",
					"example": "2",
					"description": "Situação: 1-Ativo / 2-Bloqueado",
					"enum": [
						"1",
						"2"
					],
					"x-totvs": [
						{
							"product": "protheus",
							"field": "SU5.U5_MSBLQL",
							"required": false,
							"type": "char",
							"length": "1",
							"description": "Campo Opcional, se no cadastro esse campo existir, será enviado a TAG",
							"available": true
						}
					]
				}
			}
		}
	}
}