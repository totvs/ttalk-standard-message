	"definitions": {
		"BusinessContentType": {
			"type": "object",
			"properties": {
				"VehicleCode": {
					"description": "Codigo do Veiculo",
					"type": "string"
				},
				"VehicleNumberPlate": {
					"description": "Placa do veiculo",
					"type": "string",
					"maxLength": 8
				},
				"ListOfTrackingItens": {
					"description": "Posicionamento",
					"type": "array",
