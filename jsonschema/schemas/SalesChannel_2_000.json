					"adapter": "SalesChannel"
				}
			]
		}
	},
	"definitions": {
		"PagedSalesChannel": {
			"type": "object",
			"allOf": [
				{
					"$ref": "https://raw.githubusercontent.com/totvs/ttalk-standard-message/master/jsonschema/apis/types/totvsApiTypesBase.json#/definitions/Paging"
				},
				{
					"$ref": "#/definitions/SalesChannels"
				}
			]
		},
		"SalesChannels": {
			"type": "object",
			"properties": {
				"items": {
					"type": "array",
					"items": {
						"$ref": "#/definitions/SalesChannelInfo"
					}
				}
			}
		},
		"SalesChannelInfo": {
			"type": "object",
			"properties": {
				"InternalId": {
					"description": "InternalId para o Código do Canal de Vendas",
					"type": "string",
					"maxLength": 5,
					"x-totvs": [
						{
							"product": "DATASUL",
							"field": "canal-venda.cod-canal-venda",
							"required": true,
							"type": "integer",
							"length": "3",
							"note": "O InternalID do canal de venda para a linha Datasul é formado apenas pelo código do canal de venda ",
							"available": true,
							"canUpdate": false
						}
					]
				},
				"Code": {
					"description": "Código do Canal de Venda",
					"type": "string",
					"maxLength": 5,
					"x-totvs": [
						{
							"product": "DATASUL",
							"field": "canal-venda.cod-canal-venda",
							"required": true,
							"type": "integer",
							"length": "3",
							"note": "Código do Canal de Venda",
							"available": true,
							"canUpdate": false
						}
					]
				},
				"Description": {
					"description": "Descrição",
					"type": "string",
					"maxLength": 30,
					"x-totvs": [
						{
							"product": "DATASUL",
							"field": "canal-venda.descricao",
							"required": false,
							"type": "string",
							"length": "30",
							"note": "Descrição do canal de venda",
							"available": true,
							"canUpdate": false
						}
					]
				},
				"Observation": {
					"description": "Observação",
					"type": "string",
					"maxLength": 2000,
					"x-totvs": [
						{
							"product": "DATASUL",
							"field": "canal-venda.obs",
							"required": false,
							"type": "string",
							"length": "2000",
							"note": "Observação",
							"available": true,
							"canUpdate": false
						}
					]
				},
				"StartDateQuotaControl": {
					"description": "Início Controle Cotas",
					"type": "string",
					"format": "date",
					"x-totvs": [
						{
							"product": "DATASUL",
							"field": "canal-venda.dat-inic-ctrl-cotas",
							"required": false,
							"type": "date",
							"length": "",
							"note": "Data de início do controle de cotas. Essa informação é utilizada apenas se o módulo de cotas estiver habilitado no Datasul",
							"available": true,
							"canUpdate": false
						}
					]
				}
			}
		}
	}
