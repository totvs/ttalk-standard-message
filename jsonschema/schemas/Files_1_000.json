{  
  "info":{  
     "description":"API para a entidade files (arquivos SBX) do segmento HealthCare para produtos TOTVS",
     "version":"1.000",
     "title":"Products",
     "contact":{  
        "name":"T-Talk",
        "url":"api.Totvs.com.br",
        "email":"comiteintegracao@totvs.com.br"
     },
     "x-totvs":{  
        "messageDocumentation":{  
           "name":"Files",
           "description":"Files",
           "segment":"HealthCare"
        },
        "productInformation":[  
           {  
              "product":"central-obrigacoes",
              "contact":"centeraldeobrigacoes@totvs.com.br",
              "description":"Files",
              "adapter":"plsmvcarqsib.prw"
           }
        ]
     }
  },
  "definitions":{  
     "PagedFiles":{  
        "type":"object",
        "allOf":[  
           {  
              "$ref":"https://raw.githubusercontent.com/totvs/ttalk-standard-message/master/jsonschema/apis/types/totvsApiTypesBase.json#/definitions/Paging"
           },
           {  
              "$ref":"#/definitions/filesList"
           }
        ]
     },
     "filesList":{  
        "type":"object",
        "properties":{  
           "type":"array",
           "items":{  
              "$ref":"#/definitions/files"
           }
        }
     },
     "files":{  
      "type":"object",
      "properties":{  
         "fileName":{  
            "description":"",
            "type":"string",
            "x-totvs":[  
               {  
                  "product":"central-obrigacoes",
                  "field":"B3X.B3X_ARQUIV",
                  "required":true,
                  "type":"string",
                  "note":"Nome do arquivo",
                  "available":true,
                  "canUpdate":true
               }
            ]
         }
      }
   },
  "PagedCcos":{  
     "type":"object",
     "allOf":[  
         {  
            "$ref":"https://raw.githubusercontent.com/totvs/ttalk-standard-message/master/jsonschema/apis/types/totvsApiTypesBase.json#/definitions/Paging"
         },
         {  
            "$ref":"#/definitions/ccosList" 
         }
      ]
    },       
    "ccosList":{  
      "type":"object",
      "properties":{  
         "type":"array",
         "items":{  
            "$ref":"#/definitions/ccos"
         }
       }
     },       
     "ccos":{  
        "type":"object",
        "properties":{  
           "codeCCO":{  
              "description":"",
              "type":"string",
              "x-totvs":[  
                 {  
                    "product":"central-obrigacoes",
                    "field":"B3K.B3K_CODCCO",
                    "required":true,
                    "type":"string",
                    "note":"Codigo CCO do beneficiário",
                    "available":true,
                    "canUpdate":true
                 }
              ]
           },
           "subscriberId":{  
              "description":"",
              "type":"string",
              "x-totvs":[  
                 {  
                    "product":"central-obrigacoes",
                    "field":"B3K.B3K_MATRIC",
                    "required":true,
                    "type":"string",
                    "length":"17",
                    "note":"Matricula do beneficiário na operadora",
                    "available":true,
                    "canUpdate":false
                 }
              ]
           },
           "name":{  
              "description":"",
              "type":"string",
              "x-totvs":[  
                 {  
                    "product":"central-obrigacoes",
                    "field":"B3K.B3K_NOMBEN",
                    "required":true,
                    "type":"string",
                    "length":"70",
                    "note":"Nome do beneficiário",
                    "available":true,
                    "canUpdate":true
                 }
              ]
           }             
        }
     }
  }
}