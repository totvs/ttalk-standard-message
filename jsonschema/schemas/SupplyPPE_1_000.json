{
	"$schema": "https://raw.githubusercontent.com/totvs/ttalk-standard-message/SupplyPPE/v1/1_000/jsonschema/schemas/SupplyPPE_1_000.json#",
	"info": {
		"description": "",
		"version": "1.000",
		"title": "Supply PPE",
		"contact": {
			"name": "T-Talk",
			"url": "API.Totvs.com.br",
			"email": "comiteintegracao@totvs.com.br"
		},
		"x-totvs": {
			"messageDocumentation": {
				"name": "Supply PPE",
				"description": "Fornecimento de EPI",
				"segment": "RH"
			},
			"productInformation": [
				{
					"product": "RM",
					"contact": "rm_FW@totvs.com.br",
					"description": "Fornecimento de EPI"
				}
			]
		}
	},
	"definitions": {
		"SupplyPPEGroups": {
			"type": "object",
			"allOf": [
				{
					"$ref": "https://raw.githubusercontent.com/totvs/ttalk-standard-message/master/jsonschema/apis/types/totvsApiTypesBase.json#/definitions/Paging"
				},
				{
					"type": "object",
					"properties": {
						"items": {
							"$ref": "#/definitions/SupplyPPEInfo"
						}
					}
				}
			]
		},
		"SupplyPPEInfo": {
			"type": "object",
			"properties": {
				"id": {
					"description" : "Código identificador do registro.",
					"type" : "string",
					"example" : "1|1|00001|123456-0006",
					"x-totvs": [
						{
							"product": "RM",
							"available": true,
							"field": "VEMPRESTIMOEPI.CODCOLIGADA | VEMPRESTIMOEPI.SEQUENCIAL | VEMPRESTIMOEPI.CODEPI | VEMPRESTIMOEPI.CODIDENTEPI",
							"type": "string",
							"required": true,
							"canUpdate": false,
							"note": "Campo calculado."
						}
					]
				},
				"companyCode": {
					"description": "Codigo da Coligada.",
					"type": "integer",
					"example" : 1,
					"x-totvs": [
						{
							"product": "RM",
							"available": true,
							"field": "VEMPRESTIMOEPI.CODCOLIGADA",
							"type": "smallint",
							"required": true,
							"canUpdate": false
						}
					]
				},
            	"sequentialCode": {
					"description" : "Código sequencial do Fornecimento de EPI, gerado por coligada.",
					"type" : "string",
					"example": "1",
					"x-totvs": [
						{
							"product": "RM",
							"available": true,
							"field": "VEMPRESTIMOEPI.SEQUENCIAL",
							"type": "int32",
							"required": false,
							"canUpdate": false
						}
					]
				},
            	"ppeCode": {
					"description" : "Código do EPI usado no fornecimento.",
					"type" : "string",
					"example" : "00001",
					"x-totvs": [
						{
							"product": "RM",
							"available": true,
							"field": "VEMPRESTIMOEPI.CODEPI",
							"type": "string",
							"required": true,
							"canUpdate": false,
							"note": "Código do EPI."
						}
					]
				},
				"ppeIdentifierCode": {
					"description" : "Código identificador do EPI.",
					"type" : "string",
					"example" : "123456-0006",
					"x-totvs": [
						{
							"product": "RM",
							"available": true,
							"field": "VEMPRESTIMOEPI.CODIDENTEPI",
							"type": "string",
							"required": false,
							"canUpdate": false
						}
					]
				},
				"borrowingDate": {
					"description" : "Data de empréstimo do EPI.",
					"type" : "date",
					"example" : "2019-05-26 00:00:00",
					"x-totvs": [
						{
							"product": "RM",
							"available": true,
							"field": "VEMPRESTIMOEPI.DATAEMPRESTIMO",
							"type": "datetime",
							"required": false,
							"canUpdate": false
						}
					]
				},
            	"personCode": {
					"description" : "Código da Pessoa que está fornecido o EPI.",
					"type" : "integer",
					"example" : 2,
					"x-totvs": [
						{
							"product": "RM",
							"available": true,
							"field": "VEMPRESTIMOEPI.CODPESSOA",
							"type": "int32",
							"required": true,
							"canUpdate": false
						}
					]
				},
				"devolutionDate": {
					"description" : "Data de devolução do EPI.",
					"type" : "date",
					"example" : "2019-05-30 00:00:00",
					"x-totvs": [
						{
							"product": "RM",
							"available": true,
							"field": "VEMPRESTIMOEPI.DATADEVOLUCAO",
							"type": "datetime",
							"required": false,
							"canUpdate": false
						}
					]
				},
				"shelfLifeDate": {
					"description" : "Data de validade do EPI.",
					"type" : "date",
					"example" : "2020-01-01 00:00:00",
					"x-totvs": [
						{
							"product": "RM",
							"available": true,
							"field": "VEMPRESTIMOEPI.DATAVALIDADE",
							"type": "datetime",
							"required": false,
							"canUpdate": false
						}
					]
				},
				"effective": {
					"description" : "Flag de eficaz", 
					"type": "boolean",
					"example" : true,
					"x-totvs": [
						{
							"product": "RM",
							"available": true,
							"field": "VEMPRESTIMOEPI.EFICAZ",
							"type": "boolean",
							"required": false,
							"canUpdate": false
						}
					]
				},
				"borrowingId": {
					"description" : "Código do empréstimo",
					"type" : "integer",
					"example" : 8,					
					"x-totvs": [
						{
							"product": "RM",
							"available": true,
							"field": "VEMPRESTIMOEPI.IDEMPRESTIMO",
							"type": "int32",
							"required": false,
							"canUpdate": false
						}
					]
				},
            	"returnId": {
					"description" : "Código da devolução",
					"type" : "integer",
					"example" : 0,					
					"x-totvs": [
						{
							"product": "RM",
							"available": true,
							"field": "VEMPRESTIMOEPI.IDDEVOLUCAO",
							"type": "int32",
							"required": false,
							"canUpdate": false,
							"note" : "Somentente é gerado para EPIs que podem ser devolvidos. Não é gerado para EPIs descartáveis."
						}
					]
				},
            	"discardId": {
					"description" : "Código do descarte.",
					"type" : "integer",
					"example" : 0,					
					"x-totvs": [
						{
							"product": "RM",
							"available": true,
							"field": "VEMPRESTIMOEPI.IDDESCARTE",
							"type": "int32",
							"required": false,
							"canUpdate": false,
							"note" : "Somentente é gerado para EPIs que podem ser descartados. Não é gerado para EPIs retornáveis."
						}
					]
				},
            	"batchId": {
					"description" : "Código do Lote do EPI.",
					"type" : "integer",
					"example" : 0,					
					"x-totvs": [
						{
							"product": "RM",
							"available": true,
							"field": "VEPI.IDLOTE",
							"type": "int32",
							"required": false,
							"canUpdate": false							
						}
					]
				},				
				"ppe": {
					"description": "Informações do equipamento de proteção do fornecimento.",
					"type": "object",
					"items": {
						"$ref": "https://raw.githubusercontent.com/totvs/ttalk-standard-message/PPE/v1/1_000/jsonschema/schemas/PPE_1_000.json#/definitions/PPEInfo",
						"type": "object"
					}
				}
			}
		}
	}
}
