{
	"$schema": "https://raw.githubusercontent.com/totvs/ttalk-standard-message/master/jsonschema/schemas/Documents_1_000.json#",
	"info": {
		"description": "API para retornar os documentos das entidades armazenado no banco de conhecimento, (x-totvs) para produtos TOTVS",
		"version": "1.000",
		"title": "Document",
		"contact": {
			"name": "T-Talk",
			"url": "api.Totvs.com.br",
			"email": "comiteintegracao@totvs.com.br"
		},
		"x-totvs": {
			"transactionDefinition": {
				"subType": "event",
				"businessContentType": {
					"type": "object",
					"$ref": "#/definitions/DocumentInfo"
				},
				"returnContentType": {
					"type": "object",
					"$ref": "https://raw.githubusercontent.com/totvs/ttalk-standard-message/master/jsonschema/schemas/types/ListOfInternalId_1_000.json#/definitions/ReturnContentWithModelType"
				}
			},
			"messageDocumentation": {
				"name": "Document",
				"description": "Cadastro de base de conhecimento",
				"segment": "Faturamento"
			},
			"productInformation": [
				{
					"product": "protheus",
					"contact": "squad.crm@totvs.com.br",
					"description": "Cadastro de base de conhecimento",
					"adapter": "CRMS120.prw"
				}
			]
		}
	},
	"definitions": {
		"PagedDocument": {
			"type": "object",
			"allOf": [
				{
					"$ref": "https://raw.githubusercontent.com/totvs/ttalk-standard-message/master/jsonschema/apis/types/totvsApiTypesBase.json#/definitions/Paging"
				},
				{
					"$ref": "#/definitions/Documents"
				}
			]
		},
		"Documents": {
			"type": "object",
			"properties": {
				"items": {
					"type": "array",
					"items": {
						"$ref": "#/definitions/DocumentInfo"
					}
				}
			}
		},
		"DocumentInfo": {
			"description": "Descreve uma Base de Conhecimento",
			"type": "object",
			"properties": {
				"CompanyId": {
					"type": "string",
					"description": "Código da empresa",
					"maxLength": 3
				},
				"BranchId": {
					"type": "string",
					"description": "Código da Filial",
					"example": "D MG 01",
					"maxLength": 12,
					"x-totvs": [
						{
							"product": "protheus",
							"field": "AC9.AC9_FILIAL",
							"required": true,
							"length": "8",
							"available": true,
							"canUpdate": false
						}
					]
				},
				"CompanyInternalId": {
					"type": "string",
					"description": "Código da Empresa",
					"example": "D MG 01 0000000001SA1D MG 01 000001",
					"x-totvs": [
						{
							"product": "protheus",
							"field": "EMPRESA|AC9.AC9_FILIAL|AC9.AC9_CODOBJ|AC9.AC9_ENTIDA|AC9.AC9_FILENT|AC9.AC9_CODENT",
							"required": true,
							"type": "varchar",
							"length": "101",
							"available": true,
							"canUpdate": false
						}
					]
				},
				"InternalId": {
					"type": "string",
					"example": "D MG 01 000001",
					"description": "InternalId da anotação no ERP",
					"x-totvs": [
						{
							"product": "protheus",
							"field": "AC9.AC9_FILIAL|AC9.AC9_CODOBJ",
							"type": "varchar",
							"available": true,
							"length": "18",
							"note": "O InternalId da anotação é formado por AC9.AC9_FILIAL|AC9.AC9_CODOBJ",
							"required": true
						}
					]
				},
				"Code": {
					"type": "string",
					"description": "Código do documento da base de conhecimento.",
					"example": "0000000001",
					"maxLength": 10,
					"x-totvs": [
						{
							"product": "protheus",
							"field": "AC9.AC9_CODOBJ",
							"type": "varchar",
							"length": "10",
							"required": true,
							"available": true,
							"canUpdate": false
						}
					]
				},
				"Entity": {
					"type": "string",
					"description": "Alias da entidade do documento na base de conhecimento.",
					"example": "SA1",
					"maxLength": 3,
					"x-totvs": [
						{
							"product": "protheus",
							"field": "AC9.AC9_ENTIDA",
							"type": "varchar",
							"length": "3",
							"required": true,
							"available": true,
							"canUpdate": false
						}
					]
				},
				"BranchEntity": {
					"type": "string",
					"description": "Código da Filial da entidade",
					"example": "D MG 02",
					"maxLength": 12,
					"x-totvs": [
						{
							"product": "protheus",
							"field": "AC9.AC9_FILIAL",
							"required": true,
							"length": "8",
							"available": true,
							"canUpdate": false
						}
					]
				},
				"EntityCode": {
					"type": "string",
					"description": "Código da entidade do documento na base de conhecimento.",
					"example": "000001",
					"maxLength": 70,
					"x-totvs": [
						{
							"product": "protheus",
							"field": "AC9.AC9_CODENT",
							"type": "varchar",
							"length": "70",
							"required": true,
							"available": true,
							"canUpdate": false
						}
					]
				},
				"ListOfKnowledge": {
					"type": "object",
					"$ref": "#/definitions/ListKnowledge"
				},
				"ListOfKeyWords": {
					"type": "object",
					"$ref": "#/definitions/ListKeyWords"
				}
			}
		},
		"ListKnowledge": {
			"description": "Descreve o documento na base de conhecimento",
			"type": "array",
			"items": {
				"properties": {
					"FileName": {
						"type": "string",
						"description": "Nome do documento na base de conhecimento.",
						"example": "ACE32.DLL",
						"maxLength": 200,
						"x-totvs": [
							{
								"product": "protheus",
								"field": "ACB.ACB_OBJETO",
								"type": "varchar",
								"length": "200",
								"required": true,
								"available": true,
								"canUpdate": true
							}
						]
					},
					"FileDescription": {
						"type": "string",
						"description": "Descrição do documento na base de conhecimento.",
						"example": "ACE32",
						"maxLength": 60,
						"x-totvs": [
							{
								"product": "protheus",
								"field": "ACB.ACB_DESCRI",
								"type": "varchar",
								"length": "60",
								"required": true,
								"available": true,
								"canUpdate": true
							}
						]
					},
					"FileSizeBytes": {
						"type": "integer",
						"format": "int32",
						"description": "Tamanho do documento na base de conecimento.",
						"minimum": 0,
						"maximum": 999999999999
					}
				}
			}
		},
		"ListKeyWords": {
			"description": "Palavra chave do documento na base de conhecimento.",
			"type": "array",
			"items": {
				"properties": {
					"FileSearch": {
						"type": "string",
						"description": "Palavra chave do documento na base de conecimento.",
						"example": "ACE",
						"maxLength": 20,
						"x-totvs": [
							{
								"product": "protheus",
								"field": "ACC.ACC_KEYWRD",
								"type": "varchar",
								"length": "20",
								"note": "Funcionalidade apenas para inclusão e exclusão de documento, no banco de conhecimento.",
								"required": false,
								"available": true,
								"canUpdate": false
							}
						]
					}
				}
			}
		},
