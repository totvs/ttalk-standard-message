				"borrowerCode": {
					"type": "string",
					"example": "C00001",
					"description": "Código do Tomador de Serviço"
				},
				"idCei": {
					"type": "integer",
					"example": "1",
					"description": "Identificador CEI"
				},
				"useSectionData": {
					"type": "smallint",
					"example": "1",
					"description": "Considera Dados da Seção"
				},
				"fpas": {
					"type": "string",
					"example": "515",
					"description": "FPAS"
				},
				"thirdPartyCode": {
					"type": "string",
					"example": "0115",
					"description": "Código de Terceiros"
				},
				"thirdPartySuspendedCode": {
					"type": "string",
					"example": "0115",
					"description": "Código Suspenso de Terceiros"
				}
			}
		}
