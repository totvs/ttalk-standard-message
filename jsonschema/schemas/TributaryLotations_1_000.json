							"type": "array",
							"items": {
								"$ref": "#/definitions/TributaryLotationsInfo"
							}
						}
					}
				}
			]
		},
		"TributaryLotationsInfo": {
			"type": "object",
			"properties": {
				"companyCode": {
					"type": "integer",
					"example": "1",
					"description": "Código da Empresa"
				},
				"lotationCode": {
					"type": "string",
					"example": "C01S000009",
					"description": "Código da Lotação"
				},
				"id": {
					"type": "string",
					"description": "Identificador da lotação"
				},
				"typeLotation": {
					"type": "string",
					"example": "01",
					"description": "Tipo da Lotação"
				},
				"sectionCode": {
					"type": "string",
					"example": "01",
					"description": "Código da Seção"
				},
				"companyCodeBorrower": {
					"type": "string",
					"example": "1",
					"description": "Código da Coligada do Tomador de Serviço"
