{
	"$schema": "https://raw.githubusercontent.com/totvs/ttalk-standard-message/validator-path-required-check/jsonschema/schemas/AuditChecklist_2_000.json",
	"info": {
		"description": "Contrato de Mensagem Padronizada para Checklist de Auditorias para os produtos TOTVS",
		"version": "2.000",
		"title": "Audit Checklist",
		"contact": {
			"name": "T-Talk",
			"url": "API.Totvs.com.br",
			"email": "comiteintegracao@totvs.com.br"
		},
		"x-totvs": {
			"messageDocumentation": {
				"name": "AuditChecklist",
				"description": "Checklist de Auditoria",
				"segment": "Manufatura"
			},
			"productInformation": [
				{
					"product": "protheus",
					"contact": "SUPPLY.ML.MAN.ESTOQUE@totvs.com.br",
					"description": "Contrato de Mensagem Padronizada para Checklist de Auditoria",
					"adapter": "",
					"helpUrl": ""
				}
			]
		}
	},
	"definitions": {
		"PagedChecklist": {
			"type": "object",
			"allOf": [
				{
					"$ref": "https://raw.githubusercontent.com/totvs/ttalk-standard-message/validator-path-required-check/jsonschema/apis/types/totvsApiTypesBase.json#/definitions/Paging"
				},
				{
					"type": "object",
					"properties": {
						"items": {
							"type": "array",
							"items": {
								"$ref": "#/definitions/checklistType"
							}
						}
					}
				}
			]
		},
		"checklistType": {
			"type": "object",
			"properties": {
				"companyId": {
					"type": "string",
					"description": "Empresa",
					"maxLength": 3
				},
				"branchId": {
					"description": "Filial",
					"type": "string",
					"example": "D MG 01",
					"x-totvs": [
						{
							"product": "PROTHEUS",
							"field": "QU2.QU2_FILIAL",
							"required": true,
							"type": "varchar",
							"available": true,
							"canUpdate": false
						}
					]
				},
				"checklist": {
					"description": "Número do Checklist",
					"type": "string",
					"maximum": 6,
					"x-totvs": [
						{
							"product": "PROTHEUS",
							"field": "QU2.QU2_CHKLST",
							"type": "varchar",
							"note": "Checklist",
							"available": true,
							"length": 6
						}
					]
				},
				"revision": {
					"description": "Revisão do Checklist",
					"type": "string",
					"maximum": 2,
					"x-totvs": [
						{
							"product": "PROTHEUS",
							"field": "QU2.QU2_REVIS",
							"type": "varchar",
							"note": "Revisão",
							"available": true,
							"length": 2
						}
					]
				},
				"internalId": {
					"description": "InternalId do Checklist de Auditoria",
					"type": "string",
					"x-totvs": [
						{
							"product": "PROTHEUS",
							"note": "O internalId do Checklist é formado pelos campos QU2_FILIAL|QU2_CHKLST|QU2_REVIS",
							"type": "varchar",
							"available": true
						}
					]
				},
				"description": {
					"description": "Descrição do Checklist",
					"type": "string",
					"maximum": 50,
					"x-totvs": [
						{
							"product": "PROTHEUS",
							"field": "QU2.QU2_DESCRI",
							"type": "varchar",
							"note": "Descrição",
							"available": true,
							"length": 50
						}
					]
				},
				"notes": {
					"description": "Oberservação do Checklist",
					"type": "string",
					"maximum": 50,
					"x-totvs": [
						{
							"product": "PROTHEUS",
							"field": "QU2.QU2_OBSERV",
							"type": "varchar",
							"note": "Descrição",
							"available": true,
							"length": 50
						}
					]
				},
				"lastRevision": {
					"type": "string",
					"description": "Data da última revisão",
					"format": "date",
					"example": "01-01-2018",
					"x-totvs": [
						{
							"product": "PROTHEUS",
							"field": "QU2.QU2_ULTREV",
							"type": "varchar",
							"note": "Última Revisão",
							"available": true,
							"canUpdate": false,
							"length": 8
						}
					]
				},
				"accomplished": {
					"description": "Flag de efetivação \n Tipo: \n    1 - Vigente\n    2 - Pendente\n    3 - Obsoleto\n",
					"type": "string",
					"enum": [
						"1",
						"2",
						"3"
					],
					"x-totvs": [
						{
							"product": "PROTHEUS",
							"field": "QU2.QU2_EFETIV",
							"type": "varchar",
							"note": "Efetivado",
							"available": true,
							"canUpdate": false,
							"length": 1
						}
					]
				}
			}
		}
	}
}