						{
							"product": "protheus",
							"field": "GXN.GXN_CODEST",
							"required": false,
							"type": "Char",
							"length": "8",
							"available": true,
							"canUpdate": false
						}
					]
				},
				"Period": {
					"type": "string",
					"example": "2019/10",
					"description": "Periodo",
					"x-totvs": [
						{
							"product": "protheus",
							"field": "GXN.GXN_PERIES",
							"required": true,
							"type": "Char",
							"length": "07",
							"available": true,
							"canUpdate": false
						}
					]
				},
				"CreationDate": {
					"description": "Data Criacao",
					"type": "Date",
					"example": "20191010",
					"x-totvs": [
						{
							"product": "protheus",
							"field": "GXN.GXN_DTCRIA",
							"required": false,
							"type": "Char",
							"length": "8",
							"available": true,
							"canUpdate": false
						}
					]
				},
				"FinancialStatus": {
					"type": "string",
					"description": "Situacao da Integracao",
					"example": "2",
					"minimum": 1,
					"maximum": 2,
					"enum": [
						"1 - Não Enviado",
						"2 - Pendente",
						"3 - Rejeitado",
						"4 - Atualizado",
						"5 - Pendente Desatualização",
						"6 - Estornado"
					],
					"x-totvs": [
						{
							"product": "protheus",
							"field": "GXN.GXN_SIT",
							"required": false,
							"type": "Char",
							"length": "1",
							"available": true,
							"canUpdate": true
						}
					]
				},
				"CreationUser": {
					"description": "Usuario de Criacao",
					"type": "string",
					"example": "Administrador",
					"x-totvs": [
						{
							"product": "protheus",
							"field": "GXN.GXN_USUCRI",
							"required": false,
							"type": "Char",
							"length": "50",
							"available": true,
							"canUpdate": false
						}
					]
				}
			}
		},
