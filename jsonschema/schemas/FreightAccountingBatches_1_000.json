					"x-totvs": [
						{
							"product": "protheus",
							"field": "GXO.GXO_VALOR",
							"required": true,
							"type": "Number",
							"length": "12",
							"precision": "2",
							"available": true,
							"canUpdate": true
						}
					]
				},
				"EntryType": {
					"description": "Tipo de Lancamento",
					"type": "string",
					"example": "1",
					"minimum": 1,
					"maximum": 2,
					"enum": [
						"1 - DB",
