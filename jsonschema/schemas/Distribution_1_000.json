{
  "$schema": "https://raw.githubusercontent.com/totvs/ttalk-standard-message/Distribution/v1/1_000/jsonschema/schemas/Distribution_1_000.json#",
  "info": {
    "description": "Contrato de Mensagem Padronizada para a praça do produtos TOTVS",
    "version": "1.000",
    "title": "Distribution",
    "contact": {},
    "x-totvs": {
      "messageDocumentation": {
        "name": "Distribution",
        "description": "Praças de produtos TOTVS",
        "segment": "Distribuição "
      },
      "productInformation": [
        {
          "product": "Winthor",
          "contact": "Winthor@totvs.com.br",
          "description": "Cadastro de Praça"
        }
      ],
      "transactionDefinition": {
        "subType": "event",
        "businessContentType": {
          "$ref": "#/definitions/Distribution",
          "type": "object"
        },
        "returnContentType": {
          "$ref": "#/definitions/ReturnContent",
          "type": "object"
        }
      }
    }
  },
  "definitions": {
    "PagedDistributions": {
      "type": "object",
      "allOf": [
        {
          "$ref": "https://raw.githubusercontent.com/totvs/ttalk-standard-message/master/jsonschema/apis/types/totvsApiTypesBase.json#/definitions/Paging"
        },
        {
          "type": "object",
          "properties": {
            "items": {
              "type": "array",
              "items": {
                "$ref": "#/definitions/Distribution"
              }
            }
          }
        }
      ]
    },
    "Distribution": {
      "type": "object",
      "properties": {
        "id": {
          "type": "integer",
          "description": "Código da praça",
          "minimum" : 1,
          "maximum" : 9999,
          "x-totvs": [
            {
              "product": "WinThor",
              "available": true,
              "note": "Código da praça",
              "field": "codpraca",
              "length": "4",
              "type": "integer"
            }
          ]
        },
        "description": {
          "type": "string",
          "description": "Nome da praça",
          "minLength" : 1,
          "maxLength" : 25,
          "x-totvs": [
            {
              "product": "WinThor",
              "available": true,
              "note": "Nome da praça",
              "field": "praca",
              "length": "25",
              "type": "string"
            }
          ]
        },
        "regionId": {
          "type": "integer",
          "description": "Código da região vinculada a praça",
          "minimum" : 1,
          "maximum" : 9999,
          "x-totvs": [
            {
              "product": "WinThor",
              "available": true,
              "note": "Código da região vinculada a praça",
              "field": "numregiao",
              "length": "4",
              "type": "integer"
            }
          ]
        },
        "routeId": {
          "type": "integer",
          "description": "Código da rota vinculada a praça",
          "minimum" : 1,
          "maximum" : 9999,
          "x-totvs": [
            {
              "product": "WinThor",
              "available": true,
              "note": "Código da rota vinculada a praça",
              "field": "rota",
              "length": "4",
              "type": "integer"
            }
          ]
        },
        "mainDistributionId": {
          "type": "integer",
          "description": "Código da praça principal vinculada a praça",
          "x-totvs": [
            {
              "product": "WinThor",
              "available": true,
              "note": "Código da praça principal vinculada a praça",
              "field": "codpracaprincipal",
              "length": "4",
              "type": "integer"
            }
          ]
        },
        "routeSequence": {
          "type": "integer",
          "description": "Número de sequencia da rota",
          "minimum" : 1,
          "maximum" : 9999,
          "x-totvs": [
            {
              "product": "WinThor",
              "available": true,
              "note": "Número de sequencia da rota",
              "field": "seqrota",
              "length": "4",
              "type": "integer"
            }
          ]
        },
        "population": {
          "type": "integer",
          "description": "Informação de população",
          "minimum" : 1,
          "maximum" : 9999,
          "x-totvs": [
            {
              "product": "WinThor",
              "available": true,
              "note": "Informação de população",
              "field": "populacao",
              "length": "14",
              "type": "integer"
            }
          ]
        },
        "distance": {
          "type": "number",
          "description": "Distância da praça",
          "minimum" : 0.01,
          "maximum" : 999999999999.99,
          "exclusiveMinimum" : false,
          "exclusiveMaximum" : false,
          "x-totvs": [
            {
              "product": "WinThor",
              "available": true,
              "note": "Distância da praça",
              "field": "distancia",
              "length": "12,2",
              "type": "number"
            }
          ]
        },
        "minLoadValue": {
          "type": "number",
          "description": "Valor mínimo de carregamento da praça",
          "maximum" : 999999999999.99,
          "exclusiveMaximum" : false,
          "x-totvs": [
            {
              "product": "WinThor",
              "available": true,
              "note": "Valor mínimo de carregamento da praça",
              "field": "vlmincarreg",
              "length": "12,2",
              "type": "number"
            }
          ]
        },
        "minLoadPercentage": {
          "type": "number",
          "description": "Valor percentual mínimo de carregamento da praça",
          "maximum" : 9999999999.99,
          "exclusiveMaximum" : false,
          "x-totvs": [
            {
              "product": "WinThor",
              "available": true,
              "note": "Valor percentual mínimo de carregamento da praça",
              "field": "percmincarreg",
              "length": "12,2",
              "type": "number"
            }
          ]
        },
        "activeDistribution": {
          "type": "boolean",
          "description": "Informação se a praça está ativa",
          "x-totvs": [
            {
              "product": "WinThor",
              "available": true,
              "note": "Informação se a praça está ativa",
              "field": "situacao",
              "length": "1",
              "type": "string"
            }
          ]
        },
        "initialZipCode": {
          "type": "string",
          "description": "CEP inicial da praça",
          "pattern" : "\\d{5}-\\d{3}",
          "x-totvs": [
            {
              "product": "WinThor",
              "available": true,
              "note": "CEP inicial da praça",
              "field": "cepinicial",
              "length": "9",
              "type": "string"
            }
          ]
        },
        "finalZipCode": {
          "type": "string",
          "description": "CEP final da praça",
          "pattern" : "\\d{5}-\\d{3}",
          "x-totvs": [
            {
              "product": "WinThor",
              "available": true,
              "note": "CEP final da praça",
              "field": "cepfinal",
              "length": "9",
              "type": "string"
            }
          ]
        },
        "deliveryPriority": {
          "type": "boolean",
          "description": "Informa se a praça tem prioridade de entrega",
          "x-totvs": [
            {
              "product": "WinThor",
              "available": true,
              "note": "Informação se a praça tem prioridade de entrega",
              "field": "prioridadeentrega",
              "length": "1",
              "type": "string"
            }
          ]
        }
      },
      "description": "API para cadastro de praça de produtos TOTVS"
    },
    "ReturnContent": {
      "type": "object",
      "properties": {
        "ListOfInternalId": {
          "type": "array",
          "items": {
            "$ref": "https://raw.githubusercontent.com/totvs/ttalk-standard-message/master/jsonschema/schemas/types/ListOfInternalId_1_000.json#/definitions/ListOfInternalIdType",
            "type": "object"
          }
        }
      }
    }
  }
}