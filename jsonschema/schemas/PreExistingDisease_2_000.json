{
	"$schema": "https://raw.githubusercontent.com/totvs/ttalk-standard-message/master/jsonschema/schemas/PreExistingDisease_2_000.json#",
	"servers": [
		{
			"description": "API para as doenças pré-existentes do segmento Saúde para produtos TOTVS",
			"url": "{{host}}/api/hat/healthcare/v1",
			"variables": {
				"serverUrl": {
					"default": "localhost"
				},
				"serverHttpPort": {
					"default": "4202"
				}
			}
		}
	],
	"info": {
		"description": "API para as doenças pré-existentes do segmento Saúde para produtos TOTVS",
		"version": "2.000",
		"title": "Pre-existing Disease",
		"contact": {
			"name": "T-Talk",
			"url": "api.Totvs.com.br",
			"email": "comiteintegracao@totvs.com.br"
		},
		"x-totvs": {
			"messageDocumentation": {
				"name": "PreExistingDiseases",
				"description": "Doenças pré-existentes",
				"segment": "Saúde"
			},
			"productInformation": [
				{
					"product": "HAT",
					"contact": "healthcare.autorizador@totvs.com.br",
					"description": "Plataforma de autorização de procedimentos e eventos em Saúde"
				}
			]
		}
	},
	"definitions": {
		"PreExistingDisease": {
			"type": "object",
			"properties": {
				"healthInsurerId": {
					"description": "",
					"type": "string",
					"x-totvs": [
						{
							"product": "hat",
							"required": false,
							"type": "string",
							"length": "4",
							"note": "Informativo, identifica o numero da operadora ao qual esta sendo feita a atualizacao",
							"available": true,
							"canUpdate": true
						}
					]
				},
				"ansRegistry": {
					"description": "",
					"type": "string",
					"x-totvs": [
						{
							"product": "hat",
							"required": false,
							"type": "string",
							"length": "6",
							"note": "Informativo, identifica o numero da operadora ao qual esta sendo feita a atualizacao",
							"available": true,
							"canUpdate": true
						}
					]
				},
				"diseases": {
					"description": "",
					"type": "array",
					"minItems": 1,
					"maxItems": 100,
					"items": {
						"$ref": "#/definitions/PreexistingdiseaseItems"
					}
				}
			}
		},
		"PreexistingdiseaseItems": {
			"type": "object",
			"properties": {
				"subscriberId": {
					"maxLength": 17,
					"minLength": 16,
					"description": "Numero da matricula do beneficiario",
					"type": "string",
					"x-totvs": [
						{
							"product": "hat",
							"field": "BF3.BF3_CODINT || BF3.BF3_CODEMP || BF3.BF3_MATRIC || BF3.BF3_TIPREG",
							"required": false,
							"type": "string",
							"length": "16",
							"note": "Numero da matricula do beneficiario",
							"available": true,
							"canUpdate": true
						}
					]
				},
				"diseaseCode": {
					"maxLength": 8,
					"description": "Codigo CID",
					"type": "string",
					"x-totvs": [
						{
							"product": "hat",
							"field": "BF3.BF3_CODDOE",
							"required": false,
							"type": "string",
							"length": "8",
							"note": "Codigo CID da doenca pre-existente",
							"available": true,
							"canUpdate": true
						}
					]
				},
				"waitingPeriod": {
					"description": "Carencia",
					"maxLength": 3,
					"type": "number",
					"x-totvs": [
						{
							"product": "hat",
							"field": "BF3.BF3_MESAGR",
							"required": false,
							"type": "float",
							"note": "Carencia",
							"available": true,
							"canUpdate": true
						}
					]
				},
				"waitingPeriodUnit": {
					"maxLength": 1,
					"description": "Unidade ao qual o sistema ira utilizar para calcular a carencia de acordo com o valor informado no campo waitingPeriod. Unidade: 1 = Horas; 2 = Dias; 3 = Meses; 4 = Anos",
					"type": "string",
					"enum": [
						"1",
						"2",
						"3",
						"4"
					],
					"x-totvs": [
						{
							"product": "hat",
							"field": "BF3.BF3_UNAGR",
							"required": false,
							"type": "string",
							"length": "",
							"note": "Unidade ao qual o sistema ira utilizar para calcular a carencia de acordo com o valor informado no campo mesesCarencia",
							"available": true,
							"canUpdate": true
						}
					]
				},
				"waitingPeriodDate": {
					"description": "Data do inicio da carencia",
					"type": "string",
					"maxLength": 8,
					"x-totvs": [
						{
							"product": "hat",
							"field": "BF3.BF3_DATCPT",
							"required": false,
							"type": "string",
							"length": "8",
							"note": "Data do inicio da carencia",
							"available": true,
							"canUpdate": true
						}
					]
				},
				"procedureTableCode": {
					"description": "Tabela do procedimento",
					"maxLength": 2,
					"type": "string",
					"x-totvs": [
						{
							"product": "hat",
							"field": "BF3.BF3_CODPAD",
							"required": false,
							"type": "string",
							"length": "2",
							"note": "Tabela do procedimento",
							"available": true,
							"canUpdate": true
						}
					]
				},
				"procedureCode": {
					"description": "Codigo do procedimento",
					"maxLength": 16,
					"type": "string",
					"x-totvs": [
						{
							"product": "hat",
							"field": "BF3.BF3_CODPSA",
							"required": false,
							"type": "string",
							"length": "16",
							"note": "Codigo do procedimento",
							"available": true,
							"canUpdate": true
						}
					]
				}
			}
		},
		"TokenProcessResponse": {
			"type": "object",
			"properties": {
				"detailedMessage": {
					"description": "Mensagem de resposta detalhada",
					"type": "string",
					"x-totvs": [
						{
							"product": "hat",
							"required": false,
							"type": "varchar",
							"length": "47",
							"note": "Mensagem de resposta detalhada",
							"available": true,
							"canUpdate": true
						}
					]
				},
				"tokenProcess": {
					"description": "ID do Token de processamento",
					"type": "string",
					"x-totvs": [
						{
							"product": "hat",
							"required": false,
							"type": "varchar",
							"length": "47",
							"note": "Utilizado para consultar a resposta em requisicoes de tipo long-pooling",
							"available": true,
							"canUpdate": true
						}
					]
				},
				"timeout": {
					"description": "Tempo que o cliente deve esperar por uma resposta",
					"type": "string",
					"x-totvs": [
						{
							"product": "hat",
							"required": false,
							"type": "varchar",
							"length": "47",
							"note": "Tempo que o cliente deve esperar por uma resposta",
							"available": true,
							"canUpdate": true
						}
					]
				},
				"serviceResponse": {
					"description": "Resposta do processamento do servico",
					"type": "boolean",
					"x-totvs": [
						{
							"product": "hat",
							"required": false,
							"type": "boolean",
							"note": "Resposta do processamento do servico",
							"available": true,
							"canUpdate": true
						}
					]
				},
				"averageTime": {
					"description": "Tempo medio de processamento",
					"type": "integer",
					"x-totvs": [
						{
							"product": "hat",
							"required": false,
							"type": "integer",
							"note": "Tempo medio de processamento",
							"available": true,
							"canUpdate": true
						}
					]
				}
			}
		}
	}
}