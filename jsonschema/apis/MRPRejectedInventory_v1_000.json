openapi: 3.0.1
servers:
  - description: API para manipulação dos estoques rejeitados do MRP.
    url: '{{host}}/api/pcp/v1'
    variables:
      serverUrl:
        default: localhost
      serverHttpPort:
        default: '8040'
info:
  description: API para a entidade de Estoques Rejeitado do MRP
  version: '1.000'
  title: Estoque Rejeitado do MRP
  x-totvs:
    messageDocumentation:
      name: MRPRejectedInventory
      description: Estoque Rejeitado do MRP
      segment: Manufatura
    productInformation:
      - product: Protheus
        contact: robson.klug@totvs.com.br
        description: Cadastro de Estoque Rejeitado do MRP
        adapter: ''
paths:
  /mrprejectedinventory:
    post:
      tags:
        - mrprejectedinventory
      summary: Inclui ou atualiza um ou mais registros no estoque rejeitado do MRP
      description: ''
      operationId: postrejectedinventory
      responses:
        '201':
          description: Operação realizada com sucesso
          content:
            application/json:
              schema:
                $ref: >-
                  https://raw.githubusercontent.com/totvs/ttalk-standard-message/master/jsonschema/schemas/MRPRejectedInventory_1_000.json#/definitions/PagedMRPRejectedInventory
        '207':
          description: Operação realizada parcialmente com sucesso
          content:
            application/json:
              schema:
                $ref: >-
                  https://raw.githubusercontent.com/totvs/ttalk-standard-message/master/jsonschema/schemas/MRPRejectedInventory_1_000.json#/definitions/MRPRejectedInventoryMultiple
        '400':
          description: Erro no momento da Inclusão
          content:
            application/json:
              schema:
                $ref: >-
                  https://raw.githubusercontent.com/totvs/ttalk-standard-message/master/jsonschema/apis/types/totvsApiTypesBase.json#/definitions/ErrorModel
        '503':
          description: Servidor não conseguiu processar a requisição
          content:
            application/json:
              schema:
                $ref: >-
                  https://raw.githubusercontent.com/totvs/ttalk-standard-message/master/jsonschema/apis/types/totvsApiTypesBase.json#/definitions/ErrorModel
      x-totvs:
        productInformation:
          - product: Protheus
            available: true
            note: Este verbo está disponível com todos os parâmetros
            minimalVersion: 12.1.27
      requestBody:
        content:
          application/json:
            schema:
              $ref: >-
                https://raw.githubusercontent.com/totvs/ttalk-standard-message/master/jsonschema/schemas/MRPRejectedInventory_1_000.json#/definitions/ListOfMRPRejectedInventory
        description: Objeto do Estoque Rejeitado do MRP que deve ser adicionada ou atualizada
        required: true
    get:
      tags:
        - mrprejectedinventory
      summary: Lista de estoques rejeitados do MRP
      description: Retorna lista de estoques rejeitados do MRP
      operationId: getmrprejectedinventory
      parameters:
        - $ref: >-
            https://raw.githubusercontent.com/totvs/ttalk-standard-message/master/jsonschema/apis/types/totvsApiTypesBase.json#/parameters/Order
        - $ref: >-
            https://raw.githubusercontent.com/totvs/ttalk-standard-message/master/jsonschema/apis/types/totvsApiTypesBase.json#/parameters/Page
        - $ref: >-
            https://raw.githubusercontent.com/totvs/ttalk-standard-message/master/jsonschema/apis/types/totvsApiTypesBase.json#/parameters/PageSize
        - $ref: >-
            https://raw.githubusercontent.com/totvs/ttalk-standard-message/master/jsonschema/apis/types/totvsApiTypesBase.json#/parameters/Fields
        - name: branchId
          in: query
          description: Código da filial
          required: false
          schema:
            type: string
        - name: code
          in: query
          description: Código do saldo rejeitado em estoque do produto no MRP
          required: false
          schema:
            type: string
        - name: product
          in: query
          description: Código do produto
          required: false
          schema:
            type: string
        - name: warehouse
          in: query
          description: Armazém de estoque
          required: false
          schema:
            type: string
        - name: invoiceDate
          in: query
          description: Data de validade do estoque rejeitado
          required: false
          schema:
            type: string
        - name: quantity
          in: query
          description: Quantidade total do estoque rejeitado
          required: false
          schema:
            type: string
        - name: returnedQuantity
          in: query
          description: Quantidade rejeitada em estoque
          required: false
          schema:
            type: string
      responses:
        '200':
          description: Operação realizada com sucesso
          content:
            application/json:
              schema:
                $ref: >-
                  https://raw.githubusercontent.com/totvs/ttalk-standard-message/master/jsonschema/schemas/MRPRejectedInventory_1_000.json#/definitions/PagedMRPRejectedInventory
        '404':
          description: Estoque rejeitado do MRP não localizado
          content:
            application/json:
              schema:
                $ref: >-
                  https://raw.githubusercontent.com/totvs/ttalk-standard-message/master/jsonschema/apis/types/totvsApiTypesBase.json#/definitions/ErrorModel
      x-totvs:
        productInformation:
          - product: Protheus
            available: true
            note: Este verbo está disponível com todos os parâmetros
            minimalVersion: 12.1.27
    delete:
      tags:
        - mrprejectedinventory
      summary: Remove um ou mais saldo rejeitado em Estoque do MRP
      description: Remove um ou mais saldo rejeitado em Estoque do MRP
      operationId: deletemrprejectedinventory
      responses:
        '204':
          description: Operação realizada com sucesso
        '207':
          description: Operação realizada parcialmente com sucesso
          content:
            application/json:
              schema:
                $ref: >-
                  https://raw.githubusercontent.com/totvs/ttalk-standard-message/master/jsonschema/schemas/MRPRejectedInventory_1_000.json#/definitions/MRPRejectedInventoryMultiple
        '400':
          description: Erro no momento da exclusão
          content:
            application/json:
              schema:
                $ref: >-
                  https://raw.githubusercontent.com/totvs/ttalk-standard-message/master/jsonschema/apis/types/totvsApiTypesBase.json#/definitions/ErrorModel
        '404':
          description: Estoque não localizado na base
          content:
            application/json:
              schema:
                $ref: >-
                  https://raw.githubusercontent.com/totvs/ttalk-standard-message/master/jsonschema/apis/types/totvsApiTypesBase.json#/definitions/ErrorModel
      requestBody:
        content:
          application/json:
            schema:
              $ref: >-
                https://raw.githubusercontent.com/totvs/ttalk-standard-message/master/jsonschema/schemas/MrpStockBalance_1_000.json#/definitions/ListOfMRPRejectedInventory
      x-totvs:
        productInformation:
          - product: Protheus
            available: true
            note: Este verbo está disponível com todos os parâmetros
            minimalVersion: 12.1.27
  '/mrprejectedinventory/{branchId}/{code}':
    get:
      tags:
        - mrprejectedinventory
      summary: Retorna o Estoque Rejeitado do MRP
      description: Retorna o Estoque Rejeitado do MRP
      operationId: getmrprejectedinventory1
      parameters:
        - $ref: >-
            https://raw.githubusercontent.com/totvs/ttalk-standard-message/master/jsonschema/apis/types/totvsApiTypesBase.json#/parameters/Fields
        - $ref: '#/components/parameters/branchId'
        - $ref: '#/components/parameters/code'
      responses:
        '200':
          description: Operação realizada com sucesso
          content:
            application/json:
              schema:
                $ref: >-
                  https://raw.githubusercontent.com/totvs/ttalk-standard-message/master/jsonschema/schemas/MRPRejectedInventory_1_000.json#/definitions/MRPRejectedInventory
        '404':
          description: Estoque Rejeitado do MRP não localizado
          content:
            application/json:
              schema:
                $ref: >-
                  https://raw.githubusercontent.com/totvs/ttalk-standard-message/master/jsonschema/apis/types/totvsApiTypesBase.json#/definitions/ErrorModel
      x-totvs:
        productInformation:
          - product: Protheus
            available: true
            note: Este verbo está disponível com todos os parâmetros
            minimalVersion: 12.1.27
  /mrprejectedinventory/clearProduct:
    delete:
      tags:
        - mrprejectedinventory
      summary: Zera os saldos de um ou mais produtos em um determinado armazém
      description: Zera os saldos de um ou mais produtos em um determinado armazém
      operationId: clearmrprejectedinventory
      responses:
        '204':
          description: Operação realizada com sucesso
        '207':
          description: Operação realizada parcialmente com sucesso
          content:
            application/json:
              schema:
                $ref: >-
                  https://raw.githubusercontent.com/totvs/ttalk-standard-message/master/jsonschema/schemas/MRPRejectedInventory_1_000.json#/definitions/MRPRejectedInventoryMultipleClear
        '400':
          description: Erro no momento da exclusão
          content:
            application/json:
              schema:
                $ref: >-
                  https://raw.githubusercontent.com/totvs/ttalk-standard-message/master/jsonschema/apis/types/totvsApiTypesBase.json#/definitions/ErrorModel
        '404':
          description: Estoque rejeitado não localizado na base
          content:
            application/json:
              schema:
                $ref: >-
                  https://raw.githubusercontent.com/totvs/ttalk-standard-message/master/jsonschema/apis/types/totvsApiTypesBase.json#/definitions/ErrorModel
      requestBody:
        content:
          application/json:
            schema:
              $ref: >-
                https://raw.githubusercontent.com/totvs/ttalk-standard-message/master/jsonschema/schemas/MRPRejectedInventory_1_000.json#/definitions/ListOfMRPRejectedInventoryClear
      x-totvs:
        productInformation:
          - product: Protheus
            available: true
            note: Este verbo está disponível com todos os parâmetros
            minimalVersion: 12.1.27
  /mrprejectedinventory/sync:
    post:
      tags:
        - mrprejectedinventory
      summary: Sincroniza registros do estoque rejeitado do MRP
      description: ''
      operationId: syncmrprejectedinventory
      responses:
        '201':
          description: Operação realizada com sucesso
          content:
            application/json:
              schema:
                $ref: >-
                  https://raw.githubusercontent.com/totvs/ttalk-standard-message/master/jsonschema/schemas/MRPRejectedInventory_1_000.json#/definitions/PagedMRPRejectedInventory
        '207':
          description: Operação realizada parcialmente com sucesso
          content:
            application/json:
              schema:
                $ref: >-
                  https://raw.githubusercontent.com/totvs/ttalk-standard-message/master/jsonschema/schemas/MrpStockBalance_1_000.json#/definitions/MRPRejectedInventoryMultiple
        '400':
          description: Erro no momento da Sincronização
          content:
            application/json:
              schema:
                $ref: >-
                  https://raw.githubusercontent.com/totvs/ttalk-standard-message/master/jsonschema/apis/types/totvsApiTypesBase.json#/definitions/ErrorModel
        '503':
          description: Servidor não conseguiu processar a requisição
          content:
            application/json:
              schema:
                $ref: >-
                  https://raw.githubusercontent.com/totvs/ttalk-standard-message/master/jsonschema/apis/types/totvsApiTypesBase.json#/definitions/ErrorModel
      x-totvs:
        productInformation:
          - product: Protheus
            available: true
            note: Este verbo está disponível com todos os parâmetros
            minimalVersion: 12.1.27
      requestBody:
        content:
          application/json:
            schema:
              $ref: >-
                https://raw.githubusercontent.com/totvs/ttalk-standard-message/master/jsonschema/schemas/MRPRejectedInventory_1_000.json#/definitions/ListOfMRPRejectedInventory
        description: Objeto do Estoque Rejeitado do MRP que deve ser adicionada ou atualizada
        required: true
components:
  parameters:
    branchId:
      name: branchId
      in: path
      description: Código da filial do estoque rejeitado do MRP
      required: true
      schema:
        type: string
        format: char(2)
    code:
      name: code
      in: path
      description: Código do saldo rejeitado em estoque do MRP
      required: true
      schema:
        type: string
        format: char(200)
  schemas: {}

