			"description": "API da entidade Item SKU WMS (ItemSKU) TOTVS",
			"url": "{{host}}/api/logistica/v2",
			"variables": {
				"serverUrl": {
					"default": "localhost"
				},
				"serverHttpPort": {
					"default": "8012"
				}
			}
		}
	],
	"info": {
		"description": "API da entidade Item SKU WMS (ItemSKU) TOTVS",
		"version": "2.000",
		"title": "Cadastro Item SKU WMS",
		"contact": {
			"name": "T-Talk",
			"url": "api.totvs.com.br",
			"email": "comiteintegracao@totvs.com.br"
		},
		"x-totvs": {
			"messageDocumentation": {
				"name": "ItemSKU",
				"description": "Cadastro Item SKU WMS",
				"segment": "Logística"
			},
			"productInformation": [
				{
					"product": "Logix",
					"contact": "SUPPLY.LOG.WMS-OL@totvs.com.br",
					"description": "Cadastro Item SKU WMS",
					"adapter": ""
				}
			]
		}
	},
	"paths": {
		"/ItemSKU": {			
			"post": {
				"tags": [
					"ItemSKU"
				],
				"summary": "Inclui um Item e seus respectivos SKUs",
				"operationId": "postItemSKU",
				"x-totvs": {
					"productInformation": [
						{
							"product": "Logix",
							"available": true,
							"note": "Este verbo esta disponivel com todos os parametros",
							"minimalVersion": "12.1.2309"
						}
					],
					"description": "cadastro de item e SKU"					
				},
				"responses": {
					"200": {
						"description": "Operação realizada com sucesso",
						"content": {
							"application/json": {
								"schema": {
									"$ref": "https://raw.githubusercontent.com/totvs/ttalk-standard-message/master/jsonschema/schemas/ItemSKU_2_000.json"
								}
							}
						}
					},
					"400": {
						"description": "Erro durante inclusão do Item",
						"content": {
							"application/json": {
								"schema": {
									"$ref": "https://raw.githubusercontent.com/totvs/ttalk-standard-message/master/jsonschema/apis/types/totvsApiTypesBase.json#/definitions/ErrorModel"
								}
							}
						}
					},
					"503": {
						"description": "Servidor não conseguiu processar a requisição",
						"content": {
							"application/json": {
								"schema": {
									"$ref": "https://raw.githubusercontent.com/totvs/ttalk-standard-message/master/jsonschema/apis/types/totvsApiTypesBase.json#/definitions/ErrorModel"
								}
							}
						}
					}
				},
				"parameters": [
					{
						"$ref": "https://raw.githubusercontent.com/totvs/ttalk-standard-message/master/jsonschema/apis/types/totvsApiTypesBase.json#/parameters/Authorization"
					}
				],
				"requestBody": {
					"description": "Inclui um Item e SKUs",
					"content": {
						"application/json": {
							"schema": {
								"$ref": "https://raw.githubusercontent.com/totvs/ttalk-standard-message/master/jsonschema/schemas/ItemSKU_2_000.json"
							}
						}
					}
				}
			}
		},
		"/ItemSKU/{code}": {
			"put": {
				"tags": [
					"code"
				],
				"summary": "Altera o Item/SKU",
				"x-totvs": {
					"productInformation": [
						{
							"product": "Logix",
							"description": "Atualiza as informações do item ou SKU, conforme enviado na requisição.",
							"available": true,
							"minimalVersion": "12.1.2309"
						}
					]
				},
				"operationId": "putCode",
				"parameters": [
					{
						"$ref": "https://raw.githubusercontent.com/totvs/ttalk-standard-message/master/jsonschema/apis/types/totvsApiTypesBase.json#/parameters/Authorization"
					},
					{
						"$ref": "#/components/parameters/code"
					},
					{
						"$ref": "https://raw.githubusercontent.com/totvs/ttalk-standard-message/master/jsonschema/apis/types/totvsApiTypesBase.json#/parameters/Fields"
					}
				],
				"requestBody": {
					"description": "Item/SKU a ser atualizado.",
					"content": {
						"application/json": {
							"schema": {
								"$ref": "https://raw.githubusercontent.com/totvs/ttalk-standard-message/master/jsonschema/schemas/ItemSKU_2_000.json#/definitions/PagedItemSKU"
							}
						}
					}
				},
				"responses": {
					"200": {
						"description": "Operação realizada com sucesso!",
						"content": {
							"application/json": {
								"schema": {
									"$ref": "https://raw.githubusercontent.com/totvs/ttalk-standard-message/master/jsonschema/schemas/ItemSKU_2_000.json#/definitions/PagedItemSKU"
								}
							}
						}
					},
					"400": {
						"description": "Erro durante a alteração do Item SKU!",
						"content": {
							"application/json": {
								"schema": {
									"$ref": "https://raw.githubusercontent.com/totvs/ttalk-standard-message/master/jsonschema/apis/types/totvsApiTypesBase.json#/definitions/ErrorModel"
								}
							}
						}
					},
					"404": {
						"description": "Registro não encontrado!",
						"content": {
							"application/json": {
								"schema": {
									"$ref": "https://raw.githubusercontent.com/totvs/ttalk-standard-message/master/jsonschema/apis/types/totvsApiTypesBase.json#/definitions/ErrorModel"
								}
							}
						}
					}
				}
			}
		}
	}
}
