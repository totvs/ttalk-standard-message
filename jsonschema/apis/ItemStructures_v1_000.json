					{
						"name": "search",
						"in": "query",
						"schema": {
							"type": "string"
						},
						"description": "Busca por todos os itens cujo código seja composto por este parâmetro. Exemplo: \"search=001\" trará itens como \"item-001\", \"001002\" etc."
					},
					{
						"name": "code",
