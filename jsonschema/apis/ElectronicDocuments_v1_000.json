                                                        "type": "string",
                                                        "description": "Código da empresa do documento integrado",
                                                        "example": "T1"
                                                    },
                                                    "branchId": {
                                                        "type": "string",
                                                        "description": "Código da filial do documento integrado",
                                                        "example": "D MG 01 "
                                                    },
                                                    "id": {
                                                        "type": "string",
                                                        "description": "Id do documento integrado",
                                                        "maxLength": 15,
                                                        "example": "000000000000234"
                                                    },
                                                    "numero": {
                                                        "type": "string",
                                                        "description": "Número do documento integrado",
                                                        "maxLength": 60,
                                                        "example": "000000000000000000000000000000000000000000000000000000000234"
                                                    },
                                                    "serie": {
                                                        "type": "string",
                                                        "description": "Série do documento integrado",
                                                        "maxLength": 20,
                                                        "example": "00000000000000000234"
                                                    },
                                                    "tipo": {
                                                        "type": "string",
                                                        "description": "Tipo do documento integrado",
                                                        "maxLength": 2,
                                                        "example": "06"
                                                    },
                                                    "cliente": {
                                                        "type": "string",
                                                        "description": "CNPJ ou CPF do cliente do documento integrado",
                                                        "maxLength": 14,
                                                        "example": "64863350000164"
                                                    },
                                                    "emissao": {
                                                        "type": "string",
                                                        "description": "Data de emissão do documento integrado",
                                                        "maxLength": 8,
                                                        "example": "20230101"
                                                    }
                                                },
                                                "required": [
                                                    "companyId",
                                                    "branchId",
                                                    "id",
                                                    "numero",
                                                    "serie",
                                                    "tipo",
                                                    "cliente",
                                                    "emissao"
