{
	"openapi": "3.0.1",
	"servers": [
		{
			"description": "API para cadastro Notas dos produtos TOTVS",
			"url": "{{host}}/api/ctb/v1",
			"variables": {
				"serverUrl": {
					"default": "localhost"
				},
				"serverHttpPort": {
					"default": "8080"
				}
			}
		}
	],
	"info": {
		"description": "API para a entidade Item Contábil para produtos TOTVS",
		"version": "1.000",
		"title": "Item Contábil",
		"contact": {
			"name": "T-Talk",
			"url": "https://api.totvs.com.br",
			"email": "comiteintegracao@totvs.com.br"
		},
		"x-totvs": {
			"messageDocumentation": {
				"name": "AccountingItem",
				"description": "Item Contábil",
				"segment": "Serviços"
			},
			"productInformation": [
				{
					"product": "Protheus",
					"contact": "squad.control@totvs.com.br",
					"description": "Cadastro de Item Contábil",
					"adapter": "CTBS040.prw"
				}
			]
		}
