			"InternalId": {
				"name": "InternalId",
				"in": "path",
				"required": true,
				"description": "Identificador único da Transportadora",
				"example": "D MG 01 000001",
				"schema": {
					"type": "string"
				},
				"x-totvs": {
					"productInformation": [
						{
							"product": "Protheus",
							"available": true,
							"note": "Informar os campos Filial (SA4.A4_FILIAL) e Código da Transportadora (SA4.A4_COD)",
							"minimalVersion": "12.1.21"
						}
					]
				}
			}
		},
		"schemas": {}
	}
}