        "summary": "Retorna todas as áreas de interesse ofertadas cadastradas no processo seletivo",
        "x-totvs": {
          "productInformation": [
            {
              "product": "RM",
              "available": true,
              "note": "Este verbo esta disponível com todos os parâmetros",
              "minimalVersion": "12.1.23"
            }
          ]
        },
        "description": "Retorna todas as áreas ofertadas do processo seletivo",
        "operationId": "getAll",
        "parameters": [
          {
            "$ref": "https://raw.githubusercontent.com/totvs/ttalk-standard-message/master/jsonschema/apis/types/totvsApiTypesBase.json#/parameters/Authorization"
          },
          {
            "$ref": "https://raw.githubusercontent.com/totvs/ttalk-standard-message/master/jsonschema/apis/types/totvsApiTypesBase.json#/parameters/Order"
          },
          {
            "$ref": "https://raw.githubusercontent.com/totvs/ttalk-standard-message/master/jsonschema/apis/types/totvsApiTypesBase.json#/parameters/Page"
          },
          {
            "$ref": "https://raw.githubusercontent.com/totvs/ttalk-standard-message/master/jsonschema/apis/types/totvsApiTypesBase.json#/parameters/PageSize"
          }
        ],
        "responses": {
          "200": {
            "description": "Operação realizada com sucesso",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "https://raw.githubusercontent.com/totvs/ttalk-standard-message/AvailableCourse/v1/1_000/jsonschema/schemas/AvailableCourse_1_000.json#/definitions/PagedAvailableCourse"
                }
              }
            }
          },
          "400": {
            "description": "erro no momento da listagem das áreas ofertadas",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "https://raw.githubusercontent.com/totvs/ttalk-standard-message/master/jsonschema/apis/types/totvsApiTypesBase.json#/definitions/ErrorModel"
                }
              }
            }
          }
        }
      }
    },
	"/SelectionProcess/{SelectionProcessId}/AvailableCourse/{AvailableCourseId}": {
      "get": {
        "tags": [
          "AvailableCourse"
        ],
        "summary": "Retorna os dados de uma área de interesse ofertada do processo seletivo selecionado",
        "x-totvs": {
          "productInformation": [
            {
              "product": "RM",
              "available": true,
              "note": "Este verbo esta disponível com todos os parâmetros",
              "minimalVersion": "12.1.23"
            }
          ]
        },
        "description": "Retorna somente os dados de uma área ofertada do processo seletivo",
        "operationId": "getOne",
        "parameters": [
          {
            "$ref": "https://raw.githubusercontent.com/totvs/ttalk-standard-message/master/jsonschema/apis/types/totvsApiTypesBase.json#/parameters/Authorization"
          },
          {
            "$ref": "https://raw.githubusercontent.com/totvs/ttalk-standard-message/master/jsonschema/apis/types/totvsApiTypesBase.json#/parameters/Expand"
          }
        ],
        "responses": {
          "200": {
            "description": "Operação realizada com sucesso",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "https://raw.githubusercontent.com/totvs/ttalk-standard-message/AvailableCourse/v1/1_000/jsonschema/schemas/AvailableCourse_1_000.json#/definitions/AvailableCourse"
