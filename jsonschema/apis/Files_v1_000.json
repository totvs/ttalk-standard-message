{
	"openapi": "3.0.1",
	"servers": [
		{
			"description": "API para a entidade Products (Produtos) do segmento HealthCare para produtos TOTVS",
			"url": "http://{serverUrl}:{serverHttpPort}/api/healthcare/v1",
			"variables": {
				"serverUrl": {
					"default": "localhost"
				},
				"serverHttpPort": {
					"default": "4202"
				}
			}
		}
	],
	"info": {
		"description": "API para a entidade files (arquivos SBX) do segmento HealthCare para produtos TOTVS",
		"version": "1.000",
		"title": "Files",
		"contact": {
			"name": "T-Talk",
			"url": "http://api.totvs.com.br",
			"email": "comiteintegracao@totvs.com.br"
		},
		"x-totvs": {
			"messageDocumentation": {
				"name": "Health Files",
				"description": "Files SBX",
				"segment": "HealthCare"
			},
			"productInformation": [
				{
					"product": "central-obrigacoes",
					"contact": "",
					"description": "Histórico de Arquivos SBX",
					"adapter": "plsmvcarqsib.prw"
				}
			]
		}
	},
	"paths": {
		"/files/{yearMonthRefer}": {
			"get": {
				"tags": [
					"collection"
				],
				"parameters": [
					{
						"$ref": "https://raw.githubusercontent.com/totvs/ttalk-standard-message/master/jsonschema/apis/types/totvsApiTypesBase.json#/parameters/Authorization"
					},
					{
						"$ref": "https://raw.githubusercontent.com/totvs/ttalk-standard-message/master/jsonschema/apis/types/totvsApiTypesBase.json#/parameters/Order"
					},
					{
						"$ref": "https://raw.githubusercontent.com/totvs/ttalk-standard-message/master/jsonschema/apis/types/totvsApiTypesBase.json#/parameters/Page"
					},
					{
						"$ref": "https://raw.githubusercontent.com/totvs/ttalk-standard-message/master/jsonschema/apis/types/totvsApiTypesBase.json#/parameters/PageSize"
					},
					{
						"$ref": "https://raw.githubusercontent.com/totvs/ttalk-standard-message/master/jsonschema/apis/types/totvsApiTypesBase.json#/parameters/Fields"
					},
					{
						"$ref": "#/components/parameters/yearMonthRefer"
					},
					{
						"$ref": "#/components/parameters/healthInsurerCode"
					}
				],
				"summary": "Busca os arquivos de uma competência",
				"description": "Retorna os arquivos de uma competência",
				"operationId": "getFiles",
				"responses": {
					"200": {
						"description": "Operação realizada com sucesso!",
						"content": {
							"application/json": {
								"schema": {
									"$ref": "https://raw.githubusercontent.com/totvs/ttalk-standard-message/master/jsonschema/schemas/Files_1_6900.json#/definitions/PagedFiles"
								}
							}
						}
					},
					"401": {
						"description": "Operação não autorizada",
						"content": {
							"application/json": {
								"schema": {
									"$ref": "https://raw.githubusercontent.com/totvs/ttalk-standard-message/master/jsonschema/apis/types/totvsApiTypesBase.json#/definitions/ErrorModel"
								}
							}
						}
					},
					"404": {
						"description": "Competência não encontrada",
						"content": {
							"application/json": {
								"schema": {
									"$ref": "https://raw.githubusercontent.com/totvs/ttalk-standard-message/master/jsonschema/apis/types/totvsApiTypesBase.json#/definitions/ErrorModel"
								}
							}
						}
					}
				}
			}
		},
		"/files/ccos/{fileName}": {
			"get": {
				"tags": [
					"collection"
				],
				"parameters": [
					{
						"$ref": "https://raw.githubusercontent.com/totvs/ttalk-standard-message/master/jsonschema/apis/types/totvsApiTypesBase.json#/parameters/Authorization"
					},
					{
						"$ref": "https://raw.githubusercontent.com/totvs/ttalk-standard-message/master/jsonschema/apis/types/totvsApiTypesBase.json#/parameters/Order"
					},
					{
						"$ref": "https://raw.githubusercontent.com/totvs/ttalk-standard-message/master/jsonschema/apis/types/totvsApiTypesBase.json#/parameters/Page"
					},
					{
						"$ref": "https://raw.githubusercontent.com/totvs/ttalk-standard-message/master/jsonschema/apis/types/totvsApiTypesBase.json#/parameters/PageSize"
					},
					{
						"$ref": "https://raw.githubusercontent.com/totvs/ttalk-standard-message/master/jsonschema/apis/types/totvsApiTypesBase.json#/parameters/Fields"
					},
					{
						"$ref": "#/components/parameters/fileName"
					},
					{
						"$ref": "#/components/parameters/healthInsurerCode"
					}
				],
				"summary": "Busca os CCOs de um arquivo",
				"description": "Retorna os  CCOs de um arquivo",
				"operationId": "getListCCO",
				"responses": {
					"200": {
						"description": "Arquivo não encontrado na competência",
						"content": {
							"application/json": {
								"schema": {
									"$ref": "https://raw.githubusercontent.com/totvs/ttalk-standard-message/master/jsonschema/schemas/Files_1_6900.json#/definitions/PagedCcos"
								}
							}
						}
					},
					"401": {
						"description": "Operação não autorizada",
						"content": {
							"application/json": {
								"schema": {
									"$ref": "https://raw.githubusercontent.com/totvs/ttalk-standard-message/master/jsonschema/apis/types/totvsApiTypesBase.json#/definitions/ErrorModel"
								}
							}
						}
					},
					"404": {
						"description": "Competência não encontrada",
						"content": {
							"application/json": {
								"schema": {
									"$ref": "https://raw.githubusercontent.com/totvs/ttalk-standard-message/master/jsonschema/apis/types/totvsApiTypesBase.json#/definitions/ErrorModel"
								}
							}
						}
					}
				}
			}
		}
	},
	"components": {
		"parameters": {
			"yearMonthRefer": {
				"name": "yearMonthRefer",
				"in": "path",
				"description": "Competência dos arquivos",
				"required": true,
				"schema": {
					"type": "string"
				},
				"x-totvs": [
					{
						"product": "central-obrigacoes",
						"field": "B3X.B3X_ARQUIV",
						"required": true,
						"type": "string",
						"note": "Competência dos arquivos",
						"available": true
					}
				]
			},
			"fileName": {
				"name": "fileName",
				"in": "path",
				"description": "Nome do arquivo",
				"required": true,
				"schema": {
					"type": "string"
				},
				"x-totvs": [
					{
						"product": "central-obrigacoes",
						"field": "B3R.B3R_ANO, B3R.B3R_ARQUIV",
						"required": true,
						"type": "string",
						"note": "Nome do arquivo",
						"available": true
					}
				]
			},
			"healthInsurerCode": {
				"name": "healthInsurerCode",
				"in": "query",
				"description": "Código da operadora",
				"schema": {
					"type": "string"
				},
				"x-totvs": [
					{
						"product": "central-obrigacoes",
						"field": "B3K.B3K_CODOPE",
						"required": true,
						"type": "string",
						"note": "Código da operadora",
						"available": true
					}
				]
			}
		}
	}
}