  "components": {
    "parameters": {
      "ExpiresAt": {
        "name": "Expires_at",
        "in": "header",
        "description": "Data e hora da expiração do token em segundos",
        "required": true,
        "schema": {
          "type": "string"
        }
      },
      "HealthProviderId": {
        "name": "healthProviderId",
        "in": "query",
        "required": true,
        "description": "Código do prestador na operadora",
        "schema": {
          "type": "string"
        }
      },
      "DateFrom": {
        "name": "dateFrom",
        "in": "query",
        "required": true,
        "description": "Data (de) para filtrar lotes com data maior ou igual ao informado no parâmetro",
        "schema": {
          "type": "string",
          "format": "date"
        }
      },
      "DateTo": {
        "name": "dateTo",
        "in": "query",
        "required": true,
        "description": "Data (até) para filtrar lotes com data menor ou igual ao informado no parâmetro",
        "schema": {
          "type": "string",
          "format": "date"
        }
      },
      "CodeSusep": {
        "name": "codeSusep",
        "in": "query",
        "required": true,
        "description": "Código da operadora na ANS (Agência Nacional de Saúde)",
        "schema": {
          "type": "string"
        }
      },
      "BatchNumber": {
        "name": "batchNumber",
        "in": "path",
        "required": true,
        "description": "Número do lote no prestador",
        "schema": {
          "type": "string"
        }
      },
      "Action": {
        "name": "action",
        "in": "query",
        "required": true,
        "description": "Ação do PUT - Valores aceitos: removeAuthorizations ou addAuthorizations",
        "schema": {
          "type": "string"
        }
      }
    }
