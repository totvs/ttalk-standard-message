    "version": "0.2.0",
    "configurations": [   
        {
            "type": "node",
            "request": "launch",
            "name": "Rename Open API Refs",
            "program": "${workspaceFolder}/validator/master/renameOpenApiRef.js",
            "skipFiles": [
                "<node_internals>/**"
            ]
        },

        {
            "type": "node",
            "request": "launch",
            "name": "Mocha Tests",
            "program": "${workspaceFolder}/node_modules/mocha/bin/_mocha",
            "env": { "CHANGED_FILES": ".travis.yml .travis.yml  jsonschema/apis/ECommerceTraceabilityDocuments_v1_000.json  jsonschema/apis/ExcludeTestJson.json        validator/test/testOpenAPI.js     jsonschema/schemas/AccountReceivableDocument_2_003.json"}, 
            "args": [
                "-u",
                "tdd",
                "--timeout",
                "999999",
                "--colors",
                "${workspaceFolder}/validator/test"
            ],
            "internalConsoleOptions": "openOnSessionStart"
        }
    ]
}